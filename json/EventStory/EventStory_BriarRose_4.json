    "m_Name": "EventStory_BriarRose_4",
    "unskippable": 0,
    "dialogueDataEntries": [
        {
            "languageID": "English",
            "dialogueLines": [
                {
                    "characterID": "",
                    "backgroundID": "hauntedhallway",
                    "dialogueText": "You have defeated your foes - for now.",
                    "emotionID": "",
                    "musicID": "haunted",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "hauntedhallway",
                    "dialogueText": "It was a hard fight... but not as hard as Aurora made out it would be. When you mention this to Aurora, she nods.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Aurora",
                    "backgroundID": "hauntedhallway",
                    "dialogueText": "I know. There's something wrong here.",
                    "emotionID": "annoy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Aurora",
                    "backgroundID": "hauntedhallway",
                    "dialogueText": "These creatures don't belong here and the noise we made fighting them should have brought this place's true masters down on us.",
                    "emotionID": "annoy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Aurora",
                    "backgroundID": "hauntedhallway",
                    "dialogueText": "I don't understand what is going on...",
                    "emotionID": "annoy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Aurora",
                    "backgroundID": "hauntedhallway",
                    "dialogueText": "Although... perhaps...",
                    "emotionID": "annoy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Aurora",
                    "backgroundID": "hauntedhallway",
                    "dialogueText": "Vita, can you still hear Briar's voice?",
                    "emotionID": "sad",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Vita",
                    "backgroundID": "hauntedhallway",
                    "dialogueText": "Eh? Ah! Right...",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Aurora",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Vita",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "hauntedhallway",
                    "dialogueText": "She stands still and strains her ears for a moment as the rest of you watch tensely on.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Aurora",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Vita",
                    "outerRight": ""
                },
                {
                    "characterID": "Vita",
                    "backgroundID": "hauntedhallway",
                    "dialogueText": "Yes! It's coming from that direction!",
                    "emotionID": "idle",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Aurora",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Vita",
                    "outerRight": ""
                },
                {
                    "characterID": "Aurora",
                    "backgroundID": "hauntedhallway",
                    "dialogueText": "Good. Then please lead the way.",
                    "emotionID": "sad",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Aurora",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Vita",
                    "outerRight": ""
                },
                {
                    "characterID": "Aurora",
                    "backgroundID": "hauntedhallway",
                    "dialogueText": "I believe we should still be quiet and move as quickly as possible.",
                    "emotionID": "sad",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Aurora",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Vita",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "storybriarroseportal2",
                    "dialogueText": "Vita leads you through the intimidating Gothic architecture for a while, until eventually you come upon another shimmering portal",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "storybriarroseportal2",
                    "dialogueText": "Pallid light falls from beyond.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Esme",
                    "backgroundID": "hauntedhallway",
                    "dialogueText": "Do we go through?",
                    "emotionID": "annoy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Esme",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "hauntedhallway",
                    "dialogueText": "You don't see what else you can do... you can't exactly go back.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Esme",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Aurora",
                    "backgroundID": "hauntedhallway",
                    "dialogueText": "I agree.",
                    "emotionID": "annoy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Aurora",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Esme",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "dreamrealm",
                    "dialogueText": "You pass through the new portal and find yourself in a place you thought you would never see again!",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "dreamrealm",
                    "dialogueText": "This place feels very strange... it looks like the collapsing realm that you met Aurora in, but something is off...",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "dreamrealm",
                    "dialogueText": "The last time you were here, it was infested with Dreamless creatures, but you can't see any anywhere.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Aurora",
                    "backgroundID": "dreamrealm",
                    "dialogueText": "I think I know what's going on...",
                    "emotionID": "annoy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Aurora",
                    "backgroundID": "dreamrealm",
                    "dialogueText": "This place doesn't feel like the collapsing realm... because it isn't.",
                    "emotionID": "annoy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Aurora",
                    "backgroundID": "dreamrealm",
                    "dialogueText": "We're trapped in an illusion. None of this is truly real, just as the last realm we visited wasn't truly real – which is why we weren't attacked by its real masters.",
                    "emotionID": "annoy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Aurora",
                    "backgroundID": "dreamrealm",
                    "dialogueText": "Someone has conjured all of this, a maze of different locations chained together. And all to keep someone powerful from escaping.",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Vita",
                    "backgroundID": "dreamrealm",
                    "dialogueText": "Briar Rose...",
                    "emotionID": "sad",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Aurora",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Vita",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "dreamrealm",
                    "dialogueText": "Aurora nods.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Aurora",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Vita",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "dreamrealm",
                    "dialogueText": "Then are you still in the Enchanted Realm? In the ruined city?",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Aurora",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Vita",
                    "outerRight": ""
                },
                {
                    "characterID": "Aurora",
                    "backgroundID": "dreamrealm",
                    "dialogueText": "No... we're dislocated from Phantasia. This is all a world of its own. Split off and sealed away from Phantasia, quite possibly just one step out of time.",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Aurora",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Vita",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "dreamrealm",
                    "dialogueText": "But you've still been attacked by Dreamless. If this place is sealed, how did the Dreamless get in?",
                    "emotionID": "",
                    "musicID": "threateningbattle",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_NPC_NightmareVassal",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Aurora",
                    "backgroundID": "dreamrealm",
                    "dialogueText": "I'm afraid you're wrong, Dreamer.",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Aurora",
                    "backgroundID": "dreamrealm",
                    "dialogueText": "These creatures aren't Dreamless... these are Nightmares.",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": ""
                }
            ]
        },
        {
            "languageID": "Korean",
            "dialogueLines": [
                {
                    "characterID": "",
                    "backgroundID": "hauntedhallway",
                    "dialogueText": "강한 적들이었지만, 오로라가 경고한 만큼 강하지는 않았다.",
                    "emotionID": "",
                    "musicID": "haunted",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "hauntedhallway",
                    "dialogueText": "당분간은 괜찮을 것 같다. 오로라가 동의하듯 고개를 끄덕였다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "오로라",
                    "backgroundID": "hauntedhallway",
                    "dialogueText": "꿈꾸는 자의 말이 맞아요.<br>뭔가... 이상해요.",
                    "emotionID": "annoy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "오로라",
                    "backgroundID": "hauntedhallway",
                    "dialogueText": "이들은 이 구역의 존재들이 아니에요.<br>분명 소리가 세계의 주인에게 닿았을 텐데, 나타나지도 않았고요.",
                    "emotionID": "annoy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "오로라",
                    "backgroundID": "hauntedhallway",
                    "dialogueText": "여기에서 무슨 일이 벌어지고 있는 건지...",
                    "emotionID": "annoy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "오로라",
                    "backgroundID": "hauntedhallway",
                    "dialogueText": "어쩌면...",
                    "emotionID": "annoy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "오로라",
                    "backgroundID": "hauntedhallway",
                    "dialogueText": "비타, 아직 브라이어 로즈의 목소리가 들리나요?",
                    "emotionID": "sad",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "비타",
                    "backgroundID": "hauntedhallway",
                    "dialogueText": "응? 아! 맞다...",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Aurora",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Vita",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "hauntedhallway",
                    "dialogueText": "가만히 눈을 감고 서서 집중하던 비타가 어느 한 방향을 향해 고개를 돌렸다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Aurora",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Vita",
                    "outerRight": ""
                },
                {
                    "characterID": "비타",
                    "backgroundID": "hauntedhallway",
                    "dialogueText": "찾았다!<br>저 방향이야, 저기에서 소리가 들려.",
                    "emotionID": "idle",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Aurora",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Vita",
                    "outerRight": ""
                },
                {
                    "characterID": "오로라",
                    "backgroundID": "hauntedhallway",
                    "dialogueText": "좋아요, 그러면 안내를 부탁할게요.",
                    "emotionID": "sad",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Aurora",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Vita",
                    "outerRight": ""
                },
                {
                    "characterID": "오로라",
                    "backgroundID": "hauntedhallway",
                    "dialogueText": "최대한 조용히, 그리고 빨리 움직이도록 해요.",
                    "emotionID": "sad",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Aurora",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Vita",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "storybriarroseportal2",
                    "dialogueText": "비타의 안내를 따라 어떤 건물 근처에 도착했다.<br>그 위에 지난번처럼 기이한 빛을 내는 차원문이 떠 있었다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "storybriarroseportal2",
                    "dialogueText": "푸르스름한 빛이 차원문 너머에서 흘러나온다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "에스메",
                    "backgroundID": "storybriarroseportal2",
                    "dialogueText": "우리, 또 들어가는 거야?",
                    "emotionID": "annoy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Esme",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "storybriarroseportal2",
                    "dialogueText": "들어가는 것 외에는 방법이 없다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Esme",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "오로라",
                    "backgroundID": "storybriarroseportal2",
                    "dialogueText": "네, 저도 그렇게 생각해요.",
                    "emotionID": "annoy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Aurora",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Esme",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "dreamrealm",
                    "dialogueText": "결국 우리는 다시 차원문을 통과했다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "dreamrealm",
                    "dialogueText": "낯설지만 익숙한 곳.<br>오로라를 처음 만났던 무너지는 세계와 비슷했다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "dreamrealm",
                    "dialogueText": "다른 점이라면 지난번에는 드림리스가 들끓었지만 지금은 텅 비어있다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "오로라",
                    "backgroundID": "dreamrealm",
                    "dialogueText": "꿈꾸는 자, 여기는 지난번의 그 세계가 아니에요.",
                    "emotionID": "annoy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "오로라",
                    "backgroundID": "dreamrealm",
