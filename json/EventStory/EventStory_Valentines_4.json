    "m_Name": "EventStory_Valentines_4",
    "unskippable": 0,
    "dialogueDataEntries": [
        {
            "languageID": "English",
            "dialogueLines": [
                {
                    "characterID": "",
                    "backgroundID": "feywood",
                    "dialogueText": "Does everything in this forest want you dead?",
                    "emotionID": "",
                    "musicID": "threateningbattle",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "feywood",
                    "dialogueText": "Or rather... you think you know who it is who wants you dead...",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Odette",
                    "backgroundID": "feywood",
                    "dialogueText": "Dreamer, those shades again...",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Odette",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Odile",
                    "outerRight": ""
                },
                {
                    "characterID": "Odile",
                    "backgroundID": "feywood",
                    "dialogueText": "It seems my father has a hand in whatever is going on with Juliet and her beloved.",
                    "emotionID": "sad",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Odette",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Odile",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "feywood",
                    "dialogueText": "Why is he here? Is he trying to prevent you from reaching Romeo?",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Odette",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Odile",
                    "outerRight": ""
                },
                {
                    "characterID": "Odile",
                    "backgroundID": "feywood",
                    "dialogueText": "It could be so, though I have no idea why that would be his goal...",
                    "emotionID": "sad",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Odette",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Odile",
                    "outerRight": ""
                },
                {
                    "characterID": "Juliet",
                    "backgroundID": "feywood",
                    "dialogueText": "Nothing shall keep me from my love. Onwards!",
                    "emotionID": "annoy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "bounce",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Juliet",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "feywood",
                    "dialogueText": "Ah! Wait, Juliet!",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Juliet",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "feywood",
                    "dialogueText": "But she's already marching into the trees, her staff and knife held at the ready.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Juliet",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Siegfried",
                    "backgroundID": "feywood",
                    "dialogueText": "I guess we just follow her lead, Dreamer?",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_PrinceSiegfried",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Esme",
                    "backgroundID": "feywood",
                    "dialogueText": "We need to protect her!",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_PrinceSiegfried",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Esme",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "feywood",
                    "dialogueText": "Esme's right, this will all have been for nothing if Juliet marches straight into Rothbart!",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_PrinceSiegfried",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Esme",
                    "outerRight": ""
                },
                {
                    "characterID": "Odette",
                    "backgroundID": "feywood",
                    "dialogueText": "Dreamer, I'm sorry, we have to leave and dedicate ourselves to finding Rothbart.",
                    "emotionID": "sad",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Odette",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Odile",
                    "outerRight": ""
                },
                {
                    "characterID": "Odile",
                    "backgroundID": "feywood",
                    "dialogueText": "And bring him to justice!",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Odette",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Odile",
                    "outerRight": ""
                },
                {
                    "characterID": "Siegfried",
                    "backgroundID": "feywood",
                    "dialogueText": "Ah, I should also go with them. I have my own revenge to be sought from that man...",
                    "emotionID": "sad",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Odette",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_PrinceSiegfried",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Odile"
                },
                {
                    "characterID": "Aurora",
                    "backgroundID": "feywood",
                    "dialogueText": "Wait!",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Aurora",
                    "backgroundID": "feywood",
                    "dialogueText": "If Rothbart is truly after the star-crossed lovers, for whatever reason, surely staying with us is the quickest route to finding him?",
                    "emotionID": "sad",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "feywood",
                    "dialogueText": "Odette, Odile and Siegfried all look at one another.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Odette",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_PrinceSiegfried",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Odile"
                },
                {
                    "characterID": "Odette",
                    "backgroundID": "feywood",
                    "dialogueText": "I... I suppose you're right.",
                    "emotionID": "sad",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Odette",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_PrinceSiegfried",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Odile"
                },
                {
                    "characterID": "Odette",
                    "backgroundID": "feywood",
                    "dialogueText": "But if that changes, we may have to leave at a moment's notice.",
                    "emotionID": "idle",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Odette",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_PrinceSiegfried",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Odile"
                },
                {
                    "characterID": "Aurora",
                    "backgroundID": "feywood",
                    "dialogueText": "Then please lend us your aid until that time.",
                    "emotionID": "idle",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Odette",
                    "backgroundID": "feywood",
                    "dialogueText": "Odile? Siegfried?",
                    "emotionID": "sad",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Odette",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_PrinceSiegfried",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Odile"
                },
                {
                    "characterID": "",
                    "backgroundID": "feywood",
                    "dialogueText": "They nod their agreement.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Odette",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_PrinceSiegfried",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Odile"
                },
                {
                    "characterID": "",
                    "backgroundID": "feywood",
                    "dialogueText": "There comes a now familiar cry from the forest.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Juliet",
                    "backgroundID": "feywood",
                    "dialogueText": "Dreamer!",
                    "emotionID": "surprise",
                    "musicID": "none",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "feywood",
                    "dialogueText": "It isn't a cry of alarm, it is a cry of discovery.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "feywood",
                    "dialogueText": "You rush to Juliet's side.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "abandonedcity",
                    "dialogueText": "As you do, you emerge from the trees and see buildings ahead, all wrapped in the briar that swathes this realm.",
                    "emotionID": "",
                    "musicID": "intothewild",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Juliet",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "abandonedcity",
                    "dialogueText": "Buildings and a tall tower...",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Juliet",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "abandonedcity",
                    "dialogueText": "You begin asking what this place is, but Juliet hushes you.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Juliet",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Juliet",
                    "backgroundID": "abandonedcity",
                    "dialogueText": "Speak soft! I saw a light in yonder tower!",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Juliet",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Esme",
                    "backgroundID": "abandonedcity",
                    "dialogueText": "It's him! He's inside!",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Juliet",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Esme",
                    "outerRight": ""
                },
                {
                    "characterID": "Juliet",
                    "backgroundID": "abandonedcity",
                    "dialogueText": "Romeo's within?",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Juliet",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Esme",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "abandonedcity",
                    "dialogueText": "Ah! You don't think that was a smart move from Esme...",
                    "emotionID": "",
                    "musicID": "threateningbattle",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Juliet",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Esme",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "abandonedcity",
                    "dialogueText": "Sure enough, Juliet rushes forward and as she does, Shades and Nightmares pour out of the tower and attack!",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_NPC_NightmareVassal",
                    "innerRight": "",
                    "outerRight": ""
                }
            ]
        },
        {
            "languageID": "Korean",
            "dialogueLines": [
                {
                    "characterID": "",
                    "backgroundID": "feywood",
                    "dialogueText": "오딜과 오데트가 안내한 장소에는 검은 그림자 같은 것들이 엉겨있었다.",
                    "emotionID": "",
                    "musicID": "threateningbattle",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "feywood",
                    "dialogueText": "덩굴 모양을 한 가시들이 빼곡하게 들어차 근처의 살아있는 것들을 무차별하게 공격한다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "오데트",
                    "backgroundID": "feywood",
                    "dialogueText": "로트바르트의 그림자에요.",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Odette",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Odile",
                    "outerRight": ""
                },
                {
                    "characterID": "오딜",
                    "backgroundID": "feywood",
                    "dialogueText": "아무래도 제 아버지가 별의 연인들에게 볼 일이 있으신 모양이에요.",
                    "emotionID": "sad",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Odette",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Odile",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "feywood",
                    "dialogueText": "슬픈 표정으로 이야기하는 오딜의 어깨를 오데트가 감싸안는다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Odette",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Odile",
                    "outerRight": ""
                },
                {
                    "characterID": "오딜",
                    "backgroundID": "feywood",
                    "dialogueText": "정확한 이유는 알 수 없지만... 세간의 이목을 집중시키기에 좋은 일이니, 그걸 노리시는 것 같아요.",
                    "emotionID": "sad",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Odette",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Odile",
                    "outerRight": ""
                },
                {
                    "characterID": "줄리엣",
                    "backgroundID": "feywood",
                    "dialogueText": "... 시간이 아무리 많이 흘러도, 저희의 이야기는 여전히 그렇게 소비되는군요.",
                    "emotionID": "annoy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "bounce",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Juliet",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "feywood",
                    "dialogueText": "앞에서 열심히 그림자들을 막고 있던 지그프리드 왕자가 줄리엣을 힐끔거린다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Juliet",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "feywood",
                    "dialogueText": "여전히 오딜과 오데트가 친근하게 서있는 것이 어색한 것 같았다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Juliet",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "지그프리드 왕자",
                    "backgroundID": "feywood",
                    "dialogueText": "앗, 그... 아무것도 아닙니다.",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_PrinceSiegfried",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "에스메",
                    "backgroundID": "feywood",
                    "dialogueText": "우선 이 앞으로 가자, 계속 여기 있으면 아무것도 알 수 없잖아.",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_PrinceSiegfried",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Esme",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "feywood",
                    "dialogueText": "확실히 서두르는게 좋겠지. 로트바르트가 근처에 있다면 줄리엣도 위험해질 수 있다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_PrinceSiegfried",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Esme",
                    "outerRight": ""
                },
                {
                    "characterID": "오데트",
                    "backgroundID": "feywood",
                    "dialogueText": "자세한 사정은 천천히 설명해주세요, 꿈꾸는 자.",
                    "emotionID": "sad",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Odette",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Odile",
                    "outerRight": ""
                },
                {
                    "characterID": "오딜",
                    "backgroundID": "feywood",
                    "dialogueText": "이번에는 절대 아버지를 그냥 보내지 않을거에요. 맹세할게요.",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Odette",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Odile",
                    "outerRight": ""
                },
                {
                    "characterID": "지그프리드 왕자",
                    "backgroundID": "feywood",
                    "dialogueText": "예, 저도 협력하겠습니다. 제 도움이 필요하시다면...",
                    "emotionID": "sad",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Odette",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_PrinceSiegfried",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Odile"
                },
                {
                    "characterID": "오로라",
                    "backgroundID": "feywood",
                    "dialogueText": "로트바르트가 만일 로미오의 기억 파편을 먼저 발견했다면...",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "오로라",
                    "backgroundID": "feywood",
