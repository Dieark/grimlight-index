    "m_Name": "EventStory_BriarRose_3",
    "unskippable": 0,
    "dialogueDataEntries": [
        {
            "languageID": "English",
            "dialogueLines": [
                {
                    "characterID": "",
                    "backgroundID": "hauntedhallway",
                    "dialogueText": "It feels strange passing through the portal. The world of sensation seems to shift and alter around you.",
                    "emotionID": "",
                    "musicID": "haunted",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "hauntedhallway",
                    "dialogueText": "One moment the air is pleasant and warm, the next it is cool and clammy. The light levels shift as well, and all noise dampens as if you're travelling through something that baffles the sound.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "hauntedhallway",
                    "dialogueText": "Once you step through to the other side and look around, the sound returns, the previous noise of gentle breeze and empty echoes replaced with a moaning, sighing wind.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "hauntedhallway",
                    "dialogueText": "Aurora immediately tenses.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Aurora",
                    "backgroundID": "hauntedhallway",
                    "dialogueText": "No! This isn't right!",
                    "emotionID": "surprise",
                    "musicID": "shadowsawait",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Aurora",
                    "backgroundID": "hauntedhallway",
                    "dialogueText": "Dreamer, we're in terrible danger... we should return to the ruined city, immediately!",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "hauntedhallway",
                    "dialogueText": "But when you turn around, the portal is no longer there.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Aurora",
                    "backgroundID": "hauntedhallway",
                    "dialogueText": "No no no!",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Esme",
                    "backgroundID": "hauntedhallway",
                    "dialogueText": "What is it, Aurora? Where are we?",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Esme",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Vita"
                },
                {
                    "characterID": "Aurora",
                    "backgroundID": "hauntedhallway",
                    "dialogueText": "This place... it's controlled by very dangerous beings. They don't take well to uninvited guests...",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Esme",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Vita"
                },
                {
                    "characterID": "Aurora",
                    "backgroundID": "hauntedhallway",
                    "dialogueText": "Everyone! We need to be as quiet as possible and search for a way out.",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Esme",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Vita"
                },
                {
                    "characterID": "Aurora",
                    "backgroundID": "hauntedhallway",
                    "dialogueText": "The quicker we escape this place, the better.",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Esme",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Vita"
                },
                {
                    "characterID": "Aurora",
                    "backgroundID": "hauntedhallway",
                    "dialogueText": "Vita! Can you find the way?",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Esme",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Vita"
                },
                {
                    "characterID": "Vita",
                    "backgroundID": "hauntedhallway",
                    "dialogueText": "I... I think I can...",
                    "emotionID": "annoy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Esme",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Vita"
                },
                {
                    "characterID": "Esme",
                    "backgroundID": "hauntedhallway",
                    "dialogueText": "It's too late! Something's coming!",
                    "emotionID": "angry",
                    "musicID": "threateningbattle",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Esme",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Vita"
                },
                {
                    "characterID": "",
                    "backgroundID": "hauntedhallway",
                    "dialogueText": "And sure enough, another wave of nightmarish creatures descend upon you, seemingly from every shadow.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_NPC_NightmareVassal",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "hauntedhallway",
                    "dialogueText": "And there are a lot of shadows!",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_NPC_NightmareVassal",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Aurora",
                    "backgroundID": "hauntedhallway",
                    "dialogueText": "Oh no!",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": ""
                }
            ]
        },
        {
            "languageID": "Korean",
            "dialogueLines": [
                {
                    "characterID": "",
                    "backgroundID": "hauntedhallway",
                    "dialogueText": "차원문을 넘자 기이한 감각이 전신을 감싼다.<br>그 공간 안에 존재하는 모든 감각이 살아 움직이는 것 같은 느낌이 든다.",
                    "emotionID": "",
                    "musicID": "haunted",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "hauntedhallway",
                    "dialogueText": "따뜻하다고 느낀 것은 아주 잠시였을 뿐,<br>차갑고 습한 감촉이 손끝에 닿았다.<br>주변의 풍경도 조금씩 더 어두워진다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "hauntedhallway",
                    "dialogueText": "소리가 사라진 것 같은 신비로운 세계.<br>비명처럼 차가운 바람이 공허하게 휘몰아친다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "hauntedhallway",
                    "dialogueText": "갑자기 오로라가 다급한 목소리로 나를 불러 세웠다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "오로라",
                    "backgroundID": "hauntedhallway",
                    "dialogueText": "꿈꾸는 자, 뭔가 이상해요!",
                    "emotionID": "surprise",
                    "musicID": "shadowsawait",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "오로라",
                    "backgroundID": "hauntedhallway",
                    "dialogueText": "지금 당장 도시로 돌아가야 해요...!",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "hauntedhallway",
                    "dialogueText": "뒤를 돌아 주변을 살폈지만, 차원문은 이미 사라진 뒤였다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "오로라",
                    "backgroundID": "hauntedhallway",
                    "dialogueText": "아, 이럴수가. 설마...",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "에스메",
                    "backgroundID": "hauntedhallway",
                    "dialogueText": "왜 그래, 오로라?<br>대체 여기가 어디길래? 어딘지 알아?",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Esme",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Vita"
                },
                {
                    "characterID": "오로라",
                    "backgroundID": "hauntedhallway",
                    "dialogueText": "... 이 세계를 통제하는 사람은 아주 위험한 존재예요.",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Esme",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Vita"
                },
                {
                    "characterID": "오로라",
                    "backgroundID": "hauntedhallway",
                    "dialogueText": "우리처럼 초대받지 않은 사람들은, 곧...",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Esme",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Vita"
                },
                {
                    "characterID": "오로라",
                    "backgroundID": "hauntedhallway",
                    "dialogueText": "서둘러 이곳을 빠져나갈 방법을 찾아야해요.",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Esme",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Vita"
                },
                {
                    "characterID": "오로라",
                    "backgroundID": "hauntedhallway",
                    "dialogueText": "비타, 출구를 찾을 수 있나요?",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Esme",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Vita"
                },
                {
                    "characterID": "비타",
                    "backgroundID": "hauntedhallway",
                    "dialogueText": "응? 아, 알았어.<br>잠시만 기다려봐.",
                    "emotionID": "annoy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Esme",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Vita"
                },
                {
                    "characterID": "에스메",
                    "backgroundID": "hauntedhallway",
                    "dialogueText": "...응? 뭔가 우리를 향해 오고 있는 것 같아. 조심해, 꿈꾸는 자!",
                    "emotionID": "angry",
                    "musicID": "threateningbattle",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Esme",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Vita"
                },
                {
                    "characterID": "",
                    "backgroundID": "hauntedhallway",
                    "dialogueText": "에스메의 비명 소리와 함께 어둠이 달려들었다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_NPC_NightmareVassal",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "hauntedhallway",
                    "dialogueText": "어둠이라고 생각했던 것은 모두 그림자였다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_NPC_NightmareVassal",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "오로라",
                    "backgroundID": "hauntedhallway",
                    "dialogueText": "아, 안돼...!",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": ""
                }
            ]
        }
    ]
}