    "m_Name": "EventStory_EvilQueen_8",
    "unskippable": 0,
    "dialogueDataEntries": [
        {
            "languageID": "English",
            "dialogueLines": [
                {
                    "characterID": "",
                    "backgroundID": "woodlandscreepy",
                    "dialogueText": "The Dreamless Queen shrieks and falls to the forest floor. The sheer piercing force of the wail she lets out as Snow's mace brings her low is enough to leave your ears ringing for minutes afterwards.",
                    "emotionID": "",
                    "musicID": "ominous",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_SnowWhite",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "woodlandscreepy",
                    "dialogueText": "As their commander falls, the massive Dreamless army falls into confusion and disarray, completely losing their cohesion.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "woodlandscreepy",
                    "dialogueText": "The battle quickly becomes a rout.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "woodlandscreepy",
                    "dialogueText": "Snow White steps up to the prostrate form of her old enemy, looking down at the writing seething monster Narcissa has become.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_SnowWhite",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "woodlandscreepy",
                    "dialogueText": "Snow White lowers her mace as she looks down sadly at the last dregs of life leaving the Evil Queen's body... and at that moment, the emaciated form of Narcissa rears up and lunges at her!",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_SnowWhite",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "woodlandscreepy",
                    "dialogueText": "Even Snow can't react fast enough to defeat the claws aimed at her heart... but at that moment there is a twang and a thud and the wraithlike remains of her nemesis is pitched backwards, with a crossbow bolt sprouting from what remains of her own heart...",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_SnowWhite",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "The Huntsman",
                    "backgroundID": "woodlandscreepy",
                    "dialogueText": "...",
                    "emotionID": "idle",
                    "musicID": "none",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Huntsman",
                    "center": "",
                    "innerRight": "Fullbody_Hero_SnowWhite",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "woodlandscreepy",
                    "dialogueText": "Snow White looks around at her love.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Huntsman",
                    "center": "",
                    "innerRight": "Fullbody_Hero_SnowWhite",
                    "outerRight": ""
                },
                {
                    "characterID": "Snow White",
                    "backgroundID": "woodlandscreepy",
                    "dialogueText": "Hunter...",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Huntsman",
                    "center": "",
                    "innerRight": "Fullbody_Hero_SnowWhite",
                    "outerRight": ""
                },
                {
                    "characterID": "The Huntsman",
                    "backgroundID": "woodlandscreepy",
                    "dialogueText": "It's over. She won't torture us anymore.",
                    "emotionID": "idle",
                    "musicID": "sentimentalmemories",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Huntsman",
                    "center": "",
                    "innerRight": "Fullbody_Hero_SnowWhite",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "woodlandscreepy",
                    "dialogueText": "And, as you look around, you see that he is right.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Huntsman",
                    "center": "",
                    "innerRight": "Fullbody_Hero_SnowWhite",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "woodlandscreepy",
                    "dialogueText": "The Dreamless are defeated, the Evil Queen is dead... once again the day is saved, at least for now.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Huntsman",
                    "center": "",
                    "innerRight": "Fullbody_Hero_SnowWhite",
                    "outerRight": ""
                },
                {
                    "characterID": "Snow White",
                    "backgroundID": "woodlandscreepy",
                    "dialogueText": "Let us return to Haven, Dreamer.",
                    "emotionID": "happy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_SnowWhite",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Aurora",
                    "backgroundID": "woodlandscreepy",
                    "dialogueText": "Yes, I'm sure a celebration is in order!",
                    "emotionID": "happy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "woodlandscreepy",
                    "dialogueText": "...",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "woodlandscreepy",
                    "dialogueText": "And so, the story of the Evil Queen, whose pride and resentment could never release its grip from her withered heart, finally comes to a close.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "woodlandscreepy",
                    "dialogueText": "You return to the city with your allies to much celebration.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "woodlandscreepy",
                    "dialogueText": "But somewhere in the forest outside the walls of the last bastion of humanity, two figures stand over the crumpled form of their latest failed experiement.",
                    "emotionID": "",
                    "musicID": "none",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Rothbart",
                    "backgroundID": "woodlandscreepy",
                    "dialogueText": "A shame she failed to retrieve the stones.",
                    "emotionID": "idle",
                    "musicID": "haunted",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Rothbart",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Bathory",
                    "backgroundID": "woodlandscreepy",
                    "dialogueText": "She would have tried to keep them for herself.",
                    "emotionID": "annoy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Bathory",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Rothbart",
                    "outerRight": ""
                },
                {
                    "characterID": "Rothbart",
                    "backgroundID": "woodlandscreepy",
                    "dialogueText": "And my shades would have easily taken them from her, anyways...",
                    "emotionID": "idle",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Bathory",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Rothbart",
                    "outerRight": ""
                },
                {
                    "characterID": "Rothbart",
                    "backgroundID": "woodlandscreepy",
                    "dialogueText": "Such a pity. I thought she would be stronger.",
                    "emotionID": "happy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Bathory",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Rothbart",
                    "outerRight": ""
                },
                {
                    "characterID": "Bathory",
                    "backgroundID": "woodlandscreepy",
                    "dialogueText": "Then why are you smiling?",
                    "emotionID": "annoy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Bathory",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Rothbart",
                    "outerRight": ""
                },
                {
                    "characterID": "Rothbart",
                    "backgroundID": "woodlandscreepy",
                    "dialogueText": "Because at least we know the Dreamer's powers of resurrection are no longer unique...",
                    "emotionID": "happy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Bathory",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Rothbart",
                    "outerRight": ""
                },
                {
                    "characterID": "Rothbart",
                    "backgroundID": "woodlandscreepy",
                    "dialogueText": "There will be others to take her place. Our plans will still come to fruition.",
                    "emotionID": "idle",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Bathory",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Rothbart",
                    "outerRight": ""
                },
                {
                    "characterID": "Rothbart",
                    "backgroundID": "woodlandscreepy",
                    "dialogueText": "<i>My</i> plans will...",
                    "emotionID": "idle",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Bathory",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Rothbart",
                    "outerRight": ""
                },
                {
                    "characterID": "Bathory",
                    "backgroundID": "woodlandscreepy",
                    "dialogueText": "...",
                    "emotionID": "idle",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Bathory",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Rothbart",
                    "outerRight": ""
                },
                {
                    "characterID": "Bathory",
                    "backgroundID": "woodlandscreepy",
                    "dialogueText": "I must report to my master. He will want to know about this outcome.",
                    "emotionID": "happy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Bathory",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Rothbart",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "woodlandscreepy",
                    "dialogueText": "Bathory gives the wizard a curt nod and withdraws.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Bathory",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Rothbart",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "woodlandscreepy",
                    "dialogueText": "Rothbart watches her go.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Rothbart",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Rothbart",
                    "backgroundID": "woodlandscreepy",
                    "dialogueText": "Your master may be a remarkable individual, but he may not have all things go his way...",
                    "emotionID": "idle",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Rothbart",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Rothbart",
                    "backgroundID": "woodlandscreepy",
                    "dialogueText": "Not at the end of all things.",
                    "emotionID": "happy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Rothbart",
                    "innerRight": "",
                    "outerRight": ""
                }
            ]
        },
        {
            "languageID": "Korean",
            "dialogueLines": [
                {
                    "characterID": "",
                    "backgroundID": "woodlandscreepy",
                    "dialogueText": "위태롭게 서있던 이블 퀸이 비명을 마지막으로 힘없이 쓰러졌다.",
                    "emotionID": "",
                    "musicID": "ominous",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_SnowWhite",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "woodlandscreepy",
                    "dialogueText": "드림리스에게 명령하던 이블 퀸이 쓰러지자 드림리스 군대는 곧 오합지졸이 되고 말았다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "woodlandscreepy",
                    "dialogueText": "숲의 다른 곳에서 벌어진 전투도 하나 둘 마무리 되고 있다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "woodlandscreepy",
                    "dialogueText": "드림리스의 힘으로 변질된 나르시사를 슬픈 눈으로 바라보던 백설공주가 그녀의 앞에 무릎을 꿇었을 때,",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_SnowWhite",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "woodlandscreepy",
                    "dialogueText": "나르시사가 손을 뻗어 백설공주를 공격하려 했으나.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_SnowWhite",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "woodlandscreepy",
                    "dialogueText": "바람을 가르는 날렵한 소리와 함께 날아든 화살이 나르시사의 심장에 박혔다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_SnowWhite",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "사냥꾼",
                    "backgroundID": "woodlandscreepy",
                    "dialogueText": "...",
                    "emotionID": "idle",
                    "musicID": "none",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Huntsman",
                    "center": "",
                    "innerRight": "Fullbody_Hero_SnowWhite",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "woodlandscreepy",
                    "dialogueText": "백설공주가 그녀의 연인을 바라본다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Huntsman",
                    "center": "",
                    "innerRight": "Fullbody_Hero_SnowWhite",
                    "outerRight": ""
                },
                {
                    "characterID": "백설공주",
                    "backgroundID": "woodlandscreepy",
                    "dialogueText": "사냥꾼 씨...",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Huntsman",
                    "center": "",
                    "innerRight": "Fullbody_Hero_SnowWhite",
                    "outerRight": ""
                },
                {
                    "characterID": "사냥꾼",
                    "backgroundID": "woodlandscreepy",
                    "dialogueText": "... 이걸로 끝이다. 더 이상 널 공격할 수 없을테니.",
                    "emotionID": "idle",
                    "musicID": "sentimentalmemories",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Huntsman",
                    "center": "",
                    "innerRight": "Fullbody_Hero_SnowWhite",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "woodlandscreepy",
                    "dialogueText": "드림리스 군대는 무너졌고, 그들을 이끌던 이블 퀸도 쓰러트렸다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Huntsman",
                    "center": "",
                    "innerRight": "Fullbody_Hero_SnowWhite",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "woodlandscreepy",
                    "dialogueText": "적어도 지금 당장은 안심할 수 있게 되었다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Huntsman",
                    "center": "",
                    "innerRight": "Fullbody_Hero_SnowWhite",
                    "outerRight": ""
                },
                {
                    "characterID": "백설공주",
                    "backgroundID": "woodlandscreepy",
                    "dialogueText": "안식의 도시로 돌아가요, 꿈꾸는 자.",
                    "emotionID": "happy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_SnowWhite",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "오로라",
                    "backgroundID": "woodlandscreepy",
                    "dialogueText": "부상자들은 제가 돌볼게요. 승리를 축하하는 연회도 빼놓을 수 없겠죠?",
                    "emotionID": "happy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "woodlandscreepy",
                    "dialogueText": "일행들이 전투가 끝난 숲을 수습했다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "woodlandscreepy",
                    "dialogueText": "자만심과 분노, 원망으로 만들어진 여왕의 오래된 꿈은 이루어지지 못했다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "woodlandscreepy",
                    "dialogueText": "도시로 돌아가기 직전, 숲 너머에서 시선이 느껴진 것 같았지만 정체를 알아낼 수는 없었다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "woodlandscreepy",
                    "dialogueText": "모두가 사라진 뒤 숲에 적막이 내려앉자 두 사람이 어둠 속에 모습을 드러냈다.",
                    "emotionID": "",
                    "musicID": "none",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "악인 로트바르트",
                    "backgroundID": "woodlandscreepy",
                    "dialogueText": "스톤을 탈취하지 못해 아쉽군.",
                    "emotionID": "idle",
                    "musicID": "haunted",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Rothbart",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "엘리자베스 바토리",
