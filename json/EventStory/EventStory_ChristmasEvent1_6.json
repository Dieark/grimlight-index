    "m_Name": "EventStory_ChristmasEvent1_6",
    "unskippable": 0,
    "dialogueDataEntries": [
        {
            "languageID": "English",
            "dialogueLines": [
                {
                    "characterID": "",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "Out in the wilds, Krampus, Big Bad Wolf and a large army of Dreamless rat soldiers are making steady progress towards the giant Christmas tree clearing.",
                    "emotionID": "",
                    "musicID": "intothewild",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Krampus",
                    "center": "",
                    "innerRight": "Fullbody_Hero_BigBadWolf",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "christmastree",
                    "dialogueText": "As they break the tree cover and step into the clearing, they find it empty.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Krampus",
                    "center": "",
                    "innerRight": "Fullbody_Hero_BigBadWolf",
                    "outerRight": ""
                },
                {
                    "characterID": "Krampus",
                    "backgroundID": "christmastree",
                    "dialogueText": "There it is!",
                    "emotionID": "conspire",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "bounce",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Krampus",
                    "center": "",
                    "innerRight": "Fullbody_Hero_BigBadWolf",
                    "outerRight": ""
                },
                {
                    "characterID": "Krampus",
                    "backgroundID": "christmastree",
                    "dialogueText": "The tree protects the entrance to Santa's Workshop.",
                    "emotionID": "idle",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Krampus",
                    "center": "",
                    "innerRight": "Fullbody_Hero_BigBadWolf",
                    "outerRight": ""
                },
                {
                    "characterID": "Krampus",
                    "backgroundID": "christmastree",
                    "dialogueText": "We should destroy it so we can get in and trash the place!",
                    "emotionID": "conspire",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Krampus",
                    "center": "",
                    "innerRight": "Fullbody_Hero_BigBadWolf",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "christmastree",
                    "dialogueText": "It is at that point that you decide to reveal yourself.",
                    "emotionID": "",
                    "musicID": "none",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Krampus",
                    "center": "",
                    "innerRight": "Fullbody_Hero_BigBadWolf",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "christmastree",
                    "dialogueText": "Rising from behind a snow bank, you stand between the Krampus and the giant Christmas tree.",
                    "emotionID": "",
                    "musicID": "none",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Krampus",
                    "center": "",
                    "innerRight": "Fullbody_Hero_BigBadWolf",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "christmastree",
                    "dialogueText": "She won't be going any further. You'll see to that.",
                    "emotionID": "",
                    "musicID": "none",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Krampus",
                    "center": "",
                    "innerRight": "Fullbody_Hero_BigBadWolf",
                    "outerRight": ""
                },
                {
                    "characterID": "Krampus",
                    "backgroundID": "christmastree",
                    "dialogueText": "You! You're that... person... everyone's been talking about!",
                    "emotionID": "surprise",
                    "musicID": "intothewild",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Krampus",
                    "center": "",
                    "innerRight": "Fullbody_Hero_BigBadWolf",
                    "outerRight": ""
                },
                {
                    "characterID": "Krampus",
                    "backgroundID": "christmastree",
                    "dialogueText": "The driveler... drudger... droner...?",
                    "emotionID": "pout",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Krampus",
                    "center": "",
                    "innerRight": "Fullbody_Hero_BigBadWolf",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "christmastree",
                    "dialogueText": "Dreamer.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Krampus",
                    "center": "",
                    "innerRight": "Fullbody_Hero_BigBadWolf",
                    "outerRight": ""
                },
                {
                    "characterID": "Krampus",
                    "backgroundID": "christmastree",
                    "dialogueText": "That's it!",
                    "emotionID": "happy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Krampus",
                    "center": "",
                    "innerRight": "Fullbody_Hero_BigBadWolf",
                    "outerRight": ""
                },
                {
                    "characterID": "Nutcracker",
                    "backgroundID": "christmastree",
                    "dialogueText": "Ah! I know you!",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Nutcracker",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Nutcracker",
                    "backgroundID": "christmastree",
                    "dialogueText": "You're the Krampus! You used to be one of Santa's helpers!",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Nutcracker",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Krampus",
                    "backgroundID": "christmastree",
                    "dialogueText": "That's right, what's it to you?",
                    "emotionID": "annoy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Krampus",
                    "center": "",
                    "innerRight": "Fullbody_Hero_BigBadWolf",
                    "outerRight": ""
                },
                {
                    "characterID": "Nutcracker",
                    "backgroundID": "christmastree",
                    "dialogueText": "You'll pay for betraying Santa!",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Nutcracker",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Krampus",
                    "backgroundID": "christmastree",
                    "dialogueText": "Not if I ruin Christmas first, I won't!",
                    "emotionID": "conspire",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Krampus",
                    "center": "",
                    "innerRight": "Fullbody_Hero_BigBadWolf",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "christmastree",
                    "dialogueText": "Uh... you don't think those two statements are causally linked...",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Krampus",
                    "center": "",
                    "innerRight": "Fullbody_Hero_BigBadWolf",
                    "outerRight": ""
                },
                {
                    "characterID": "Krampus",
                    "backgroundID": "christmastree",
                    "dialogueText": "Who cares! We're here to ruin Christmas and you won't be able to stop us!",
                    "emotionID": "annoy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Krampus",
                    "center": "",
                    "innerRight": "Fullbody_Hero_BigBadWolf",
                    "outerRight": ""
                },
                {
                    "characterID": "Big Bad Wolf",
                    "backgroundID": "christmastree",
                    "dialogueText": "Krampus, I've had battles with that Dreamer before! He beat me up easily!",
                    "emotionID": "shy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Krampus",
                    "center": "",
                    "innerRight": "Fullbody_Hero_BigBadWolf",
                    "outerRight": ""
                },
                {
                    "characterID": "Krampus",
                    "backgroundID": "christmastree",
                    "dialogueText": "But I bet you didn't have an invincible rat army that time, right?",
                    "emotionID": "conspire",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Krampus",
                    "center": "",
                    "innerRight": "Fullbody_Hero_BigBadWolf",
                    "outerRight": ""
                },
                {
                    "characterID": "Big Bad Wolf",
                    "backgroundID": "christmastree",
                    "dialogueText": "I-I suppose...",
                    "emotionID": "shy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Krampus",
                    "center": "",
                    "innerRight": "Fullbody_Hero_BigBadWolf",
                    "outerRight": ""
                },
                {
                    "characterID": "Krampus",
                    "backgroundID": "christmastree",
                    "dialogueText": "Don't worry, Wolfie, there's only a few of them and our numbers are nearly limitless!",
                    "emotionID": "idle",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Krampus",
                    "center": "",
                    "innerRight": "Fullbody_Hero_BigBadWolf",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "christmastree",
                    "dialogueText": "She talks big for such a little girl.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Krampus",
                    "center": "",
                    "innerRight": "Fullbody_Hero_BigBadWolf",
                    "outerRight": ""
                },
                {
                    "characterID": "Krampus",
                    "backgroundID": "christmastree",
                    "dialogueText": "SH-SHUT UP!!",
                    "emotionID": "angry",
                    "musicID": "christmasbattle",
                    "sfxID": "",
                    "animationID": "shake",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Krampus",
                    "center": "",
                    "innerRight": "Fullbody_Hero_BigBadWolf",
                    "outerRight": ""
                },
                {
                    "characterID": "Krampus",
                    "backgroundID": "christmastree",
                    "dialogueText": "You're all getting coal this year!!",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Krampus",
                    "center": "",
                    "innerRight": "Fullbody_Hero_BigBadWolf",
                    "outerRight": ""
                },
                {
                    "characterID": "Krampus",
                    "backgroundID": "christmastree",
                    "dialogueText": "Get them, minions!",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Krampus",
                    "center": "",
                    "innerRight": "Fullbody_Hero_BigBadWolf",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "christmastree",
                    "dialogueText": "The hordes of Dreamless sweep down upon you and your allies...",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_NPC_RatSoldier",
                    "innerRight": "",
                    "outerRight": ""
                }
            ]
        },
        {
            "languageID": "Korean",
            "dialogueLines": [
                {
                    "characterID": "",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "크람푸스와 나쁜 늑대 두 사람은 드림리스 군단을 이끌며 크리스마스트리를 향해 나아갔다.",
                    "emotionID": "",
                    "musicID": "intothewild",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Krampus",
                    "center": "",
                    "innerRight": "Fullbody_Hero_BigBadWolf",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "christmastree",
                    "dialogueText": "마침내 거대한 트리 아래에 도착하자 텅 빈 공터가 그들을 반겼다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Krampus",
                    "center": "",
                    "innerRight": "Fullbody_Hero_BigBadWolf",
                    "outerRight": ""
                },
                {
                    "characterID": "크람푸스",
                    "backgroundID": "christmastree",
                    "dialogueText": "도착했어!",
                    "emotionID": "conspire",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "bounce",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Krampus",
                    "center": "",
                    "innerRight": "Fullbody_Hero_BigBadWolf",
                    "outerRight": ""
                },
                {
                    "characterID": "크람푸스",
                    "backgroundID": "christmastree",
                    "dialogueText": "이 트리가 산타의 작업실을 보호하는 마법의 근원이야.",
                    "emotionID": "idle",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Krampus",
                    "center": "",
                    "innerRight": "Fullbody_Hero_BigBadWolf",
                    "outerRight": ""
                },
                {
                    "characterID": "크람푸스",
                    "backgroundID": "christmastree",
                    "dialogueText": "트리부터 부숴버리자!",
                    "emotionID": "conspire",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Krampus",
                    "center": "",
                    "innerRight": "Fullbody_Hero_BigBadWolf",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "christmastree",
                    "dialogueText": "크람푸스가 공터 안으로 뛰어들자,",
                    "emotionID": "",
                    "musicID": "none",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Krampus",
                    "center": "",
                    "innerRight": "Fullbody_Hero_BigBadWolf",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "christmastree",
                    "dialogueText": "거대한 눈 벽 뒤에서 한 사람이 걸어 나와 그들을 막아섰다.",
                    "emotionID": "",
                    "musicID": "none",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Krampus",
                    "center": "",
                    "innerRight": "Fullbody_Hero_BigBadWolf",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "christmastree",
                    "dialogueText": "눈을 가늘게 뜨고 바라보던 크람푸스가 소스라치며 늑대의 팔을 움켜쥐었다.",
                    "emotionID": "",
                    "musicID": "none",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Krampus",
                    "center": "",
                    "innerRight": "Fullbody_Hero_BigBadWolf",
                    "outerRight": ""
                },
                {
                    "characterID": "크람푸스",
                    "backgroundID": "christmastree",
                    "dialogueText": "엑, 설마! 저거 꿈꾸는 잠탱이... 꿈탱이...",
                    "emotionID": "surprise",
                    "musicID": "intothewild",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Krampus",
                    "center": "",
                    "innerRight": "Fullbody_Hero_BigBadWolf",
                    "outerRight": ""
                },
                {
                    "characterID": "크람푸스",
                    "backgroundID": "christmastree",
                    "dialogueText": "아무튼! 바토리가 조심하라고 말했던 그거 아냐? 그거!",
                    "emotionID": "pout",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Krampus",
                    "center": "",
                    "innerRight": "Fullbody_Hero_BigBadWolf",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "christmastree",
                    "dialogueText": "늑대가 작은 목소리로 꿈꾸는 자라고 일러 주었다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Krampus",
                    "center": "",
                    "innerRight": "Fullbody_Hero_BigBadWolf",
                    "outerRight": ""
                },
                {
                    "characterID": "크람푸스",
                    "backgroundID": "christmastree",
                    "dialogueText": "그, 그래. 꿈꾸는 자!",
                    "emotionID": "happy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Krampus",
                    "center": "",
                    "innerRight": "Fullbody_Hero_BigBadWolf",
                    "outerRight": ""
                },
                {
                    "characterID": "호두까기 인형",
                    "backgroundID": "christmastree",
                    "dialogueText": "당신, 크람푸스가 아닙니까?",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Nutcracker",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "호두까기 인형",
                    "backgroundID": "christmastree",
                    "dialogueText": "당신은 자신을 산타의 조력자라고 소개 했었습니다. 지금 이 행동에 관해 설명해주십시오.",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Nutcracker",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "크람푸스",
                    "backgroundID": "christmastree",
                    "dialogueText": "예, 옛날에는 그랬지... 으으, 짜증 나! 이제와서 그런 게 무슨 상관이야?",
                    "emotionID": "annoy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Krampus",
                    "center": "",
                    "innerRight": "Fullbody_Hero_BigBadWolf",
                    "outerRight": ""
                },
                {
                    "characterID": "호두까기 인형",
                    "backgroundID": "christmastree",
                    "dialogueText": "동료를 배신한 겁니까! 그렇다면 절대 당신을 용서하지 않겠습니다.",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Nutcracker",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "크람푸스",
                    "backgroundID": "christmastree",
                    "dialogueText": "사.. 상관없어! 내가 먼저 크리스마스를 망치기만 하면 어차피 다 끝이잖아!",
                    "emotionID": "conspire",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Krampus",
                    "center": "",
                    "innerRight": "Fullbody_Hero_BigBadWolf",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "christmastree",
                    "dialogueText": "크리스마스와 산타를 배신한 건 별 상관없는 이야기 같은데.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Krampus",
                    "center": "",
                    "innerRight": "Fullbody_Hero_BigBadWolf",
                    "outerRight": ""
                },
                {
                    "characterID": "크람푸스",
                    "backgroundID": "christmastree",
                    "dialogueText": "조심하는 게 좋을걸? 안 그럼 석탄가루를 뒤집어쓸 거야!",
                    "emotionID": "annoy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Krampus",
                    "center": "",
                    "innerRight": "Fullbody_Hero_BigBadWolf",
                    "outerRight": ""
                },
                {
                    "characterID": "나쁜 늑대",
                    "backgroundID": "christmastree",
                    "dialogueText": "크람푸스야, 조심하는 게 좋을 거라니깐. 나 전에 꿈꾸는 자와 싸웠다가 졌거든~",
                    "emotionID": "shy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
