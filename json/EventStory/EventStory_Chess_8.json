    "m_Name": "EventStory_Chess_8",
    "unskippable": 0,
    "dialogueDataEntries": [
        {
            "languageID": "English",
            "dialogueLines": [
                {
                    "characterID": "",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "As you move on, others in your party start reporting seeing the same kind of phantoms that Hatter did.",
                    "emotionID": "",
                    "musicID": "shoptheme",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "Not only that, but the low thrum of energy that seems to resonate through the walls seems to change in quality.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "The further you travel, the more the thrum begins to take on the sound of battle...",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "But there is no battle to create such sounds.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "You continue on, the tension building the longer you go without being attacked, until-",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Lily",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "Dreamer!",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Lily",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "What is it?",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Lily",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Lily",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "I don't know what it is, but... look at this.",
                    "emotionID": "idle",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Lily",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "You move to where Lily is pointing.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Lily",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "Out of the blank face of crystalline wall, a shape extends.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Lily",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "It looks like... and arm and hand? But it's made out of the same slightly translucent material as the wall.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Lily",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "Between its outstretched fingers is another playing card.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Lily",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "You pluck it from the crystal fingers and look at it.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Lily",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "A Jack of Spades.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Lily",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "What do all these cards mean?",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Lily",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "You barely have time to consider that, however, as almost immediately, the Dreamless attack once more!",
                    "emotionID": "",
                    "musicID": "threateningbattle",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_NPC_DreamlessPawn_Red",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Aurora",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "Dreamer!",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "Right!",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "You move to meet them and your party ready themselves.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "And that's when you see her, fighting down a long tunnel towards you.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_WhteKnight",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "White Knight",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "Down you fiends! Hyaaaa!",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "bounce",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_WhteKnight",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "The newcomer crashes into the rear of the Dreamless assaulting you!",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_WhteKnight",
                    "innerRight": "",
                    "outerRight": ""
                }
            ]
        },
        {
            "languageID": "Korean",
            "dialogueLines": [
                {
                    "characterID": "",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "앞으로 쭉 이어진 길을 따라 모퉁이를 돌자 길 너비가 갑자기 넓어졌다.",
                    "emotionID": "",
                    "musicID": "shoptheme",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "어두운 구역 안으로 들어서자 크리스탈 벽 너머로 일정한 진동이 느껴졌다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "심장 소리처럼 균일하게 쿵쿵거리는 소리에서 거대한 힘이 느껴진다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "강력한 메르헨이나 마법이 맞부딪힐 때 느껴지는 파장과 비슷했다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "매드 해터가 말했던대로 유령이 나타났는데, 그들도 그 힘을 피해 도망치던 중인지 우리를 공격하지 않았다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "화이트 폰 릴리",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "아!",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Lily",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "릴리가 내 손을 잡아당겼다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Lily",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "화이트 폰 릴리",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "꿈꾸는 자, 이거요!",
                    "emotionID": "idle",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Lily",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "릴리의 꽃송이가 크리스탈 벽면의 작은 틈을 가리켰다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Lily",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "그 안에 비스듬히 끼워진 트럼프 카드가 보였다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Lily",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "트럼프 카드를 손처럼 생긴 크리스탈 조각이 움켜쥐고 있었다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Lily",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "어느새 곁으로 다가온 오로라가 트럼프 카드를 꺼내주었다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Lily",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "카드에 그려진 그림은...",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Lily",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "스페이드 잭.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Lily",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "마법의 거울로 이끌어 주는 이정표일까?",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Lily",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "다시 시끄러운 굉음이 울렸고, 오로라가 내 앞을 막아섰다.",
                    "emotionID": "",
                    "musicID": "threateningbattle",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_NPC_DreamlessPawn_Red",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "오로라",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "꿈꾸는 자, 드림리스에요!",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "서둘러 매드 해터와 삼월 토끼 곁으로 달려갔다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "전열을 가다듬자마자 드림리스 무리와 맞부딪혔다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "치열한 접전 속에서 정신없이 드림리스와 싸우는데, 낯선 목소리가 울려 퍼졌다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_WhteKnight",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "하얀 기사",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "승리를 위하여 이 깃발을 높게 들어 올리니, 고결한 아침이여! 부름에 답하라!",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "bounce",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_WhteKnight",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "새하얀 섬광을 두른 인물이 아침 햇살처럼 드림리스를 뚫고 나타났다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_WhteKnight",
                    "innerRight": "",
                    "outerRight": ""
                }
            ]
        }
    ]
}