    "m_Name": "EventStory_SleepHollow_7",
    "unskippable": 0,
    "dialogueDataEntries": [
        {
            "languageID": "English",
            "dialogueLines": [
                {
                    "characterID": "",
                    "backgroundID": "burningwoods",
                    "dialogueText": "The forest has become a whirling maelstrom of combat.",
                    "emotionID": "",
                    "musicID": "threateningbattle",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "burningwoods",
                    "dialogueText": "Riding through it, the Horseman is a dervish of action, his horse black as night, hot steam jetting in streamers from its nostrils and between the teeth of its clenched jaws.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_HeadlessHorseman",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "burningwoods",
                    "dialogueText": "Zombies fight heroes, heroes fight pumpkin-heads, pumpkin-heads fight zombies... and the Horseman seems to fight everyone, alike!",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_HeadlessHorseman",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "burningwoods",
                    "dialogueText": "Such a maelstrom cannot last forever and the numbers of zombies and pumpkin-heads steadily begins to thin out.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "burningwoods",
                    "dialogueText": "This fact isn't lost on a certain puppet-maker.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Geppeto",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Geppetto",
                    "backgroundID": "burningwoods",
                    "dialogueText": "Frankenstein! We're running out of zombies!",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Geppeto",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Dr. Frankenstein",
                    "backgroundID": "burningwoods",
                    "dialogueText": "Don't worry, I can just make more!",
                    "emotionID": "happy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Geppeto",
                    "center": "",
                    "innerRight": "Fullbody_Hero_DoctorFrankenstein",
                    "outerRight": ""
                },
                {
                    "characterID": "Geppetto",
                    "backgroundID": "burningwoods",
                    "dialogueText": "And I can make more puppets, but neither of us can do that in the middle of a fight! We have to get out of here!",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Geppeto",
                    "center": "",
                    "innerRight": "Fullbody_Hero_DoctorFrankenstein",
                    "outerRight": ""
                },
                {
                    "characterID": "Bathory",
                    "backgroundID": "burningwoods",
                    "dialogueText": "Geppetto's right! We're not going to have enough forces to capture the Horseman at this rate!",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Geppeto",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_DoctorFrankenstein",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Bathory"
                },
                {
                    "characterID": "Bathory",
                    "backgroundID": "burningwoods",
                    "dialogueText": "We should withdraw!",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Geppeto",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_DoctorFrankenstein",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Bathory"
                },
                {
                    "characterID": "",
                    "backgroundID": "burningwoods",
                    "dialogueText": "Bathory and Geppetto both turn about and leave Frankenstein, heading back into the woods. However, Frankenstein does not follow.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_DoctorFrankenstein",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Dr. Frankenstein",
                    "backgroundID": "burningwoods",
                    "dialogueText": "Ha! Cowards!",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_DoctorFrankenstein",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Dr. Frankenstein",
                    "backgroundID": "burningwoods",
                    "dialogueText": "I was never after the Horseman, anyway...",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_DoctorFrankenstein",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Dr. Frankenstein",
                    "backgroundID": "burningwoods",
                    "dialogueText": "Grruauaaarr!",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_DoctorFrankenstein",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Dr. Frankenstein",
                    "backgroundID": "burningwoods",
                    "dialogueText": "That's right, Bob, you know who we're after.",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_DoctorFrankenstein",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Dr. Frankenstein",
                    "backgroundID": "burningwoods",
                    "dialogueText": "Let's go...",
                    "emotionID": "happy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_DoctorFrankenstein",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "burningwoods",
                    "dialogueText": "...",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "burningwoods",
                    "dialogueText": "As you continue trying to protect the village and keep your heroes together, Red fights her way to your side.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_RedRidingHood",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "burningwoods",
                    "dialogueText": "She seems to want to tell you something.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_RedRidingHood",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Red",
                    "backgroundID": "burningwoods",
                    "dialogueText": "Dreamer! These zombies... they're behaving like an army!",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_RedRidingHood",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "burningwoods",
                    "dialogueText": "They ARE an army! No, more like a horde... a shambling horde...",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_RedRidingHood",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Red",
                    "backgroundID": "burningwoods",
                    "dialogueText": "No, you don't understand what I mean. They're not just attacking us mindlessly...",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_RedRidingHood",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Red",
                    "backgroundID": "burningwoods",
                    "dialogueText": "It's like they're coordinated!",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_RedRidingHood",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Red",
                    "backgroundID": "burningwoods",
                    "dialogueText": "Someone's controlling them!",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_RedRidingHood",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "burningwoods",
                    "dialogueText": "If that's true, you need to find and defeat the one that's controlling them.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_RedRidingHood",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Red",
                    "backgroundID": "burningwoods",
                    "dialogueText": "My thoughts exactly!",
                    "emotionID": "idle",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_RedRidingHood",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Red",
                    "backgroundID": "burningwoods",
                    "dialogueText": "I'll spread the word. Find the general and we win this fight!",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_RedRidingHood",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "burningwoods",
                    "dialogueText": "And no sooner has she said that than Red is gone from your side. You can see her progress away from you in the swings of her giant sword over the heads of the horde on all sides.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "burningwoods",
                    "dialogueText": "Instead, you see another figure: the strange woman wearing a lab coat.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_DoctorFrankenstein",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "burningwoods",
                    "dialogueText": "She is making her way towards you with a crazy look in her eye and an absolutely huge zombie at her side!",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_DoctorFrankenstein",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "burningwoods",
                    "dialogueText": "Could this be the one who is controlling the zombies? Who is she?",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_DoctorFrankenstein",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Dr. Frankenstein",
                    "backgroundID": "burningwoods",
                    "dialogueText": "Who am I?",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_DoctorFrankenstein",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Dr. Frankenstein",
                    "backgroundID": "burningwoods",
                    "dialogueText": "Frankenstein! DOCTOR Frankenstein!",
                    "emotionID": "happy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "bounce",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_DoctorFrankenstein",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Dr. Frankenstein",
                    "backgroundID": "burningwoods",
                    "dialogueText": "I'll have your head, Dreamer! And the rest of you! Even the Headless Horseman won't deny me!",
                    "emotionID": "happy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_DoctorFrankenstein",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Dr. Frankenstein",
                    "backgroundID": "burningwoods",
                    "dialogueText": "GO, BOB! BRING ME MY PRIZE!",
                    "emotionID": "happy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_DoctorFrankenstein",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "burningwoods",
                    "dialogueText": "...",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "burningwoods",
                    "dialogueText": "<i>Bob</i>...?",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                }
            ]
        },
        {
            "languageID": "Korean",
            "dialogueLines": [
                {
                    "characterID": "",
                    "backgroundID": "burningwoods",
                    "dialogueText": "숲의 평화로운 풍경은 뜨거운 불길 아래 사라져버렸다.",
                    "emotionID": "",
                    "musicID": "threateningbattle",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "burningwoods",
                    "dialogueText": "일대는 이미 전장이 되었다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_HeadlessHorseman",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "burningwoods",
                    "dialogueText": "목 없는 기사는 말에 올라 전장을 누볐다. 아군과 적군이 누구인지 분간할 수 없었다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_HeadlessHorseman",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "burningwoods",
                    "dialogueText": "좀비는 메르헨을 공격하고, 메르헨은 모든 괴물과 싸웠다. 괴물은 좀비와 메르헨을 동시에 공격했고...",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "burningwoods",
                    "dialogueText": "목 없는 기사는 자신을 가로막는 모든 것을 공격했다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Geppeto",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "제페토",
                    "backgroundID": "burningwoods",
                    "dialogueText": "프랑켄슈타인, 좀비의 숫자가 줄어드는 게 너무 빨라.",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Geppeto",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "프랑켄슈타인",
                    "backgroundID": "burningwoods",
                    "dialogueText": "상관없어! 더 만들면 돼!",
                    "emotionID": "happy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Geppeto",
                    "center": "",
                    "innerRight": "Fullbody_Hero_DoctorFrankenstein",
                    "outerRight": ""
                },
                {
                    "characterID": "제페토",
                    "backgroundID": "burningwoods",
                    "dialogueText": "불필요한 소모전이 될 뿐이야. 더 늦기 전에 빠져나가자.",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Geppeto",
                    "center": "",
                    "innerRight": "Fullbody_Hero_DoctorFrankenstein",
                    "outerRight": ""
                },
                {
                    "characterID": "엘리자베스 바토리",
                    "backgroundID": "burningwoods",
                    "dialogueText": "그의 말대로 해요, 이대로라면 목 없는 기사는커녕 당신도 무사하지 못할 거예요.",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Geppeto",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_DoctorFrankenstein",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Bathory"
                },
                {
                    "characterID": "엘리자베스 바토리",
                    "backgroundID": "burningwoods",
                    "dialogueText": "여기선 물러서죠.",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Geppeto",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_DoctorFrankenstein",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Bathory"
                },
                {
                    "characterID": "",
                    "backgroundID": "burningwoods",
                    "dialogueText": "바토리가 먼저 숲의 어둠 속으로 몸을 숨겼다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_DoctorFrankenstein",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "프랑켄슈타인",
                    "backgroundID": "burningwoods",
                    "dialogueText": "하! 겁쟁이들! 실험체들이 여기 있는데, 가긴 어딜 간다는 거야?",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_DoctorFrankenstein",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "프랑켄슈타인",
                    "backgroundID": "burningwoods",
                    "dialogueText": "목 없는 기사는 이제 아무래도 상관없어!",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_DoctorFrankenstein",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "프랑켄슈타인",
                    "backgroundID": "burningwoods",
                    "dialogueText": "그르르르르르!",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_DoctorFrankenstein",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "프랑켄슈타인",
                    "backgroundID": "burningwoods",
                    "dialogueText": "그래, 네 말이 맞아. 역시 고지능 밥, 내 목적이 뭔지 잘 알고 있구나.",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_DoctorFrankenstein",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "프랑켄슈타인",
                    "backgroundID": "burningwoods",
                    "dialogueText": "가자, 밥!",
                    "emotionID": "happy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
