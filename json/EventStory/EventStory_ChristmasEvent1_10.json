  "dialogueDataEntries": [
    {
      "languageID": "English",
      "dialogueLines": [
        {
          "characterID": "",
          "backgroundID": "christmastree",
          "dialogueText": "Red and Goldilocks get the better of the Big Bad Wolf and knock her to the ground.",
          "emotionID": "",
          "musicID": "christmasbattle",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "LITTLERED",
          "innerLeft": "",
          "center": "BIG BAD WOLF",
          "innerRight": "",
          "outerRight": "GOLDILOCKS"
        },
        {
          "characterID": "",
          "backgroundID": "christmastree",
          "dialogueText": "The wolf girl lies panting and yowling in pain.",
          "emotionID": "",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "LITTLERED",
          "innerLeft": "",
          "center": "BIG BAD WOLF",
          "innerRight": "",
          "outerRight": "GOLDILOCKS"
        },
        {
          "characterID": "BIG BAD WOLF",
          "backgroundID": "christmastree",
          "dialogueText": "Awwwoooo owwwowwwoooo!",
          "emotionID": "shy",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "LITTLERED",
          "innerLeft": "",
          "center": "BIG BAD WOLF",
          "innerRight": "",
          "outerRight": "GOLDILOCKS"
        },
        {
          "characterID": "BIG BAD WOLF",
          "backgroundID": "christmastree",
          "dialogueText": "That hurts, you meanies!",
          "emotionID": "shy",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "LITTLERED",
          "innerLeft": "",
          "center": "BIG BAD WOLF",
          "innerRight": "",
          "outerRight": "GOLDILOCKS"
        },
        {
          "characterID": "",
          "backgroundID": "christmastree",
          "dialogueText": "It's about to hurt a lot more, wolf!",
          "emotionID": "",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "LITTLERED",
          "innerLeft": "",
          "center": "BIG BAD WOLF",
          "innerRight": "",
          "outerRight": "GOLDILOCKS"
        },
        {
          "characterID": "BIG BAD WOLF",
          "backgroundID": "christmastree",
          "dialogueText": "Not if I-",
          "emotionID": "surprise",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "",
          "center": "BIG BAD WOLF",
          "innerRight": "",
          "outerRight": ""
        },
        {
          "characterID": "BIG BAD WOLF",
          "backgroundID": "christmastree",
          "dialogueText": "RUN AWAY!",
          "emotionID": "angry",
          "musicID": "",
          "sfxID": "",
          "animationID": "bounce",
          "outerLeft": "",
          "innerLeft": "",
          "center": "BIG BAD WOLF",
          "innerRight": "",
          "outerRight": ""
        },
        {
          "characterID": "",
          "backgroundID": "christmastree",
          "dialogueText": "And in a moment, the wolf girl has taken her feet and is sprinting away through the masses of Dreamless.",
          "emotionID": "",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "",
          "center": "",
          "innerRight": "",
          "outerRight": ""
        },
        {
          "characterID": "",
          "backgroundID": "christmastree",
          "dialogueText": "Try as they might, Red and Goldilocks can't pursue.",
          "emotionID": "",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "LITTLERED",
          "center": "",
          "innerRight": "GOLDILOCKS",
          "outerRight": ""
        },
        {
          "characterID": "LITTLERED",
          "backgroundID": "christmastree",
          "dialogueText": "Damn it! She got away again!",
          "emotionID": "angry",
          "musicID": "",
          "sfxID": "",
          "animationID": "shake",
          "outerLeft": "",
          "innerLeft": "LITTLERED",
          "center": "",
          "innerRight": "GOLDILOCKS",
          "outerRight": ""
        },
        {
          "characterID": "GOLDILOCKS",
          "backgroundID": "christmastree",
          "dialogueText": "You'll get her next time, Red.",
          "emotionID": "idle",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "LITTLERED",
          "center": "",
          "innerRight": "GOLDILOCKS",
          "outerRight": ""
        },
        {
          "characterID": "GINGER",
          "backgroundID": "christmastree",
          "dialogueText": "Or I will!",
          "emotionID": "annoy",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "LITTLERED",
          "innerLeft": "",
          "center": "GINGER",
          "innerRight": "",
          "outerRight": "GOLDILOCKS"
        },
        {
          "characterID": "GINGER",
          "backgroundID": "christmastree",
          "dialogueText": "Seriously! What is it with that girl and eating people's loved ones!",
          "emotionID": "angry",
          "musicID": "",
          "sfxID": "",
          "animationID": "shake",
          "outerLeft": "LITTLERED",
          "innerLeft": "",
          "center": "GINGER",
          "innerRight": "",
          "outerRight": "GOLDILOCKS"
        },
        {
          "characterID": "GOLDILOCKS",
          "backgroundID": "christmastree",
          "dialogueText": "Well... she's a wolf, I guess?",
          "emotionID": "sad",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "LITTLERED",
          "innerLeft": "",
          "center": "GINGER",
          "innerRight": "",
          "outerRight": "GOLDILOCKS"
        },
        {
          "characterID": "GOLDILOCKS",
          "backgroundID": "christmastree",
          "dialogueText": "I wonder how she got like that?",
          "emotionID": "idle",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "LITTLERED",
          "innerLeft": "",
          "center": "GINGER",
          "innerRight": "",
          "outerRight": "GOLDILOCKS"
        },
        {
          "characterID": "",
          "backgroundID": "christmastree",
          "dialogueText": "A question for another time, you think. There are still matters at hand to deal with.",
          "emotionID": "",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "LITTLERED",
          "innerLeft": "",
          "center": "GINGER",
          "innerRight": "",
          "outerRight": "GOLDILOCKS"
        },
        {
          "characterID": "GOLDILOCKS",
          "backgroundID": "christmastree",
          "dialogueText": "Ah! Of course, Dreamer...",
          "emotionID": "surprise",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "LITTLERED",
          "innerLeft": "",
          "center": "GINGER",
          "innerRight": "",
          "outerRight": "GOLDILOCKS"
        },
        {
          "characterID": "",
          "backgroundID": "christmastree",
          "dialogueText": "It seems the wolf girl's flight hasn't gone unnoticed.",
          "emotionID": "",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "",
          "center": "KRAMPUS",
          "innerRight": "",
          "outerRight": ""
        },
        {
          "characterID": "KRAMPUS",
          "backgroundID": "christmastree",
          "dialogueText": "Wolfie! Get back here you big coward!",
          "emotionID": "angry",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "",
          "center": "KRAMPUS",
          "innerRight": "",
          "outerRight": ""
        },
        {
          "characterID": "",
          "backgroundID": "christmastree",
          "dialogueText": "You see Marie signalling to you through the crowd.",
          "emotionID": "",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "",
          "center": "MARIE",
          "innerRight": "",
          "outerRight": ""
        },
        {
          "characterID": "MARIE",
          "backgroundID": "christmastree",
          "dialogueText": "Dreamer! Krampus is over there!",
          "emotionID": "surprise",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "",
          "center": "MARIE",
          "innerRight": "",
          "outerRight": ""
        },
        {
          "characterID": "",
          "backgroundID": "christmastree",
          "dialogueText": "You look around and see Krampus is exposed, with only a few Dreamless around her. Nutcracker has seen her, as well.",
          "emotionID": "",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "",
          "center": "KRAMPUS",
          "innerRight": "",
          "outerRight": ""
        },
        {
          "characterID": "KRAMPUS",
          "backgroundID": "christmastree",
          "dialogueText": "What are you all looking at?! Don't even think about it!",
          "emotionID": "conspire",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "",
          "center": "KRAMPUS",
          "innerRight": "",
          "outerRight": ""
        },
        {
          "characterID": "",
          "backgroundID": "christmastree",
          "dialogueText": "With the light of vengeance in your eyes, Marie, the Nutcracker and yourself converge and drive at the Krampus, attempting to get in range of the troublesome girl.",
          "emotionID": "",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "",
          "center": "KRAMPUS",
          "innerRight": "",
          "outerRight": ""
        },
        {
          "characterID": "KRAMPUS",
          "backgroundID": "christmastree",
          "dialogueText": "Ahhh! Help me!",
          "emotionID": "surprise",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "",
          "center": "KRAMPUS",
          "innerRight": "",
          "outerRight": ""
        },
        {
          "characterID": "KRAMPUS",
          "backgroundID": "christmastree",
          "dialogueText": "Rat King! Protect me!",
          "emotionID": "angry",
          "musicID": "",
          "sfxID": "",
          "animationID": "bounce",
          "outerLeft": "",
          "innerLeft": "",
          "center": "KRAMPUS",
          "innerRight": "",
          "outerRight": ""
        },
        {
          "characterID": "RAT KING",
          "backgroundID": "christmastree",
          "dialogueText": "Sssss, such a troublesome ally!",
          "emotionID": "annoy",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "KRAMPUS",
          "center": "",
          "innerRight": "RAT KING",
          "outerRight": ""
        }
      ]
    },
    {
      "languageID": "Korean",
      "dialogueLines": [
        {
          "characterID": "",
          "backgroundID": "christmastree",
          "dialogueText": "빨간 망토가 늑대의 공격을 막았다.",
          "emotionID": "",
          "musicID": "christmasbattle",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "LITTLERED",
          "innerLeft": "",
          "center": "BIG BAD WOLF",
          "innerRight": "",
          "outerRight": "GOLDILOCKS"
        },
        {
          "characterID": "",
          "backgroundID": "christmastree",
          "dialogueText": "튕겨 나간 나쁜 늑대가 몸부림치며 비명을 질렀다.",
          "emotionID": "",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "LITTLERED",
          "innerLeft": "",
          "center": "BIG BAD WOLF",
          "innerRight": "",
          "outerRight": "GOLDILOCKS"
        },
        {
          "characterID": "BIG BAD WOLF",
          "backgroundID": "christmastree",
          "dialogueText": "아우우우!",
          "emotionID": "shy",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "LITTLERED",
          "innerLeft": "",
          "center": "BIG BAD WOLF",
          "innerRight": "",
          "outerRight": "GOLDILOCKS"
        },
        {
          "characterID": "BIG BAD WOLF",
          "backgroundID": "christmastree",
          "dialogueText": "이게 무슨 짓이야, 아프잖아! 배고파... 배고프다고!",
          "emotionID": "shy",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "LITTLERED",
          "innerLeft": "",
          "center": "BIG BAD WOLF",
          "innerRight": "",
          "outerRight": "GOLDILOCKS"
        },
        {
          "characterID": "",
          "backgroundID": "christmastree",
          "dialogueText": "골디락스는 그 틈을 놓치지 않고 공격했다.",
          "emotionID": "",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "LITTLERED",
          "innerLeft": "",
          "center": "BIG BAD WOLF",
          "innerRight": "",
          "outerRight": "GOLDILOCKS"
        },
        {
          "characterID": "BIG BAD WOLF",
          "backgroundID": "christmastree",
          "dialogueText": "너무 배고파... 주변도 너무 밝고...",
          "emotionID": "surprise",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "",
          "center": "BIG BAD WOLF",
          "innerRight": "",
          "outerRight": ""
        },
        {
          "characterID": "BIG BAD WOLF",
          "backgroundID": "christmastree",
          "dialogueText": "오호호, 하는 수 없지. 다음에 보자니깐, 꼬마들아. 그땐 확실하게 너희를... 잡아먹을 거야!",
          "emotionID": "angry",
          "musicID": "",
          "sfxID": "",
          "animationID": "bounce",
          "outerLeft": "",
          "innerLeft": "",
          "center": "BIG BAD WOLF",
          "innerRight": "",
          "outerRight": ""
        },
        {
          "characterID": "",
          "backgroundID": "christmastree",
          "dialogueText": "빨간 망토와 골디락스가 잠시 공격을 멈춘 사이 나쁜 늑대는 드림리스 쥐 병사들 사이로 뛰어들었다.",
          "emotionID": "",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "",
          "center": "",
          "innerRight": "",
          "outerRight": ""
        },
        {
          "characterID": "",
          "backgroundID": "christmastree",
          "dialogueText": "쥐 병사들이 두 사람을 포위했다. 두 사람은 나쁜 늑대를 놓치고 말았다.",
          "emotionID": "",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "LITTLERED",
          "center": "",
          "innerRight": "GOLDILOCKS",
          "outerRight": ""
        },
        {
          "characterID": "LITTLERED",
          "backgroundID": "christmastree",
          "dialogueText": "쳇, 또 놓쳤잖아!",
          "emotionID": "angry",
          "musicID": "",
          "sfxID": "",
          "animationID": "shake",
          "outerLeft": "",
          "innerLeft": "LITTLERED",
          "center": "",
          "innerRight": "GOLDILOCKS",
          "outerRight": ""
        },
        {
          "characterID": "GOLDILOCKS",
          "backgroundID": "christmastree",
          "dialogueText": "다음 기회를 노리는 게 좋겠어요, 지금은 꿈꾸는 자를 보조해야죠.",
          "emotionID": "idle",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "LITTLERED",
          "center": "",
          "innerRight": "GOLDILOCKS",
          "outerRight": ""
        },
        {
          "characterID": "GINGER",
          "backgroundID": "christmastree",
          "dialogueText": "그 늑대를 놓친 거야?",
          "emotionID": "annoy",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "LITTLERED",
          "innerLeft": "",
          "center": "GINGER",
          "innerRight": "",
          "outerRight": "GOLDILOCKS"
        },
        {
          "characterID": "GINGER",
          "backgroundID": "christmastree",
          "dialogueText": "다음엔 나도 도와줄게! 그 늑대 대체 뭐 하는 녀석이야?",
          "emotionID": "angry",
          "musicID": "",
          "sfxID": "",
          "animationID": "shake",
          "outerLeft": "LITTLERED",
          "innerLeft": "",
          "center": "GINGER",
          "innerRight": "",
          "outerRight": "GOLDILOCKS"
        },
        {
          "characterID": "GOLDILOCKS",
          "backgroundID": "christmastree",
          "dialogueText": "뭐든 잡아먹는 늑대라니...",
          "emotionID": "sad",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "LITTLERED",
          "innerLeft": "",
          "center": "GINGER",
          "innerRight": "",
          "outerRight": "GOLDILOCKS"
        },
        {
          "characterID": "GOLDILOCKS",
          "backgroundID": "christmastree",
          "dialogueText": "어쩌다 그런 모습이 되어버린 걸까요?",
          "emotionID": "idle",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "LITTLERED",
          "innerLeft": "",
          "center": "GINGER",
          "innerRight": "",
          "outerRight": "GOLDILOCKS"
        },
        {
          "characterID": "",
          "backgroundID": "christmastree",
          "dialogueText": "골디락스가 무기를 점검했다.",
          "emotionID": "",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "LITTLERED",
          "innerLeft": "",
          "center": "GINGER",
          "innerRight": "",
          "outerRight": "GOLDILOCKS"
        },
        {
          "characterID": "GOLDILOCKS",
          "backgroundID": "christmastree",
          "dialogueText": "늑대가 도망친 걸 알아차린 것 같아요, 꿈꾸는 자.",
          "emotionID": "surprise",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "LITTLERED",
          "innerLeft": "",
          "center": "GINGER",
          "innerRight": "",
          "outerRight": "GOLDILOCKS"
        },
        {
          "characterID": "",
          "backgroundID": "christmastree",
          "dialogueText": "과연 골디락스의 말대로 크람푸스가 허둥거리며 누군가를 찾고 있었다.",
          "emotionID": "",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "",
          "center": "KRAMPUS",
          "innerRight": "",
          "outerRight": ""
        },
        {
          "characterID": "KRAMPUS",
          "backgroundID": "christmastree",
          "dialogueText": "늑대야, 어디 간 거야? 나만 놔두고 가버린 건 아니지!? 빨리 돌아와!",
          "emotionID": "angry",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "",
          "center": "KRAMPUS",
          "innerRight": "",
          "outerRight": ""
        },
        {
          "characterID": "",
          "backgroundID": "christmastree",
          "dialogueText": "드림리스를 처리한 마리가 높은 소리로 외쳤다.",
          "emotionID": "",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "",
          "center": "MARIE",
          "innerRight": "",
          "outerRight": ""
        },
        {
          "characterID": "MARIE",
          "backgroundID": "christmastree",
          "dialogueText": "꿈꾸는 자, 크람푸스를 부탁드릴게요!",
          "emotionID": "surprise",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "",
          "center": "MARIE",
          "innerRight": "",
          "outerRight": ""
        },
        {
          "characterID": "",
          "backgroundID": "christmastree",
          "dialogueText": "드림리스 쥐 병사들 뒤로 뛰어가 몸을 웅크린 크람푸스가 다급한 목소리로 외쳤다.",
          "emotionID": "",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "",
          "center": "KRAMPUS",
          "innerRight": "",
          "outerRight": ""
        },
        {
          "characterID": "KRAMPUS",
          "backgroundID": "christmastree",
          "dialogueText": "뭐, 뭘 봐?! 왜 그렇게 봐! 나, 날 어떻게 할 셈이야!?",
          "emotionID": "conspire",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "",
          "center": "KRAMPUS",
          "innerRight": "",
          "outerRight": ""
        },
        {
          "characterID": "",
          "backgroundID": "christmastree",
          "dialogueText": "마리와 호두까기 인형이 크람푸스를 향해 다가갔다.",
          "emotionID": "",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "",
          "center": "KRAMPUS",
          "innerRight": "",
          "outerRight": ""
        },
        {
