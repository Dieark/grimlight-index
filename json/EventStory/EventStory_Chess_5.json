    "m_Name": "EventStory_Chess_5",
    "unskippable": 0,
    "dialogueDataEntries": [
        {
            "languageID": "English",
            "dialogueLines": [
                {
                    "characterID": "",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "The Dreamless finally seem to be cleared out of this section of labyrinth and, as the monstrosity falls, the rift magically seals itself, disappearing from reality.",
                    "emotionID": "",
                    "musicID": "none",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "But who conjured it in the first place? Or was it a trap left for anyone trying to find a way in, or out?",
                    "emotionID": "",
                    "musicID": "none",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "Red Knight cleans her sword of black Dreamless ichor and strides to your side.",
                    "emotionID": "",
                    "musicID": "none",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_RedKnight",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Red Knight",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "You fought well, Dreamer!",
                    "emotionID": "happy",
                    "musicID": "intothewild",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_RedKnight",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "As did Red Knight! But where are the others?",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_RedKnight",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Red Knight",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "I don't know. In the confusion of that ambush, we got separ-",
                    "emotionID": "annoy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_RedKnight",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Lily",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "AAAH! I know you!",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Lily",
                    "center": "",
                    "innerRight": "Fullbody_Hero_RedKnight",
                    "outerRight": ""
                },
                {
                    "characterID": "Lily",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "TRAITOR!",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "bounce",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Lily",
                    "center": "",
                    "innerRight": "Fullbody_Hero_RedKnight",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "What's this, now?",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Lily",
                    "center": "",
                    "innerRight": "Fullbody_Hero_RedKnight",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "Lily is pointing a shaking finger accusingly at Red Knight.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Lily",
                    "center": "",
                    "innerRight": "Fullbody_Hero_RedKnight",
                    "outerRight": ""
                },
                {
                    "characterID": "Lily",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "She's as monstrous as the Dreamless! She's <i>Red Knight!</i>",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Lily",
                    "center": "",
                    "innerRight": "Fullbody_Hero_RedKnight",
                    "outerRight": ""
                },
                {
                    "characterID": "Red Knight",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "Aye, little pipsqueak, that's who I am.",
                    "emotionID": "annoy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Lily",
                    "center": "",
                    "innerRight": "Fullbody_Hero_RedKnight",
                    "outerRight": ""
                },
                {
                    "characterID": "Lily",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "P-Pipsqueak!",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "shake",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Lily",
                    "center": "",
                    "innerRight": "Fullbody_Hero_RedKnight",
                    "outerRight": ""
                },
                {
                    "characterID": "Lily",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "You betrayed your own master, threw your hat in with the mad queen! Dreamer, she'll kill us all!",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Lily",
                    "center": "",
                    "innerRight": "Fullbody_Hero_RedKnight",
                    "outerRight": ""
                },
                {
                    "characterID": "Red Knight",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "Heh! Little Lily White... You <i>have</i> been gone a long time...",
                    "emotionID": "happy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Lily",
                    "center": "",
                    "innerRight": "Fullbody_Hero_RedKnight",
                    "outerRight": ""
                },
                {
                    "characterID": "Red Knight",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "You have nothing to fear from me, little one. I no longer have any ties to the Queen of Hearts.",
                    "emotionID": "idle",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Lily",
                    "center": "",
                    "innerRight": "Fullbody_Hero_RedKnight",
                    "outerRight": ""
                },
                {
                    "characterID": "Lily",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "You betrayed another master?! Dreamer, you can't trust her!",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Lily",
                    "center": "",
                    "innerRight": "Fullbody_Hero_RedKnight",
                    "outerRight": ""
                },
                {
                    "characterID": "Red Knight",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "No! The mad queen betrayed <i>me!</i>",
                    "emotionID": "shy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Lily",
                    "center": "",
                    "innerRight": "Fullbody_Hero_RedKnight",
                    "outerRight": ""
                },
                {
                    "characterID": "Red Knight",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "She and that red knave drove me out of her court for disagreeing with her!",
                    "emotionID": "sad",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Lily",
                    "center": "",
                    "innerRight": "Fullbody_Hero_RedKnight",
                    "outerRight": ""
                },
                {
                    "characterID": "Red Knight",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "I was lucky to escape with my head!",
                    "emotionID": "sad",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Lily",
                    "center": "",
                    "innerRight": "Fullbody_Hero_RedKnight",
                    "outerRight": ""
                },
                {
                    "characterID": "Red Knight",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "After years of service! After everything I sacrificed to serve her!",
                    "emotionID": "sad",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Lily",
                    "center": "",
                    "innerRight": "Fullbody_Hero_RedKnight",
                    "outerRight": ""
                },
                {
                    "characterID": "Lily",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "After betraying your old master and trapping my family in this terrible maze!",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Lily",
                    "center": "",
                    "innerRight": "Fullbody_Hero_RedKnight",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "Wait... Red Knight's responsible for this maze?",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Lily",
                    "center": "",
                    "innerRight": "Fullbody_Hero_RedKnight",
                    "outerRight": ""
                },
                {
                    "characterID": "Red Knight",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "No... but I allowed it to happen.",
                    "emotionID": "sad",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Lily",
                    "center": "",
                    "innerRight": "Fullbody_Hero_RedKnight",
                    "outerRight": ""
                },
                {
                    "characterID": "Esme",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "Why don't I know about this?",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Lily",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_RedKnight",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "Red Knight",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "Because it was done in secret.",
                    "emotionID": "sad",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Lily",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_RedKnight",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "Red Knight",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "Before the Queen of Hearts lost what remained of her mind... she sealed the other royal kingdoms in the realm away using mirror magic, towns and subjects included.",
                    "emotionID": "sad",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Lily",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_RedKnight",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "Oh! And that's why there are ghost buildings drifting across Wonderland!",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Lily",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_RedKnight",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "Red Knight",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "Yes... the power of the spell must be weakening now the Queen is gone. Things are slowly returning.",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Lily",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_RedKnight",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "Lily",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "But not fast enough! My family is still trapped! My mother!",
                    "emotionID": "sad",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Lily",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_RedKnight",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "Red Knight",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "True, and for that I'm thankful.",
                    "emotionID": "sad",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Lily",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_RedKnight",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "Red Knight",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "She may have been mad, but the Queen of Hearts was right to do it.",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Lily",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_RedKnight",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "Why is that? What could justify trapping them all in here?",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Lily",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_RedKnight",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "Red Knight",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "You'll find out Dreamer. At the end of this maze. For me... my shame prevents me from saying any more.",
                    "emotionID": "sad",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Lily",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_RedKnight",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "Red Knight",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "For what it's worth, I still believe she did the right thing. Even after everything she did since.",
                    "emotionID": "sad",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Lily",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_RedKnight",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "Lily",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "No! That's not true!",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Lily",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_RedKnight",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "Lily",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "Dreamer, please, you have to find a way to release my family!",
                    "emotionID": "sad",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Lily",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_RedKnight",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "Red Knight",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "If you decide to do that, I will follow you, of course, Dreamer. But I warn you...",
                    "emotionID": "sad",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Lily",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_RedKnight",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "Red Knight",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "You may not like the situation you find yourself in should you choose to grant Lily's request...",
                    "emotionID": "sad",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Lily",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_RedKnight",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "Red Knight",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "Whatever happens, Dreamer, my sword will always be yours... because I know that I can trust you to do the right thing.",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Lily",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_RedKnight",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                }
            ]
        },
        {
            "languageID": "Korean",
            "dialogueLines": [
                {
                    "characterID": "",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "마지막 드림리스를 쓰러트리자 균열이 완전히 닫혀 사라졌다.",
                    "emotionID": "",
                    "musicID": "none",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "균열이 있던 벽면을 만져봐도 아무것도 느껴지지 않는다.",
                    "emotionID": "",
                    "musicID": "none",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "검을 크게 휘둘러 묻은 것을 털어낸 붉은 기사가 다가왔다.",
                    "emotionID": "",
                    "musicID": "none",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_RedKnight",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "붉은 기사",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "즐거운 여흥이었다, 꿈꾸는 자!",
                    "emotionID": "happy",
                    "musicID": "intothewild",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_RedKnight",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "호탕하게 웃던 그녀가 돌연 인상을 찡그렸다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_RedKnight",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "붉은 기사",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "여기 있는 게 전부인가? 나머지는 모두 흩어진 모양이군.",
                    "emotionID": "annoy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_RedKnight",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "화이트 폰 릴리",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "이 배신자!",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Lily",
                    "center": "",
                    "innerRight": "Fullbody_Hero_RedKnight",
                    "outerRight": ""
                },
                {
                    "characterID": "화이트 폰 릴리",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "당신이 왜 여기 있는 거죠?!",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "bounce",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Lily",
                    "center": "",
                    "innerRight": "Fullbody_Hero_RedKnight",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "하얗게 질린 얼굴로 릴리가 소리쳤다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Lily",
                    "center": "",
                    "innerRight": "Fullbody_Hero_RedKnight",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "붉은 기사는 흥미를 잃어버린 듯 손가락으로 귀를 만지작거렸다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Lily",
                    "center": "",
                    "innerRight": "Fullbody_Hero_RedKnight",
                    "outerRight": ""
                },
                {
                    "characterID": "화이트 폰 릴리",
                    "backgroundID": "shatteredchessrealm",
