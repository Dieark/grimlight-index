  "m_Name": "EventStory_ChristmasEvent1_9",
  "unskippable": 0,
  "dialogueDataEntries": [
    {
      "languageID": "English",
      "dialogueLines": [
        {
          "characterID": "",
          "backgroundID": "christmastree",
          "dialogueText": "As the mayhem of battle ebbs and flows, the tides of fighting bring Big Bad Wolf and Ginger face to face for the first time.",
          "emotionID": "",
          "musicID": "christmasbattle",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "",
          "center": "",
          "innerRight": "",
          "outerRight": ""
        },
        {
          "characterID": "BIG BAD WOLF",
          "backgroundID": "christmastree",
          "dialogueText": "Ahahaha! You must be Ginger! I've heard your gingerbread men are quite delicious!",
          "emotionID": "happy",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "",
          "center": "BIG BAD WOLF",
          "innerRight": "",
          "outerRight": ""
        },
        {
          "characterID": "",
          "backgroundID": "christmastree",
          "dialogueText": "Ginger looks quite horrified at the idea of her animated gingerbread men getting eaten.",
          "emotionID": "",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "",
          "center": "GINGER",
          "innerRight": "",
          "outerRight": ""
        },
        {
          "characterID": "GINGER",
          "backgroundID": "christmastree",
          "dialogueText": "Don't you dare, you smelly old wolf!",
          "emotionID": "angry",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "",
          "center": "GINGER",
          "innerRight": "",
          "outerRight": ""
        },
        {
          "characterID": "",
          "backgroundID": "christmastree",
          "dialogueText": "But even as she watches, the wolf girl snatches up one of her gingerbread men and scoffs it down!",
          "emotionID": "",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "",
          "center": "GINGER",
          "innerRight": "",
          "outerRight": ""
        },
        {
          "characterID": "BIG BAD WOLF",
          "backgroundID": "christmastree",
          "dialogueText": "<i>Arff, scarf, scoff...</i>",
          "emotionID": "angry",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "",
          "center": "BIG BAD WOLF",
          "innerRight": "",
          "outerRight": ""
        },
        {
          "characterID": "BIG BAD WOLF",
          "backgroundID": "christmastree",
          "dialogueText": "Mmmmmmmm!!!",
          "emotionID": "happy",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "",
          "center": "BIG BAD WOLF",
          "innerRight": "",
          "outerRight": ""
        },
        {
          "characterID": "BIG BAD WOLF",
          "backgroundID": "christmastree",
          "dialogueText": "Delicious!!",
          "emotionID": "happy",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "",
          "center": "BIG BAD WOLF",
          "innerRight": "",
          "outerRight": ""
        },
        {
          "characterID": "GINGER",
          "backgroundID": "christmastree",
          "dialogueText": "SHE ATE MY GINGERBREAD!",
          "emotionID": "surprise",
          "musicID": "",
          "sfxID": "",
          "animationID": "shake",
          "outerLeft": "",
          "innerLeft": "",
          "center": "GINGER",
          "innerRight": "",
          "outerRight": ""
        },
        {
          "characterID": "GINGER",
          "backgroundID": "christmastree",
          "dialogueText": "Goldi! She ate my gingerbread man!",
          "emotionID": "sad",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "",
          "center": "GINGER",
          "innerRight": "",
          "outerRight": ""
        },
        {
          "characterID": "GOLDILOCKS",
          "backgroundID": "christmastree",
          "dialogueText": "I saw, Ginger, slightly more pressing things going on!",
          "emotionID": "annoy",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "GOLDILOCKS",
          "center": "",
          "innerRight": "GINGER",
          "outerRight": ""
        },
        {
          "characterID": "",
          "backgroundID": "christmastree",
          "dialogueText": "But suddenly, Red is there.",
          "emotionID": "",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "GOLDILOCKS",
          "innerLeft": "",
          "center": "LITTLERED",
          "innerRight": "",
          "outerRight": "GINGER"
        },
        {
          "characterID": "LITTLERED",
          "backgroundID": "christmastree",
          "dialogueText": "I'll avenge your gingerbread, Ginger!",
          "emotionID": "angry",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "GOLDILOCKS",
          "innerLeft": "",
          "center": "LITTLERED",
          "innerRight": "",
          "outerRight": "GINGER"
        },
        {
          "characterID": "GOLDILOCKS",
          "backgroundID": "christmastree",
          "dialogueText": "Ah! Wait! That's my job!",
          "emotionID": "surprise",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "GOLDILOCKS",
          "innerLeft": "",
          "center": "LITTLERED",
          "innerRight": "",
          "outerRight": "GINGER"
        },
        {
          "characterID": "",
          "backgroundID": "christmastree",
          "dialogueText": "Both Red and Goldilocks advance on Big Bad Wolf, Red swinging her colossal sword and Goldi reloading her shotgun.",
          "emotionID": "",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "GOLDILOCKS",
          "innerLeft": "",
          "center": "LITTLERED",
          "innerRight": "",
          "outerRight": "GINGER"
        },
        {
          "characterID": "BIG BAD WOLF",
          "backgroundID": "christmastree",
          "dialogueText": "Awwwooo-wait wait! We can talk about this!",
          "emotionID": "surprise",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "",
          "center": "BIG BAD WOLF",
          "innerRight": "",
          "outerRight": ""
        },
        {
          "characterID": "LITTLERED",
          "backgroundID": "christmastree",
          "dialogueText": "Today will be the day my Grandmother finally finds peace!",
          "emotionID": "angry",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "GOLDILOCKS",
          "innerLeft": "",
          "center": "LITTLERED",
          "innerRight": "",
          "outerRight": "GINGER"
        },
        {
          "characterID": "BIG BAD WOLF",
          "backgroundID": "christmastree",
          "dialogueText": "AWWWOOOOO!",
          "emotionID": "surprise",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "",
          "center": "BIG BAD WOLF",
          "innerRight": "",
          "outerRight": ""
        }
      ]
    },
    {
      "languageID": "Korean",
      "dialogueLines": [
        {
          "characterID": "",
          "backgroundID": "christmastree",
          "dialogueText": "전투의 흐름이 완전히 우리 쪽으로 넘어왔다. 나쁜 늑대가 진저를 바라보며 음흉하게 웃는다.",
          "emotionID": "",
          "musicID": "christmasbattle",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "",
          "center": "",
          "innerRight": "",
          "outerRight": ""
        },
        {
          "characterID": "BIG BAD WOLF",
          "backgroundID": "christmastree",
          "dialogueText": "오호호! 네가 바로 진저구나? 네가 만든 진저 브래드 맨이 정말 맛있다고 들었다니깐~",
          "emotionID": "happy",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "",
          "center": "BIG BAD WOLF",
          "innerRight": "",
          "outerRight": ""
        },
        {
          "characterID": "",
          "backgroundID": "christmastree",
          "dialogueText": "진저가 인상을 찡그리며 케이크 박스를 뒤로 숨겼다.",
          "emotionID": "",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "",
          "center": "GINGER",
          "innerRight": "",
          "outerRight": ""
        },
        {
          "characterID": "GINGER",
          "backgroundID": "christmastree",
          "dialogueText": "브래드 맨은 먹으라고 만든 게 아니야, 이 못된 늑대야!",
          "emotionID": "angry",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "",
          "center": "GINGER",
          "innerRight": "",
          "outerRight": ""
        },
        {
          "characterID": "",
          "backgroundID": "christmastree",
          "dialogueText": "브래드 맨이 늑대를 공격하기 위해 달려들었다!",
          "emotionID": "",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "",
          "center": "GINGER",
          "innerRight": "",
          "outerRight": ""
        },
        {
          "characterID": "BIG BAD WOLF",
          "backgroundID": "christmastree",
          "dialogueText": "오호호! 진저 브래드 맨아, 잡아 먹히고 싶어서 온 거니?",
          "emotionID": "angry",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "",
          "center": "BIG BAD WOLF",
          "innerRight": "",
          "outerRight": ""
        },
        {
          "characterID": "BIG BAD WOLF",
          "backgroundID": "christmastree",
          "dialogueText": "그렇다면 먹지 않을 수 없지.",
          "emotionID": "happy",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "",
          "center": "BIG BAD WOLF",
          "innerRight": "",
          "outerRight": ""
        },
        {
          "characterID": "BIG BAD WOLF",
          "backgroundID": "christmastree",
          "dialogueText": "음, 냠냠... 너 정말 맛있구나, 아~! 배고파졌어!",
          "emotionID": "happy",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "",
          "center": "BIG BAD WOLF",
          "innerRight": "",
          "outerRight": ""
        },
        {
          "characterID": "GINGER",
          "backgroundID": "christmastree",
          "dialogueText": "내...",
          "emotionID": "surprise",
          "musicID": "",
          "sfxID": "",
          "animationID": "shake",
          "outerLeft": "",
          "innerLeft": "",
          "center": "GINGER",
          "innerRight": "",
          "outerRight": ""
        },
        {
          "characterID": "GINGER",
          "backgroundID": "christmastree",
          "dialogueText": "내 진저 브래드 맨을 먹었어. 골디! 늑대가 내 진저 브래드 맨을!",
          "emotionID": "sad",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "",
          "center": "GINGER",
          "innerRight": "",
          "outerRight": ""
        },
        {
          "characterID": "GOLDILOCKS",
          "backgroundID": "christmastree",
          "dialogueText": "쿠키잖아요, 진저. 진저 브래드 맨 쿠키!",
          "emotionID": "annoy",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "GOLDILOCKS",
          "center": "",
          "innerRight": "GINGER",
          "outerRight": ""
        },
        {
          "characterID": "",
          "backgroundID": "christmastree",
          "dialogueText": "드림리스 쥐 병사들을 쓰러트린 빨간 망토가 거대한 검을 휘둘렀다.",
          "emotionID": "",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "GOLDILOCKS",
          "innerLeft": "",
          "center": "LITTLERED",
          "innerRight": "",
          "outerRight": "GINGER"
        },
        {
          "characterID": "LITTLERED",
          "backgroundID": "christmastree",
          "dialogueText": "브래드 맨의 복수는 나한테 맡겨, 진저.",
          "emotionID": "angry",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "GOLDILOCKS",
          "innerLeft": "",
          "center": "LITTLERED",
          "innerRight": "",
          "outerRight": "GINGER"
        },
        {
          "characterID": "GOLDILOCKS",
          "backgroundID": "christmastree",
          "dialogueText": "빨간 망토 씨! 그건 제 역할이에요!",
          "emotionID": "surprise",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "GOLDILOCKS",
          "innerLeft": "",
          "center": "LITTLERED",
          "innerRight": "",
          "outerRight": "GINGER"
        },
        {
          "characterID": "",
          "backgroundID": "christmastree",
          "dialogueText": "빨간 망토의 뒤를 쫓아 골디락스가 늑대에게 다가선다. 샷건을 장전하는 소리가 스산하게 울렸다.",
          "emotionID": "",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "GOLDILOCKS",
          "innerLeft": "",
          "center": "LITTLERED",
          "innerRight": "",
          "outerRight": "GINGER"
        },
        {
          "characterID": "BIG BAD WOLF",
          "backgroundID": "christmastree",
          "dialogueText": "빨간 망토야, 그 사이 훨씬 맛있어졌구나. 그렇게 자극하면 먹고 싶어진다?",
          "emotionID": "surprise",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "",
          "center": "BIG BAD WOLF",
          "innerRight": "",
          "outerRight": ""
        },
        {
          "characterID": "LITTLERED",
          "backgroundID": "christmastree",
          "dialogueText": "오늘 널 이기고 정의를 실현하겠어, 나쁜 늑대!",
          "emotionID": "angry",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "GOLDILOCKS",
          "innerLeft": "",
          "center": "LITTLERED",
          "innerRight": "",
          "outerRight": "GINGER"
        },
        {
          "characterID": "BIG BAD WOLF",
          "backgroundID": "christmastree",
          "dialogueText": "오호호, 장난도 참!",
          "emotionID": "surprise",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "",
          "center": "BIG BAD WOLF",
          "innerRight": "",
          "outerRight": ""
        }
      ]
    }
  ]
}