    "m_Name": "EventStory_ChristmasEvent1_8",
    "unskippable": 0,
    "dialogueDataEntries": [
        {
            "languageID": "English",
            "dialogueLines": [
                {
                    "characterID": "",
                    "backgroundID": "christmastree",
                    "dialogueText": "Despite your best efforts, it's impossible to prevent being pushed back by the sheer weight of numbers Krampus and the Rat King can throw at you.",
                    "emotionID": "",
                    "musicID": "christmasbattle",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Krampus",
                    "center": "",
                    "innerRight": "Fullbody_Hero_RatKing",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "christmastree",
                    "dialogueText": "All seems hopeless as Krampus gloats in front of you.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Krampus",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Krampus",
                    "backgroundID": "christmastree",
                    "dialogueText": "Yes! Yes!",
                    "emotionID": "happy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Krampus",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Krampus",
                    "backgroundID": "christmastree",
                    "dialogueText": "Christmas will soon be a thing of the past!",
                    "emotionID": "conspire",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Krampus",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "christmastree",
                    "dialogueText": "And it seems like that is really true.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Krampus",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Nutcracker",
                    "backgroundID": "christmastree",
                    "dialogueText": "We can't hold them!",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Nutcracker",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "christmastree",
                    "dialogueText": "It is at that moment that Marie appears and catches Nutcracker before she can fall!",
                    "emotionID": "",
                    "musicID": "none",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Nutcracker",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Marie",
                    "outerRight": ""
                },
                {
                    "characterID": "Marie",
                    "backgroundID": "christmastree",
                    "dialogueText": "NUTCRACKER! I've got you.",
                    "emotionID": "angry",
                    "musicID": "heroicboss",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Nutcracker",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Marie",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "christmastree",
                    "dialogueText": "And with Marie comes a fairy you've never met.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_SugarPlumFairy",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Sugar Plum Fairy",
                    "backgroundID": "christmastree",
                    "dialogueText": "I'm the Sugar Plum Fairy. Any friend of Marie and Nutcracker is a friend of mine!",
                    "emotionID": "happy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_SugarPlumFairy",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "christmastree",
                    "dialogueText": "And to your surprise, Jack Frost, as well!",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_JackFrost",
                    "center": "",
                    "innerRight": "Fullbody_Hero_SugarPlumFairy",
                    "outerRight": ""
                },
                {
                    "characterID": "Jack Frost",
                    "backgroundID": "christmastree",
                    "dialogueText": "Looks like you're in a bit of trouble, Dreamer. How about we put this situation on ice?",
                    "emotionID": "happy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_JackFrost",
                    "center": "",
                    "innerRight": "Fullbody_Hero_SugarPlumFairy",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "christmastree",
                    "dialogueText": "And not only them, but in the next moment, Aurora, Esme and Vita pop out of thin air along with a few familiar faces!",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Esme",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Vita"
                },
                {
                    "characterID": "Aurora",
                    "backgroundID": "christmastree",
                    "dialogueText": "DREAMER!",
                    "emotionID": "happy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Esme",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Vita"
                },
                {
                    "characterID": "Esme",
                    "backgroundID": "christmastree",
                    "dialogueText": "See, I told you they would have survived this long!",
                    "emotionID": "happy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Esme",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Vita"
                },
                {
                    "characterID": "Vita",
                    "backgroundID": "christmastree",
                    "dialogueText": "We brought militia and townsfolk to help fight!",
                    "emotionID": "happy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Esme",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Vita"
                },
                {
                    "characterID": "",
                    "backgroundID": "christmastree",
                    "dialogueText": "Red! Goldilocks! Ginger!",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Goldilocks",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_RedRidingHood",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Ginger"
                },
                {
                    "characterID": "Ginger",
                    "backgroundID": "christmastree",
                    "dialogueText": "We heard you were under siege on all sides, without even any cakes to help you!",
                    "emotionID": "idle",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Goldilocks",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_RedRidingHood",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Ginger"
                },
                {
                    "characterID": "",
                    "backgroundID": "christmastree",
                    "dialogueText": "You're not sure how cakes would help the situation.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Goldilocks",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_RedRidingHood",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Ginger"
                },
                {
                    "characterID": "Ginger",
                    "backgroundID": "christmastree",
                    "dialogueText": "Couldn't hurt!",
                    "emotionID": "idle",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Goldilocks",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_RedRidingHood",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Ginger"
                },
                {
                    "characterID": "Red",
                    "backgroundID": "christmastree",
                    "dialogueText": "Time to turn the tide!",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Goldilocks",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_RedRidingHood",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Ginger"
                },
                {
                    "characterID": "Red",
                    "backgroundID": "christmastree",
                    "dialogueText": "Everyone, charge!",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Goldilocks",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_RedRidingHood",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Ginger"
                }
            ]
        },
        {
            "languageID": "Korean",
            "dialogueLines": [
                {
                    "characterID": "",
                    "backgroundID": "christmastree",
                    "dialogueText": "쥐마왕이 전장에 나타나자 드림리스의 기세가 한층 더 커졌다.",
                    "emotionID": "",
                    "musicID": "christmasbattle",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Krampus",
                    "center": "",
                    "innerRight": "Fullbody_Hero_RatKing",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "christmastree",
                    "dialogueText": "잠깐 당황하는 것 같던 크람푸스도 기쁜 얼굴로 쥐마왕을 응원하기 시작했다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Krampus",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "크람푸스",
                    "backgroundID": "christmastree",
                    "dialogueText": "좋아! 이대로 다 부숴버리라구!",
                    "emotionID": "happy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Krampus",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "크람푸스",
                    "backgroundID": "christmastree",
                    "dialogueText": "크리스마스는 오늘로 끝이야, 앞으로 다신 없을 테니 각오해!",
                    "emotionID": "conspire",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Krampus",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "christmastree",
                    "dialogueText": "버티고 서있던 히나와 호두까기 인형이 힘겹게 숨을 몰아쉰다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Krampus",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "호두까기 인형",
                    "backgroundID": "christmastree",
                    "dialogueText": "제가 막을 테니 당신은 꿈꾸는 자와 함께 후퇴하십시오. 승리를 장담할 수 없습니다.",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Nutcracker",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "christmastree",
                    "dialogueText": "호두까기 인형의 목소리에 절망이 섞여 있었다.",
                    "emotionID": "",
                    "musicID": "none",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Nutcracker",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Marie",
                    "outerRight": ""
                },
                {
                    "characterID": "마리",
                    "backgroundID": "christmastree",
                    "dialogueText": "거기! 나의 기사님을 공격하는 불한당이 대체 누구죠? 용서할 수 없어요!",
                    "emotionID": "angry",
                    "musicID": "heroicboss",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Nutcracker",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Marie",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "christmastree",
                    "dialogueText": "그때 지팡이를 휘두르며 나타난 마리가 호두까기 인형의 팔을 붙잡았다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_SugarPlumFairy",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "설탕 요정",
                    "backgroundID": "christmastree",
                    "dialogueText": "웃후후, 쥐마왕 소년을 이런 곳에서 다시 보는군요. 달콤함이 뭔지 보여드리죠!",
                    "emotionID": "happy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_SugarPlumFairy",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "christmastree",
                    "dialogueText": "마리와 함께 나타난 나비 날개를 가진 요정 뒤에서 뜻밖의 소년이 고개를 불쑥 내밀었다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_JackFrost",
                    "center": "",
                    "innerRight": "Fullbody_Hero_SugarPlumFairy",
                    "outerRight": ""
                },
                {
                    "characterID": "잭 프로스트",
                    "backgroundID": "christmastree",
                    "dialogueText": "야, 꼴이 그게 뭐냐? 너무 한심해서 도와줄 수밖에 없잖아.",
                    "emotionID": "happy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_JackFrost",
                    "center": "",
                    "innerRight": "Fullbody_Hero_SugarPlumFairy",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "christmastree",
                    "dialogueText": "잭 프로스트가 히죽 웃는 얼굴로 말했다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Esme",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Vita"
                },
                {
                    "characterID": "오로라",
                    "backgroundID": "christmastree",
                    "dialogueText": "꿈꾸는 자!",
                    "emotionID": "happy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Esme",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Vita"
                },
                {
                    "characterID": "에스메",
                    "backgroundID": "christmastree",
                    "dialogueText": "빠진 사람 없이 다 온 거지?",
                    "emotionID": "happy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Esme",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Vita"
                },
                {
                    "characterID": "비타",
                    "backgroundID": "christmastree",
                    "dialogueText": "야, 잠탱아! 민병대원이랑 메르헨들도 몇 명 데리고 왔어!",
                    "emotionID": "happy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Esme",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Vita"
                },
                {
                    "characterID": "",
                    "backgroundID": "christmastree",
                    "dialogueText": "빨간 망토와 골디락스가 드림리스를 매섭게 노려본다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Goldilocks",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_RedRidingHood",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Ginger"
                },
                {
                    "characterID": "진저",
                    "backgroundID": "christmastree",
                    "dialogueText": "앗하하, 꿈꾸는 자~ 위험하다길래 구하러 왔어! 케이크도 가져왔는데, 먹을래?",
                    "emotionID": "idle",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Goldilocks",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_RedRidingHood",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Ginger"
                },
                {
                    "characterID": "",
                    "backgroundID": "christmastree",
                    "dialogueText": "케이크 상자를 높게 들어 올린 진저가 상큼하게 미소 지었다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Goldilocks",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_RedRidingHood",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Ginger"
                },
                {
                    "characterID": "진저",
                    "backgroundID": "christmastree",
                    "dialogueText": "힘들 때, 아플 때, 지칠 때, 삶의 의욕이 없을 때 필요한 건 바로 당분! 케이크지. 그치?",
                    "emotionID": "idle",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Goldilocks",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_RedRidingHood",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Ginger"
                },
                {
                    "characterID": "빨간 망토",
                    "backgroundID": "christmastree",
                    "dialogueText": "흐하항, 맞는 말이야! 근데 일단, 어디 보자.",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Goldilocks",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_RedRidingHood",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Ginger"
                },
                {
                    "characterID": "빨간 망토",
                    "backgroundID": "christmastree",
                    "dialogueText": "난 말이지, 내 친구를 괴롭히면 똑같이 갚아주자는 편이라서 말이야. 상황 정리부터 하자구!",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Goldilocks",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_RedRidingHood",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Ginger"
                }
            ]
        }
    ]
}