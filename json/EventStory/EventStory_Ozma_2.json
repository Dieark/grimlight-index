    "m_Name": "EventStory_Ozma_2",
    "unskippable": 0,
    "dialogueDataEntries": [
        {
            "languageID": "English",
            "dialogueLines": [
                {
                    "characterID": "",
                    "backgroundID": "queenspalace",
                    "dialogueText": "Days of searching and analysis of Dorothy's conversation have led you to an abandoned palace located within the Queen of Heart's domain.",
                    "emotionID": "",
                    "musicID": "shoptheme",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "queenspalace",
                    "dialogueText": "Esme is not too thrilled about this development.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "queenspalace",
                    "dialogueText": "You step into the brightly lit hallway with only the echoes of your footsteps ringing in your ears.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Esme",
                    "backgroundID": "queenspalace",
                    "dialogueText": "Why are we all the way out here again?! I don't like it here at all!",
                    "emotionID": "sad",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Esme",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Vita",
                    "backgroundID": "queenspalace",
                    "dialogueText": "Calm down, Esme! The Queen of Hearts is gone now. She can't eat you anymore.",
                    "emotionID": "happy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Esme",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Vita",
                    "outerRight": ""
                },
                {
                    "characterID": "Esme",
                    "backgroundID": "queenspalace",
                    "dialogueText": "T-That's not what I was worried about! Not at all... hmph!",
                    "emotionID": "shy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Esme",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Vita",
                    "outerRight": ""
                },
                {
                    "characterID": "Esme",
                    "backgroundID": "queenspalace",
                    "dialogueText": "I just don't like abandoned palaces, okay? ",
                    "emotionID": "shy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Esme",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Vita",
                    "outerRight": ""
                },
                {
                    "characterID": "Vita",
                    "backgroundID": "queenspalace",
                    "dialogueText": "Oh, shush. I love it when abandoned buildings are reclaimed by nature. Vines creeping up the parapets, flowers blossoming between stone pathways...",
                    "emotionID": "happy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Esme",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Vita",
                    "outerRight": ""
                },
                {
                    "characterID": "Vita",
                    "backgroundID": "queenspalace",
                    "dialogueText": "There's a subtle beauty to it all~!",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Esme",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Vita",
                    "outerRight": ""
                },
                {
                    "characterID": "Vita",
                    "backgroundID": "queenspalace",
                    "dialogueText": "Speaking of which... why isn't this place overrun with wildlife and plants by now? This area should have been destroyed by Dreamless and abandoned many centuries ago!",
                    "emotionID": "sad",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Esme",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Vita",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "queenspalace",
                    "dialogueText": "At that moment, small taps ring out from atop the main staircase... the sound of elegant shoes clinking upon the marble.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Esme",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Vita",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "queenspalace",
                    "dialogueText": "They belong to a small girl dressed in a jet black dress adorned with brilliant red roses. The loose gown flutters with each step as she descends the staircase.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_PrincessOzma",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Ozma",
                    "backgroundID": "queenspalace",
                    "dialogueText": "Are you here to take away what's left of my dignity?",
                    "emotionID": "angry",
                    "musicID": "challengeofwills",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_PrincessOzma",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Ozma",
                    "backgroundID": "queenspalace",
                    "dialogueText": "Was my kingdom not enough? Begone, foul Dreamless!",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "shake",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_PrincessOzma",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "queenspalace",
                    "dialogueText": "A wave of despair washes over you, and you feel frightened beneath the heel of Ozma's magic.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_PrincessOzma",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "queenspalace",
                    "dialogueText": "You fall to your knees, unable to remain standing before her.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_PrincessOzma",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "queenspalace",
                    "dialogueText": "If she can command such power, then Dorothy was right about her...",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_PrincessOzma",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Esme",
                    "backgroundID": "queenspalace",
                    "dialogueText": "So she's the one we're looking for? Tell her who we are, Dreamer! The pressure of this magic... is immense!",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Esme",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "queenspalace",
                    "dialogueText": "You muster all of your strength and barely manage to lift your head enough to get a proper look at her.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_PrincessOzma",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "queenspalace",
                    "dialogueText": "Ozma's eyes are deep black and sorrowful. She's in pain.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_PrincessOzma",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "queenspalace",
                    "dialogueText": "You need to stop her... at least enough to have a proper conversation. You are only able to summon a single one of your heroes to perform this task.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_PrincessOzma",
                    "innerRight": "",
                    "outerRight": ""
                }
            ]
        },
        {
            "languageID": "Korean",
            "dialogueLines": [
                {
                    "characterID": "",
                    "backgroundID": "queenspalace",
                    "dialogueText": "우리는 왕녀 오즈마가 머물고 있을법한 장소를 추려 조사했다.",
                    "emotionID": "",
                    "musicID": "shoptheme",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "queenspalace",
                    "dialogueText": "그 중 가장 유력한 장소는 퀸 오브 하트의 버려진 궁전이었는데, 가는 내내 에스메가 삐죽한 목소리로 투덜거렸다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "queenspalace",
                    "dialogueText": "여기저기 부서진 흔적이 남아있는 빈 궁전의 복도에 일행들의 발자국 소리가 메아리처럼 울린다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "에스메",
                    "backgroundID": "queenspalace",
                    "dialogueText": "하필 왜 여기야? 으으. 난 여기 싫어.",
                    "emotionID": "sad",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Esme",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "비타",
                    "backgroundID": "queenspalace",
                    "dialogueText": "햐! 조용히 좀 해, 바보야! 퀸 오브 하트는 이제 없다궁. 왜 이렇게 삐죽거려? 삐죽삐죽.",
                    "emotionID": "happy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Esme",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Vita",
                    "outerRight": ""
                },
                {
                    "characterID": "에스메",
                    "backgroundID": "queenspalace",
                    "dialogueText": "뭐!? 내가 언제 삐죽거렸다고!",
                    "emotionID": "shy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Esme",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Vita",
                    "outerRight": ""
                },
                {
                    "characterID": "에스메",
                    "backgroundID": "queenspalace",
                    "dialogueText": "아니, 말을 말자. 흥!",
                    "emotionID": "shy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Esme",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Vita",
                    "outerRight": ""
                },
                {
                    "characterID": "비타",
                    "backgroundID": "queenspalace",
                    "dialogueText": "너 계속 말하고 있는데? 흠, 흠. 이런거 좋지 않아? 버려진 건물들이 자연의 일부가 되는 거!",
                    "emotionID": "happy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Esme",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Vita",
                    "outerRight": ""
                },
                {
                    "characterID": "비타",
                    "backgroundID": "queenspalace",
                    "dialogueText": "난간은 꽃을 위한 지지대가 되고, 깨진 타일은 나무 뿌리를 위한 덮개가 되지. 아름답지 않아?",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Esme",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Vita",
                    "outerRight": ""
                },
                {
                    "characterID": "비타",
                    "backgroundID": "queenspalace",
                    "dialogueText": "뭐, 이 근처는 자연의 재생이 쪼오끔 느린 것 같긴 하지만... 버려진지 꽤 오래 됐는데 식물은 그렇다치고 동물들이 안 보이네.",
                    "emotionID": "sad",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Esme",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Vita",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "queenspalace",
                    "dialogueText": "비타가 춤을 추듯 바닥에 핀 민들레를 폴짝 넘어서며 말했다. 그때, 중앙 계단에서 단정한 발소리가 들려온다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Esme",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Vita",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "queenspalace",
                    "dialogueText": "깨진 대리석 바닥을 우아한 걸음으로 딛고 내려온 사람은 붉은 장미로 장식한 드레스를 입은 작은 소녀였다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_PrincessOzma",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "왕녀 오즈마",
                    "backgroundID": "queenspalace",
                    "dialogueText": "이곳은 사치스러운 무덤이야. 조용하고 화려하지. 하지만...",
                    "emotionID": "angry",
                    "musicID": "challengeofwills",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_PrincessOzma",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "왕녀 오즈마",
                    "backgroundID": "queenspalace",
                    "dialogueText": "감히 내 앞에 다시 나타날 생각을 하다니, 이 역겨운 드림리스.",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "shake",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_PrincessOzma",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "queenspalace",
                    "dialogueText": "서늘한 목소리가 중앙홀에 내려앉는다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_PrincessOzma",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "queenspalace",
                    "dialogueText": "잔잔한 표정아래 숨겨진 절망 섞인 분노가 내리꽂혔다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_PrincessOzma",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "queenspalace",
                    "dialogueText": "도로시의 말대로, 오즈마는 강력한 마법을 다룰 수 있었다. 나도 모르게 짓눌려 무릎을 꿇고 말았다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_PrincessOzma",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "에스메",
                    "backgroundID": "queenspalace",
                    "dialogueText": "네가 찾는 오즈마가 저 사람이야!? 조심해, 꿈꾸는 자! 이 마법... 보통 힘이 아니야!",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Esme",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "queenspalace",
                    "dialogueText": "온 힘을 다해 오즈마의 마법에 저항했다. 겨우 고개를 들어 그녀를 바라볼 수 있었다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_PrincessOzma",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "queenspalace",
                    "dialogueText": "고통으로 일그러진 새카만 눈동자에 분노가 일렁인다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_PrincessOzma",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "queenspalace",
                    "dialogueText": "섣불리 자극하고 싶지 않았지만, 이대로 두면 폭주할지도 모른다. 빠르게 제압해야한다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_PrincessOzma",
                    "innerRight": "",
                    "outerRight": ""
                }
            ]
        }
    ]
}