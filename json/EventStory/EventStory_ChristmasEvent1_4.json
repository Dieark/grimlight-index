    "m_Name": "EventStory_ChristmasEvent1_4",
    "unskippable": 0,
    "dialogueDataEntries": [
        {
            "languageID": "English",
            "dialogueLines": [
                {
                    "characterID": "",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "The dreamless rat-soldiers are defeated and the pair turn to you in gratitude",
                    "emotionID": "",
                    "musicID": "intothewild",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Marie",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "Thank you, thank you!",
                    "emotionID": "happy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Marie",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Nutcracker",
                    "outerRight": ""
                },
                {
                    "characterID": "Marie",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "I thought we were going to perish out here!",
                    "emotionID": "idle",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Marie",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Nutcracker",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "They might yet if there are more of those creatures around! But who are they and what's going on here?",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Marie",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Nutcracker",
                    "outerRight": ""
                },
                {
                    "characterID": "Marie",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "I'm Marie and this is my companion, Nutcracker.",
                    "emotionID": "happy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Marie",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Nutcracker",
                    "outerRight": ""
                },
                {
                    "characterID": "Marie",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "She used to be a toy... but she magically came to life.",
                    "emotionID": "idle",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Marie",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Nutcracker",
                    "outerRight": ""
                },
                {
                    "characterID": "Nutcracker",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "Sweet Marie, it only appeared that way to you. I was alive all the time, you just couldn't see it.",
                    "emotionID": "shy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Marie",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Nutcracker",
                    "outerRight": ""
                },
                {
                    "characterID": "Nutcracker",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "It wasn't until I was wounded by my arch nemesis that you were able to see, through the kindness of your heart.",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Marie",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Nutcracker",
                    "outerRight": ""
                },
                {
                    "characterID": "Marie",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "I thought my brother broke you when he was playing with you. Who could have known you were truly my gallant saviour and not just a wooden toy!",
                    "emotionID": "happy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Marie",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Nutcracker",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "...",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Marie",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Nutcracker",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "Huh... okay, well, you've heard stranger stories since coming to Phantasia – but not many.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Marie",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Nutcracker",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "But why are they out here? And what were those things?",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Marie",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Nutcracker",
                    "outerRight": ""
                },
                {
                    "characterID": "Marie",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "They're Dreamless... but a very particular kind of Dreamless.",
                    "emotionID": "annoy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Marie",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Nutcracker",
                    "outerRight": ""
                },
                {
                    "characterID": "Nutcracker",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "We were tracking these creatures. They belong to an old enemy of ours.",
                    "emotionID": "idle",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Marie",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Nutcracker",
                    "outerRight": ""
                },
                {
                    "characterID": "Marie",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "It took us a while to work out what they're doing, but we think they're heading for Santa's workshop!",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Marie",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Nutcracker",
                    "outerRight": ""
                },
                {
                    "characterID": "Marie",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "There's a huge army of them!",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Marie",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Nutcracker",
                    "outerRight": ""
                },
                {
                    "characterID": "Nutcracker",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "Unfortunately, we were discovered and ambushed before we could raise any alarm or send any warning.",
                    "emotionID": "idle",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Marie",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Nutcracker",
                    "outerRight": ""
                },
                {
                    "characterID": "Marie",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "We've been running ever since!",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Marie",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Nutcracker",
                    "outerRight": ""
                },
                {
                    "characterID": "Lis",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "Santa's workshop! Oh no!",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "bounce",
                    "outerLeft": "Fullbody_Hero_Marie",
                    "innerLeft": "",
                    "center": "Fullbody_NPC_WinterLis",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Nutcracker"
                },
                {
                    "characterID": "Lis",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "We have to warn them!",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Marie",
                    "innerLeft": "",
                    "center": "Fullbody_NPC_WinterLis",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Nutcracker"
                },
                {
                    "characterID": "",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "You agree. Can this pair lead you there?",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Marie",
                    "innerLeft": "",
                    "center": "Fullbody_NPC_WinterLis",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Nutcracker"
                },
                {
                    "characterID": "Nutcracker",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "Of course, Dreamer, follow us!",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Marie",
                    "innerLeft": "",
                    "center": "Fullbody_NPC_WinterLis",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Nutcracker"
                }
            ]
        },
        {
            "languageID": "Korean",
            "dialogueLines": [
                {
                    "characterID": "",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "전투 끝에 우리는 쥐 병사들을 모두 물리칠 수 있었다.",
                    "emotionID": "",
                    "musicID": "intothewild",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "마리",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "어머, 정말 감사해요. 성함이 꿈꾸는 자이시군요.",
                    "emotionID": "happy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Marie",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Nutcracker",
                    "outerRight": ""
                },
                {
                    "characterID": "마리",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "아후후, 여러분을 만나지 않았다면 정말 큰일 날뻔했지 뭐예요!",
                    "emotionID": "idle",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Marie",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Nutcracker",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "춤을 추듯 가볍게 한 바퀴 빙글 돈 여자가 치맛단을 살짝 들어 올리며 인사했다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Marie",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Nutcracker",
                    "outerRight": ""
                },
                {
                    "characterID": "마리",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "제 소개가 늦었네요! 안녕하세요, 꿈꾸는 자! 판타지아는 정말 신비한 곳이네요.",
                    "emotionID": "happy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Marie",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Nutcracker",
                    "outerRight": ""
                },
                {
                    "characterID": "마리",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "저는 마리라고 해요, 이쪽은 저의 기사님인 호두까기 인형이랍니다.",
                    "emotionID": "idle",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Marie",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Nutcracker",
                    "outerRight": ""
                },
                {
                    "characterID": "호두까기 인형",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "도움에 감사드립니다, 꿈꾸는 자. 적들은 후퇴하였으니 당분간은 괜찮을 겁니다.",
                    "emotionID": "shy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Marie",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Nutcracker",
                    "outerRight": ""
                },
                {
                    "characterID": "호두까기 인형",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "서둘러 이 숲을 벗어나시길 추천 드립니다. 밤이 오면 더 위험해질 겁니다.",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Marie",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Nutcracker",
                    "outerRight": ""
                },
                {
                    "characterID": "마리",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "기사님도 참, 네가 있는데 무슨 걱정이니? 위험해지면 또 나를 지켜줄 거잖아.",
                    "emotionID": "happy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Marie",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Nutcracker",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "호두까기 인형이 은근슬쩍 마리의 시선을 피한다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Marie",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Nutcracker",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "창백한 인형의 얼굴에 은은한 붉은 기운이 맴도는 것 같았다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Marie",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Nutcracker",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "두 사람이 어쩌다 여기까지 오게 된 건지, 방금 쥐 병사들은 무엇이었는지 물었다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Marie",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Nutcracker",
                    "outerRight": ""
                },
                {
                    "characterID": "마리",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "그들은 드림리스에요. 정확하게 말하자면, 조금 특별한 드림리스라고 할 수 있겠네요.",
                    "emotionID": "annoy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Marie",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Nutcracker",
                    "outerRight": ""
                },
                {
                    "characterID": "호두까기 인형",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "저는 드림리스를 조종하는 이가 누구인지 알고 있습니다. 때문에 마리 님과 함께 그들을 추적하고 있었습니다.",
                    "emotionID": "idle",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Marie",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Nutcracker",
                    "outerRight": ""
                },
                {
                    "characterID": "마리",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "맞아요! 대체 무슨 속셈인지는 모르지만, 아주 나쁜 짓을 꾸미고 있는 게 분명해요.",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Marie",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Nutcracker",
                    "outerRight": ""
                },
                {
                    "characterID": "마리",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "더군다나 여긴 겨울의 영역이잖아요! 제 추측이지만 드림리스들이 산타의 작업실을 노리고 있는 것 같아요, 정말 너무하죠?",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Marie",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Nutcracker",
                    "outerRight": ""
                },
                {
                    "characterID": "호두까기 인형",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "산타클로스님에게 그 사실을 전하러 가던 길이었습니다만...",
                    "emotionID": "idle",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Marie",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Nutcracker",
                    "outerRight": ""
                },
                {
                    "characterID": "마리",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "일이 이렇게 꼬일 줄 누가 알았겠어요? 너무 슬퍼요.",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Marie",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Nutcracker",
                    "outerRight": ""
                },
                {
                    "characterID": "리즈",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "그럼 산타클로스는 어떻게 되는 거야? 이대로 두면 안 되는 거잖아!",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "bounce",
                    "outerLeft": "Fullbody_Hero_Marie",
                    "innerLeft": "",
                    "center": "Fullbody_NPC_WinterLis",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Nutcracker"
                },
                {
                    "characterID": "리즈",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "어, 얼른 산타에게 알려줘야 해! 위험한 거지? 그렇지? 꿈꾸는 자, 얼른 가서 도와주자!",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Marie",
                    "innerLeft": "",
                    "center": "Fullbody_NPC_WinterLis",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Nutcracker"
                },
                {
                    "characterID": "",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "리즈가 안절부절못하며 손을 잡아당겼다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Marie",
                    "innerLeft": "",
                    "center": "Fullbody_NPC_WinterLis",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Nutcracker"
                },
                {
                    "characterID": "호두까기 인형",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "저는 명령을 따를 준비를 모두 마쳤습니다, 꿈꾸는 자. 함께 가시겠습니까?",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Marie",
                    "innerLeft": "",
                    "center": "Fullbody_NPC_WinterLis",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Nutcracker"
                }
            ]
        }
    ]
}