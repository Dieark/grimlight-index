    "m_Name": "EventStory_BriarRose_2",
    "unskippable": 0,
    "dialogueDataEntries": [
        {
            "languageID": "English",
            "dialogueLines": [
                {
                    "characterID": "",
                    "backgroundID": "abandonedcity",
                    "dialogueText": "You have returned to the ruined city. It is still dangerous, but there seem to be less Dreamless here than there once were.",
                    "emotionID": "",
                    "musicID": "intothewild",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "abandonedcity",
                    "dialogueText": "You wander the streets. Fortunately, they don't display the changing, maze-like quality of your dream.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "abandonedcity",
                    "dialogueText": "You wonder where this maze is supposed to be and how you're supposed to gain entrance to it?",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "abandonedcity",
                    "dialogueText": "It doesn't take long, however, for Vita to stop talking and call for quiet.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Vita",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Vita",
                    "backgroundID": "abandonedcity",
                    "dialogueText": "Listen!",
                    "emotionID": "angry",
                    "musicID": "haunted",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Vita",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Vita",
                    "backgroundID": "abandonedcity",
                    "dialogueText": "Can you hear that?",
                    "emotionID": "idle",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Vita",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Esme",
                    "backgroundID": "abandonedcity",
                    "dialogueText": "Hear what?",
                    "emotionID": "idle",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Aurora",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Vita",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "Vita",
                    "backgroundID": "abandonedcity",
                    "dialogueText": "That voice...",
                    "emotionID": "idle",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Aurora",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Vita",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "Esme",
                    "backgroundID": "abandonedcity",
                    "dialogueText": "Vita, what are you talking about, there's no voice...",
                    "emotionID": "annoy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Aurora",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Vita",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "Vita",
                    "backgroundID": "abandonedcity",
                    "dialogueText": "No! It's faint, but...",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Aurora",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Vita",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "Aurora",
                    "backgroundID": "abandonedcity",
                    "dialogueText": "Is it Briar?",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Aurora",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Vita",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "Aurora",
                    "backgroundID": "abandonedcity",
                    "dialogueText": "Where is it coming from?",
                    "emotionID": "idle",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Aurora",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Vita",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "",
                    "backgroundID": "abandonedcity",
                    "dialogueText": "Vita turns this way and that, searching.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Aurora",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Vita",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "Vita",
                    "backgroundID": "abandonedcity",
                    "dialogueText": "Ahhhh... over there!",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Aurora",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Vita",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "",
                    "backgroundID": "abandonedcity",
                    "dialogueText": "Vita points in a direction that takes you deeper into the city.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Aurora",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Vita",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "",
                    "backgroundID": "abandonedcity",
                    "dialogueText": "Could that be where Briar is being held captive?",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Aurora",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Vita",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "",
                    "backgroundID": "abandonedcity",
                    "dialogueText": "You tell her to lead in the direction she can most easily hear the voice.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Aurora",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Vita",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "Vita",
                    "backgroundID": "abandonedcity",
                    "dialogueText": "Got it!",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Aurora",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Vita",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "",
                    "backgroundID": "abandonedcity",
                    "dialogueText": "As you all walk through the empty streets, you find yourself walking alongside Esme and voice a question that has been plaguing you.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Aurora",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Vita",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "",
                    "backgroundID": "abandonedcity",
                    "dialogueText": "How come Vita can hear Briar but no one else can?",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Aurora",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Vita",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "Esme",
                    "backgroundID": "abandonedcity",
                    "dialogueText": "We world stones have a kind of... connection to the realms we are assigned to. We almost become part of that realm and the Dominion is like a nexus of the realm's power, so...",
                    "emotionID": "idle",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Aurora",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Vita",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "Esme",
                    "backgroundID": "abandonedcity",
                    "dialogueText": "We kind of have a mental link.",
                    "emotionID": "idle",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Aurora",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Vita",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "",
                    "backgroundID": "abandonedcity",
                    "dialogueText": "Like telepathy?",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Aurora",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Vita",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "Esme",
                    "backgroundID": "abandonedcity",
                    "dialogueText": "I guess you could call it that, though it doesn't happen all the time. In moments of stress, though, or if we concentrate really hard, we can sometimes hear each other's thoughts.",
                    "emotionID": "idle",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Aurora",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Vita",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "Vita",
                    "backgroundID": "abandonedcity",
                    "dialogueText": "There! I see something!",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Aurora",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Vita",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "",
                    "backgroundID": "storybriarroseportal1",
                    "dialogueText": "As you round a corner, you witness the strangest of sights in the distance.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "storybriarroseportal1",
                    "dialogueText": "A disc of some kind of whirling energy hangs static in midair, and through the centre of it...",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Vita",
                    "backgroundID": "abandonedpalace",
                    "dialogueText": "Her voice is coming from inside... whatever that thing is.",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Aurora",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Vita",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "",
                    "backgroundID": "abandonedpalace",
                    "dialogueText": "A portal of some kind?",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Aurora",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Vita",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "",
                    "backgroundID": "abandonedpalace",
                    "dialogueText": "It obviously leads somewhere else in Phantasia, perhaps even beyond the borders of the Enchanted Realm.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Aurora",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Vita",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "",
                    "backgroundID": "abandonedpalace",
                    "dialogueText": "Is that where Briar is being held? Is that how Drak was able to become Dominion?",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Aurora",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Vita",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "Aurora",
                    "backgroundID": "abandonedpalace",
                    "dialogueText": "It would seem that is our next destination... Dreamer, shall we?",
                    "emotionID": "idle",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Aurora",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Vita",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                }
            ]
        },
        {
            "languageID": "Korean",
            "dialogueLines": [
                {
                    "characterID": "",
                    "backgroundID": "abandonedcity",
                    "dialogueText": "폐혀가 된 도시는 여전히 위험한 장소였지만 드림리스의 수는 확실히 줄었다.",
                    "emotionID": "",
                    "musicID": "intothewild",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "abandonedcity",
                    "dialogueText": "꿈속에서 본 도시의 여기저기를 배회하며 조사했지만 <br>미궁을 찾을 수는 없었다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "abandonedcity",
                    "dialogueText": "미궁으로 들어갈 방법을 찾아야한다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "abandonedcity",
                    "dialogueText": "탐색을 시작한지 얼마 후, 멀리서 비타의 목소리가 들렸다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Vita",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "비타",
                    "backgroundID": "abandonedcity",
                    "dialogueText": "잠깐! 조용히 해봐!",
                    "emotionID": "angry",
                    "musicID": "haunted",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Vita",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "비타",
                    "backgroundID": "abandonedcity",
                    "dialogueText": "저 소리 안 들려?",
                    "emotionID": "idle",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Vita",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "에스메",
                    "backgroundID": "abandonedcity",
                    "dialogueText": "무슨 소리?",
                    "emotionID": "idle",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Aurora",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Vita",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "비타",
                    "backgroundID": "abandonedcity",
                    "dialogueText": "지금 이 소리 말야! <br>목소리가 날 부르고 있어.",
                    "emotionID": "idle",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Aurora",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Vita",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "에스메",
                    "backgroundID": "abandonedcity",
                    "dialogueText": "너 무슨 말을 하는 거야?<br>네 목소리 말고는 아무 소리도 안 들리는데.",
                    "emotionID": "annoy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Aurora",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Vita",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "비타",
                    "backgroundID": "abandonedcity",
                    "dialogueText": "아냐, 정말 들린다구!<br>아주 작은 소리지만, 틀림없어.",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Aurora",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Vita",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "오로라",
                    "backgroundID": "abandonedcity",
                    "dialogueText": "비타, 브라이어 로즈인가요?",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Aurora",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Vita",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "오로라",
                    "backgroundID": "abandonedcity",
                    "dialogueText": "어느 쪽에서 들리는지 알려 주시겠어요?",
                    "emotionID": "idle",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Aurora",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Vita",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "",
                    "backgroundID": "abandonedcity",
                    "dialogueText": "목소리가 들리는 방향을 찾아 비타가 이리저리 돌아다닌다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Aurora",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Vita",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "비타",
                    "backgroundID": "abandonedcity",
                    "dialogueText": "여기서 아주 가까워. <br>그래! 바로 저기야.",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Aurora",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Vita",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "",
                    "backgroundID": "abandonedcity",
                    "dialogueText": "비타가 도시 안쪽으로 들어가는 길을 가리킨다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Aurora",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Vita",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "",
                    "backgroundID": "abandonedcity",
                    "dialogueText": "지금까지 가본 적 없는 길이다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Aurora",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Vita",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "",
                    "backgroundID": "abandonedcity",
                    "dialogueText": "비타에게 안내를 부탁했다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Aurora",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Vita",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "비타",
                    "backgroundID": "abandonedcity",
                    "dialogueText": "응, 맡겨줘!",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Aurora",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Vita",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "",
                    "backgroundID": "abandonedcity",
