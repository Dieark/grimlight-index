  "m_Name": "EventStory_ChristmasEvent1_1",
  "unskippable": 0,
  "dialogueDataEntries": [
    {
      "languageID": "English",
      "dialogueLines": [
        {
          "characterID": "",
          "backgroundID": "christmashaven",
          "dialogueText": "Snow is falling in Haven, and the people are hurrying to their homes, arms laden with brightly wrapped gifts from toy stores, candy shops, and Ginger's bakery.",
          "emotionID": "",
          "musicID": "holidaycheer",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "",
          "center": "",
          "innerRight": "",
          "outerRight": ""
        },
        {
          "characterID": "",
          "backgroundID": "christmashaven",
          "dialogueText": "It's nearly Christmas!",
          "emotionID": "",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "",
          "center": "",
          "innerRight": "",
          "outerRight": ""
        },
        {
          "characterID": "",
          "backgroundID": "christmashaven",
          "dialogueText": "And one young girl is more excited about it than most...",
          "emotionID": "",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "",
          "center": "",
          "innerRight": "",
          "outerRight": ""
        },
        {
          "characterID": "WINTERLIS",
          "backgroundID": "christmashaven",
          "dialogueText": "Mal, it's Christmas soon, I can't wait!",
          "emotionID": "happy",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "WINTERLIS",
          "center": "",
          "innerRight": "WINTERMAL",
          "outerRight": ""
        },
        {
          "characterID": "WINTERLIS",
          "backgroundID": "christmashaven",
          "dialogueText": "Do you think we'll get to see him?",
          "emotionID": "surprise",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "WINTERLIS",
          "center": "",
          "innerRight": "WINTERMAL",
          "outerRight": ""
        },
        {
          "characterID": "WINTERMAL",
          "backgroundID": "christmashaven",
          "dialogueText": "See who?",
          "emotionID": "",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "WINTERLIS",
          "center": "",
          "innerRight": "WINTERMAL",
          "outerRight": ""
        },
        {
          "characterID": "WINTERLIS",
          "backgroundID": "christmashaven",
          "dialogueText": "Santa, of course!",
          "emotionID": "happy",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "WINTERLIS",
          "center": "",
          "innerRight": "WINTERMAL",
          "outerRight": ""
        },
        {
          "characterID": "WINTERMAL",
          "backgroundID": "christmashaven",
          "dialogueText": "Don't be silly, Lis, Santa doesn't exist – except in children's stories!",
          "emotionID": "annoy",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "WINTERLIS",
          "center": "",
          "innerRight": "WINTERMAL",
          "outerRight": ""
        },
        {
          "characterID": "WINTERLIS",
          "backgroundID": "christmashaven",
          "dialogueText": "He does so exist!",
          "emotionID": "angry",
          "musicID": "",
          "sfxID": "",
          "animationID": "shake",
          "outerLeft": "",
          "innerLeft": "WINTERLIS",
          "center": "",
          "innerRight": "WINTERMAL",
          "outerRight": ""
        },
        {
          "characterID": "WINTERMAL",
          "backgroundID": "christmashaven",
          "dialogueText": "No he doesn't!",
          "emotionID": "angry",
          "musicID": "",
          "sfxID": "",
          "animationID": "shake",
          "outerLeft": "",
          "innerLeft": "WINTERLIS",
          "center": "",
          "innerRight": "WINTERMAL",
          "outerRight": ""
        },
        {
          "characterID": "WINTERLIS",
          "backgroundID": "christmashaven",
          "dialogueText": "Then where do all the presents come from?",
          "emotionID": "annoy",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "WINTERLIS",
          "center": "",
          "innerRight": "WINTERMAL",
          "outerRight": ""
        },
        {
          "characterID": "WINTERMAL",
          "backgroundID": "christmashaven",
          "dialogueText": "Lis, look around, people are literally carrying presents home from the shops right now.",
          "emotionID": "idle",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "WINTERLIS",
          "center": "",
          "innerRight": "WINTERMAL",
          "outerRight": ""
        },
        {
          "characterID": "WINTERLIS",
          "backgroundID": "christmashaven",
          "dialogueText": "I don't mean presents from friends and relatives.",
          "emotionID": "annoy",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "WINTERLIS",
          "center": "",
          "innerRight": "WINTERMAL",
          "outerRight": ""
        },
        {
          "characterID": "WINTERLIS",
          "backgroundID": "christmashaven",
          "dialogueText": "I mean the presents you get in Christmas stockings!",
          "emotionID": "angry",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "WINTERLIS",
          "center": "",
          "innerRight": "WINTERMAL",
          "outerRight": ""
        },
        {
          "characterID": "WINTERMAL",
          "backgroundID": "christmashaven",
          "dialogueText": "They come from the same place!",
          "emotionID": "annoy",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "WINTERLIS",
          "center": "",
          "innerRight": "WINTERMAL",
          "outerRight": ""
        },
        {
          "characterID": "WINTERLIS",
          "backgroundID": "christmashaven",
          "dialogueText": "Do not!",
          "emotionID": "angry",
          "musicID": "",
          "sfxID": "",
          "animationID": "shake",
          "outerLeft": "",
          "innerLeft": "WINTERLIS",
          "center": "",
          "innerRight": "WINTERMAL",
          "outerRight": ""
        },
        {
          "characterID": "WINTERMAL",
          "backgroundID": "christmashaven",
          "dialogueText": "Do too!",
          "emotionID": "angry",
          "musicID": "",
          "sfxID": "",
          "animationID": "shake",
          "outerLeft": "",
          "innerLeft": "WINTERLIS",
          "center": "",
          "innerRight": "WINTERMAL",
          "outerRight": ""
        },
        {
          "characterID": "",
          "backgroundID": "christmashaven",
          "dialogueText": "You're about to step in, before things get out of hand, when you are surprised by Hina placing a gentle hand on your shoulder.",
          "emotionID": "",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "",
          "center": "HINA",
          "innerRight": "",
          "outerRight": ""
        },
        {
          "characterID": "",
          "backgroundID": "christmashaven",
          "dialogueText": "She smiles at you and shakes her head, before stepping forward and interjecting, herself.",
          "emotionID": "",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "",
          "center": "HINA",
          "innerRight": "",
          "outerRight": ""
        },
        {
          "characterID": "HINA",
          "backgroundID": "christmashaven",
          "dialogueText": "Now now, you two, don't fall out over something so small.",
          "emotionID": "surprise",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "WINTERLIS",
          "innerLeft": "",
          "center": "HINA",
          "innerRight": "",
          "outerRight": "WINTERMAL"
        },
        {
          "characterID": "HINA",
          "backgroundID": "christmashaven",
          "dialogueText": "Especially at Christmas! It's a time for goodwill, after all.",
          "emotionID": "angry",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "WINTERLIS",
          "innerLeft": "",
          "center": "HINA",
          "innerRight": "",
          "outerRight": "WINTERMAL"
        },
        {
          "characterID": "WINTERMAL",
          "backgroundID": "christmashaven",
          "dialogueText": "Ah... yes, Hina.",
          "emotionID": "surprise",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "WINTERLIS",
          "innerLeft": "",
          "center": "HINA",
          "innerRight": "",
          "outerRight": "WINTERMAL"
        },
        {
          "characterID": "WINTERMAL",
          "backgroundID": "christmashaven",
          "dialogueText": "I'm sorry, my sister was just being a bit childish.",
          "emotionID": "annoy",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "WINTERLIS",
          "innerLeft": "",
          "center": "HINA",
          "innerRight": "",
          "outerRight": "WINTERMAL"
        },
        {
          "characterID": "WINTERLIS",
          "backgroundID": "christmashaven",
          "dialogueText": "I wasn't!",
          "emotionID": "annoy",
          "musicID": "",
          "sfxID": "",
          "animationID": "shake",
          "outerLeft": "WINTERLIS",
          "innerLeft": "",
          "center": "HINA",
          "innerRight": "",
          "outerRight": "WINTERMAL"
        },
        {
          "characterID": "WINTERLIS",
          "backgroundID": "christmashaven",
          "dialogueText": "Santa is really real, isn't he, Hina?",
          "emotionID": "sad",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "WINTERLIS",
          "innerLeft": "",
          "center": "HINA",
          "innerRight": "",
          "outerRight": "WINTERMAL"
        },
        {
          "characterID": "HINA",
          "backgroundID": "christmashaven",
          "dialogueText": "Of course Santa is real.",
          "emotionID": "happy",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "WINTERLIS",
          "innerLeft": "",
          "center": "HINA",
          "innerRight": "",
          "outerRight": "WINTERMAL"
        },
        {
          "characterID": "WINTERMAL",
          "backgroundID": "christmashaven",
          "dialogueText": "Eh?!",
          "emotionID": "surprise",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "WINTERLIS",
          "innerLeft": "",
          "center": "HINA",
          "innerRight": "",
          "outerRight": "WINTERMAL"
        },
        {
          "characterID": "WINTERMAL",
          "backgroundID": "christmashaven",
          "dialogueText": "Ah, you're just saying that to make Lis feel better.",
          "emotionID": "idle",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "WINTERLIS",
          "innerLeft": "",
          "center": "HINA",
          "innerRight": "",
          "outerRight": "WINTERMAL"
        },
        {
          "characterID": "HINA",
          "backgroundID": "christmashaven",
          "dialogueText": "Am I?",
          "emotionID": "idle",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "WINTERLIS",
          "innerLeft": "",
          "center": "HINA",
          "innerRight": "",
          "outerRight": "WINTERMAL"
        },
        {
          "characterID": "HINA",
          "backgroundID": "christmashaven",
          "dialogueText": "Would you like to meet him?",
          "emotionID": "happy",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "WINTERLIS",
          "innerLeft": "",
          "center": "HINA",
          "innerRight": "",
          "outerRight": "WINTERMAL"
        },
        {
          "characterID": "WINTERMAL",
          "backgroundID": "christmashaven",
          "dialogueText": "Meet him?",
          "emotionID": "surprise",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "WINTERLIS",
          "innerLeft": "",
          "center": "HINA",
          "innerRight": "",
          "outerRight": "WINTERMAL"
        },
        {
          "characterID": "HINA",
          "backgroundID": "christmashaven",
          "dialogueText": "Yes! He lives in my realm, after all: the Realm of Everlasting Winter.",
          "emotionID": "happy",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "WINTERLIS",
          "innerLeft": "",
          "center": "HINA",
          "innerRight": "",
          "outerRight": "WINTERMAL"
        },
        {
          "characterID": "WINTERMAL",
          "backgroundID": "christmashaven",
          "dialogueText": "Wait? He does?!",
          "emotionID": "surprise",
          "musicID": "",
          "sfxID": "",
          "animationID": "bounce",
          "outerLeft": "WINTERLIS",
          "innerLeft": "",
          "center": "HINA",
          "innerRight": "",
          "outerRight": "WINTERMAL"
        },
        {
          "characterID": "HINA",
          "backgroundID": "christmashaven",
          "dialogueText": "Of course! Where else in Phantasia would he live?",
          "emotionID": "happy",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "WINTERLIS",
          "innerLeft": "",
          "center": "HINA",
          "innerRight": "",
          "outerRight": "WINTERMAL"
        },
        {
          "characterID": "HINA",
          "backgroundID": "christmashaven",
          "dialogueText": "He hasn't been very active recently, but now the Winter Queen has released her grip on the realm, I understand he's ready to make a comeback!",
          "emotionID": "idle",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "WINTERLIS",
          "innerLeft": "",
          "center": "HINA",
          "innerRight": "",
          "outerRight": "WINTERMAL"
        },
        {
          "characterID": "HINA",
          "backgroundID": "christmashaven",
          "dialogueText": "Perhaps you'd like to take a trip to his workshop to see him?",
          "emotionID": "happy",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "WINTERLIS",
          "innerLeft": "",
          "center": "HINA",
          "innerRight": "",
          "outerRight": "WINTERMAL"
        },
        {
          "characterID": "",
          "backgroundID": "christmashaven",
          "dialogueText": "Travel to the Realm of Everlasting Winter? But you've just been there...",
          "emotionID": "",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "WINTERLIS",
          "innerLeft": "",
          "center": "HINA",
          "innerRight": "",
          "outerRight": "WINTERMAL"
        },
        {
          "characterID": "",
          "backgroundID": "christmashaven",
          "dialogueText": "You do technically have some days and weeks before Christmas day, so you could probably make it in time... right?",
          "emotionID": "",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "WINTERLIS",
          "innerLeft": "",
          "center": "HINA",
          "innerRight": "",
          "outerRight": "WINTERMAL"
        },
        {
          "characterID": "WINTERLIS",
          "backgroundID": "christmashaven",
          "dialogueText": "Oh, can we? Can we, please, Dreamer?",
          "emotionID": "shy",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "WINTERLIS",
          "innerLeft": "",
          "center": "HINA",
          "innerRight": "",
          "outerRight": "WINTERMAL"
        },
        {
          "characterID": "HINA",
          "backgroundID": "christmashaven",
          "dialogueText": "Dreamer?",
          "emotionID": "happy",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "WINTERLIS",
          "innerLeft": "",
          "center": "HINA",
          "innerRight": "",
          "outerRight": "WINTERMAL"
        },
        {
          "characterID": "",
          "backgroundID": "christmashaven",
          "dialogueText": "Looking at how excited and hopeful Lis is... you can't really say no.",
          "emotionID": "",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "WINTERLIS",
          "innerLeft": "",
          "center": "HINA",
          "innerRight": "",
          "outerRight": "WINTERMAL"
        },
        {
          "characterID": "WINTERLIS",
          "backgroundID": "christmashaven",
          "dialogueText": "Yay!",
          "emotionID": "happy",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "WINTERLIS",
          "innerLeft": "",
          "center": "HINA",
          "innerRight": "",
          "outerRight": "WINTERMAL"
        },
        {
          "characterID": "WINTERLIS",
          "backgroundID": "christmashaven",
          "dialogueText": "Mal! We're going to see Santa for Christmas!",
          "emotionID": "happy",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "WINTERLIS",
          "innerLeft": "",
          "center": "HINA",
          "innerRight": "",
          "outerRight": "WINTERMAL"
        },
        {
          "characterID": "WINTERMAL",
          "backgroundID": "christmashaven",
          "dialogueText": "Ah! Yeah...",
          "emotionID": "shy",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "WINTERLIS",
          "innerLeft": "",
          "center": "HINA",
          "innerRight": "",
          "outerRight": "WINTERMAL"
        },
        {
          "characterID": "HINA",
          "backgroundID": "christmashaven",
          "dialogueText": "Then it's decided. I'll tell my sisters, Dreamer, you gather Celeste and whoever else you think might like to join our party.",
          "emotionID": "happy",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "WINTERLIS",
          "innerLeft": "",
          "center": "HINA",
          "innerRight": "",
          "outerRight": "WINTERMAL"
        },
        {
          "characterID": "",
          "backgroundID": "christmashaven",
          "dialogueText": "Okay!",
          "emotionID": "",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "WINTERLIS",
          "innerLeft": "",
          "center": "HINA",
          "innerRight": "",
          "outerRight": "WINTERMAL"
        },
        {
          "characterID": "",
          "backgroundID": "christmashaven",
          "dialogueText": "It kind of came out of the blue, but it looks like you're visiting Santa's workshop for Christmas!",
          "emotionID": "",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "WINTERLIS",
          "innerLeft": "",
          "center": "HINA",
          "innerRight": "",
          "outerRight": "WINTERMAL"
        }
      ]
    },
    {
      "languageID": "Korean",
      "dialogueLines": [
        {
          "characterID": "",
          "backgroundID": "christmashaven",
          "dialogueText": "안식의 도시에 첫눈이 내렸다.",
          "emotionID": "",
          "musicID": "holidaycheer",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "",
          "center": "",
          "innerRight": "",
          "outerRight": ""
        },
        {
          "characterID": "",
          "backgroundID": "christmashaven",
          "dialogueText": "주민들이 온갖 가게들을 돌아다니며 크리스마스 선물을 준비하는 모습이 평화로웠다.",
          "emotionID": "",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "",
          "center": "",
          "innerRight": "",
          "outerRight": ""
        },
        {
          "characterID": "",
          "backgroundID": "christmashaven",
          "dialogueText": "우리 일행 중에서도 특히 더 들뜬 사람이 있었는데, 그건...",
          "emotionID": "",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "",
          "center": "",
          "innerRight": "",
          "outerRight": ""
        },
        {
          "characterID": "WINTERLIS",
          "backgroundID": "christmashaven",
          "dialogueText": "오빠, 오빠!",
          "emotionID": "happy",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "WINTERLIS",
          "center": "",
          "innerRight": "WINTERMAL",
          "outerRight": ""
        },
        {
          "characterID": "WINTERLIS",
          "backgroundID": "christmashaven",
          "dialogueText": "곧 있으면 크리스마스야!",
          "emotionID": "surprise",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "WINTERLIS",
          "center": "",
          "innerRight": "WINTERMAL",
          "outerRight": ""
        },
        {
          "characterID": "WINTERMAL",
          "backgroundID": "christmashaven",
          "dialogueText": "벌써 그렇게 됐나?",
          "emotionID": "",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "WINTERLIS",
          "center": "",
          "innerRight": "WINTERMAL",
          "outerRight": ""
        },
        {
          "characterID": "WINTERLIS",
          "backgroundID": "christmashaven",
          "dialogueText": "올해는 산타클로스를 볼 수 있겠지? 산타 할아버지!",
          "emotionID": "happy",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "WINTERLIS",
          "center": "",
          "innerRight": "WINTERMAL",
          "outerRight": ""
        },
        {
          "characterID": "WINTERMAL",
          "backgroundID": "christmashaven",
          "dialogueText": "헹, 애도 아니고 산타를 아직 믿어? 산타클로스는 남들이 꾸며낸 이야기야!",
          "emotionID": "annoy",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "WINTERLIS",
          "center": "",
          "innerRight": "WINTERMAL",
          "outerRight": ""
        },
        {
          "characterID": "WINTERLIS",
          "backgroundID": "christmashaven",
          "dialogueText": "아냐! 산타는 진짜 있다구!",
          "emotionID": "angry",
          "musicID": "",
          "sfxID": "",
          "animationID": "shake",
          "outerLeft": "",
          "innerLeft": "WINTERLIS",
          "center": "",
          "innerRight": "WINTERMAL",
