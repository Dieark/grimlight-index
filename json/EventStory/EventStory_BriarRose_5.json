    },
    "m_Name": "EventStory_BriarRose_5",
    "unskippable": 0,
    "dialogueDataEntries": [
        {
            "languageID": "English",
            "dialogueLines": [
                {
                    "characterID": "",
                    "backgroundID": "dreamrealm",
                    "dialogueText": "The collapsing realm drifts around you, seemingly suspended in the moment of falling into the void.",
                    "emotionID": "",
                    "musicID": "threateningbattle",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "storybriarroseportal3",
                    "dialogueText": "Vita leads you on through the chaotic landscapes and eventually, you come upon the next portal.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Vita",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "storybriarroseportal3",
                    "dialogueText": "It looks very orange beyond... and not in a good way.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Esme",
                    "backgroundID": "dreamrealm",
                    "dialogueText": "Does that look like... fire?",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Esme",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Aurora",
                    "backgroundID": "dreamrealm",
                    "dialogueText": "We can't hesitate after coming this far. Lead us on, Dreamer.",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Aurora",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Esme",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "storybriarroseportal3",
                    "dialogueText": "You nod and step forward.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "storybriarroseportal3",
                    "dialogueText": "The world changes again and suddenly, you find yourself in –",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Esme",
                    "backgroundID": "havenwar",
                    "dialogueText": "This is Haven!",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Vita",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Esme",
                    "backgroundID": "havenwar",
                    "dialogueText": "It's under attack!",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Vita",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Esme",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "havenwar",
                    "dialogueText": "Not only that, it is burning! Townspeople run in all directions and Dreamless creatures are everywhere!",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Vita",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Esme",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "havenwar",
                    "dialogueText": "Though, even though you are in the middle of it, they strangely don't move to intercept you and your party.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Vita",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Esme",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "havenwar",
                    "dialogueText": "Nevertheless, screams of pain and fear punctuate the smoky air, mixing with the roar and crackle of out of control flames and the odd crash of a collapsing building.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Vita",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Esme",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "havenwar",
                    "dialogueText": "The townspeople appear to be defenseless!",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Vita",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Esme",
                    "outerRight": ""
                },
                {
                    "characterID": "Esme",
                    "backgroundID": "havenwar",
                    "dialogueText": "We have to help them!",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Vita",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Esme",
                    "outerRight": ""
                },
                {
                    "characterID": "Aurora",
                    "backgroundID": "havenwar",
                    "dialogueText": "NO!",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Vita",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "Aurora",
                    "backgroundID": "havenwar",
                    "dialogueText": "Don't forget, this is an illusion. We can't be distracted, no matter how terrible things appear.",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Vita",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "Vita",
                    "backgroundID": "havenwar",
                    "dialogueText": "But the people –",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Vita",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "Aurora",
                    "backgroundID": "havenwar",
                    "dialogueText": "Aren't real. We wouldn't be helping anyone by losing sight of why we're here.",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Vita",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "Aurora",
                    "backgroundID": "havenwar",
                    "dialogueText": "Can you still hear Briar, Vita.",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Vita",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "Vita",
                    "backgroundID": "havenwar",
                    "dialogueText": "It's difficult with all this noise... but yes, I think so.",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Vita",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "Aurora",
                    "backgroundID": "havenwar",
                    "dialogueText": "Then lead us on. Hopefully, the sooner we find her, the sooner this nightmare will end.",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Vita",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "",
                    "backgroundID": "havenwar",
                    "dialogueText": "You carry on, trying not to be taken in by the carnage all around you.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Vita",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "",
                    "backgroundID": "havenwar",
                    "dialogueText": "More than once, a towns-person or a Dreamless runs straight through you, passing by like smoke.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Vita",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "",
                    "backgroundID": "havenwar",
                    "dialogueText": "It's such a distracting situation, you almost don't notice when you really do come under attack...",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_NPC_NightmareVassal",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "havenwar",
                    "dialogueText": "The Nightmares have returned!",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_NPC_NightmareVassal",
                    "innerRight": "",
                    "outerRight": ""
                }
            ]
        },
        {
            "languageID": "Korean",
            "dialogueLines": [
                {
                    "characterID": "",
                    "backgroundID": "dreamrealm",
                    "dialogueText": "무너져가는 세계의 모습이 점점 희미해진다.<br>지난번에 마주했던 세계가 공허 속으로 빨려 들어가는 모습과 비슷했다.",
                    "emotionID": "",
                    "musicID": "threateningbattle",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "storybriarroseportal3",
                    "dialogueText": "비타에게 들리는 목소리를 따라<br>우리는 다시 차원문을 넘었고, 또 다른 세계에 도착했다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Vita",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "storybriarroseportal3",
                    "dialogueText": "따스함이라고는 전혀 느껴지지 않는 석양이 진다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "에스메",
                    "backgroundID": "dreamrealm",
                    "dialogueText": "저기, 있잖아.<br>저거... 내 눈에만 불기둥처럼 보여?",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Esme",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "오로라",
                    "backgroundID": "dreamrealm",
                    "dialogueText": "여기까지 왔는데 물러설 수는 없어요.<br>꿈꾸는 자, 부탁할게요.",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Aurora",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Esme",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "storybriarroseportal3",
                    "dialogueText": "고개를 끄덕이며 일행의 앞을 막아선다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "storybriarroseportal3",
                    "dialogueText": "주변 환경이 다시 뒤바뀐다. 그 풍경은 마치...",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "에스메",
                    "backgroundID": "havenwar",
                    "dialogueText": "여기...<br>안식의 도시잖아?",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Vita",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "에스메",
                    "backgroundID": "havenwar",
                    "dialogueText": "뭐야, 이거...?",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Vita",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Esme",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "havenwar",
                    "dialogueText": "도시 전체가 불타고 있었다.<br>도시의 사람들을 습격하고 있는 것은 드림리스다.<br>드림리스가 모든 거리를 가득 메우고 있었다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Vita",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Esme",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "havenwar",
                    "dialogueText": "기이한 점이 있다면<br>절망과 비명으로 가득 찬 도시 한복판에 서있는 우리를...<br>아무도 공격하지 않는다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Vita",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Esme",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "havenwar",
                    "dialogueText": "고통과 두려움에 가득 찬 비명소리가 안식 대신 도시를 채운다.<br>하늘을 뒤덮은 검은 연기, 태양이 떨어진 듯 타오르는 도시의 풍경.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Vita",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Esme",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "havenwar",
                    "dialogueText": "건물이 무너지는 굉음에 사람들의 비명이 묻힌다.<br>안식의 도시는 지금 무방비 상태다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Vita",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Esme",
                    "outerRight": ""
                },
                {
                    "characterID": "에스메",
                    "backgroundID": "havenwar",
                    "dialogueText": "도와주러 가자, 꿈꾸는 자!",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Vita",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Esme",
                    "outerRight": ""
                },
                {
                    "characterID": "오로라",
                    "backgroundID": "havenwar",
                    "dialogueText": "에스메!",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Vita",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "오로라",
                    "backgroundID": "havenwar",
                    "dialogueText": "이 모든 건 그저 환상일 뿐이에요.<br>환상에 속으면 안 돼요!",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Vita",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "비타",
                    "backgroundID": "havenwar",
                    "dialogueText": "하, 하지만... 하지만 사람들이!",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Vita",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "오로라",
                    "backgroundID": "havenwar",
                    "dialogueText": "진짜가 아니에요. 냉정해지세요.<br>저희는 저 환상을 구하러 온 것이 아니잖아요.",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Vita",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "오로라",
                    "backgroundID": "havenwar",
                    "dialogueText": "비타, 브라이어 로즈의 목소리는요?",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Vita",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "비타",
                    "backgroundID": "havenwar",
                    "dialogueText": "너무 시끄러워서 잘 안 들려...<br>그, 그래도 여전히 부르고 있는 것 같아.",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Vita",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "오로라",
                    "backgroundID": "havenwar",
                    "dialogueText": "그럼 계속 안내해주세요.<br>그녀를 찾으면 이 모든 악몽도 전부 끝날 거예요.",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Vita",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "",
                    "backgroundID": "havenwar",
                    "dialogueText": "도시의 참상을 뒤로하고 애써 앞으로 나아간다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Vita",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "",
                    "backgroundID": "havenwar",
                    "dialogueText": "사람들과 드림리스의 환상이 연기처럼 우리를 통과해 지나간다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Vita",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
