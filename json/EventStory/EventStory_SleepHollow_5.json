  "dialogueDataEntries": [
    {
      "languageID": "English",
      "dialogueLines": [
        {
          "characterID": "",
          "backgroundID": "burningwoods",
          "dialogueText": "In the forests nearby, the shady trio are making steady progress when, all of a sudden, Frankenstein stops them.",
          "emotionID": "",
          "musicID": "threateningbattle",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "GEPPETTO",
          "innerLeft": "",
          "center": "FRANKENSTEIN",
          "innerRight": "",
          "outerRight": "BATHORY"
        },
        {
          "characterID": "FRANKENSTEIN",
          "backgroundID": "burningwoods",
          "dialogueText": "Listen!",
          "emotionID": "surprise",
          "musicID": "",
          "sfxID": "bounce",
          "animationID": "",
          "outerLeft": "GEPPETTO",
          "innerLeft": "",
          "center": "FRANKENSTEIN",
          "innerRight": "",
          "outerRight": "BATHORY"
        },
        {
          "characterID": "GEPPETTO",
          "backgroundID": "burningwoods",
          "dialogueText": "...",
          "emotionID": "surprise",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "GEPPETTO",
          "innerLeft": "",
          "center": "FRANKENSTEIN",
          "innerRight": "",
          "outerRight": "BATHORY"
        },
        {
          "characterID": "GEPPETTO",
          "backgroundID": "burningwoods",
          "dialogueText": "What am I listening to?",
          "emotionID": "annoy",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "GEPPETTO",
          "innerLeft": "",
          "center": "FRANKENSTEIN",
          "innerRight": "",
          "outerRight": "BATHORY"
        },
        {
          "characterID": "FRANKENSTEIN",
          "backgroundID": "burningwoods",
          "dialogueText": "Don't you hear it? Fighting!",
          "emotionID": "surprise",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "GEPPETTO",
          "innerLeft": "",
          "center": "FRANKENSTEIN",
          "innerRight": "",
          "outerRight": "BATHORY"
        },
        {
          "characterID": "FRANKENSTEIN",
          "backgroundID": "burningwoods",
          "dialogueText": "There!",
          "emotionID": "surprise",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "GEPPETTO",
          "innerLeft": "",
          "center": "FRANKENSTEIN",
          "innerRight": "",
          "outerRight": "BATHORY"
        },
        {
          "characterID": "GEPPETTO",
          "backgroundID": "burningwoods",
          "dialogueText": "I can't hear much over Bob's growling and the moaning of your zombies...",
          "emotionID": "annoy",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "GEPPETTO",
          "innerLeft": "",
          "center": "FRANKENSTEIN",
          "innerRight": "",
          "outerRight": "BATHORY"
        },
        {
          "characterID": "BATHORY",
          "backgroundID": "burningwoods",
          "dialogueText": "No, she's right, I hear it too...",
          "emotionID": "idle",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "GEPPETTO",
          "innerLeft": "",
          "center": "FRANKENSTEIN",
          "innerRight": "",
          "outerRight": "BATHORY"
        },
        {
          "characterID": "GEPPETTO",
          "backgroundID": "burningwoods",
          "dialogueText": "...",
          "emotionID": "idle",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "GEPPETTO",
          "innerLeft": "",
          "center": "FRANKENSTEIN",
          "innerRight": "",
          "outerRight": "BATHORY"
        },
        {
          "characterID": "GEPPETTO",
          "backgroundID": "burningwoods",
          "dialogueText": "Okay, so do I.",
          "emotionID": "annoy",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "GEPPETTO",
          "innerLeft": "",
          "center": "FRANKENSTEIN",
          "innerRight": "",
          "outerRight": "BATHORY"
        },
        {
          "characterID": "GEPPETTO",
          "backgroundID": "burningwoods",
          "dialogueText": "It must be the Horseman engaging with someone or something.",
          "emotionID": "annoy",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "GEPPETTO",
          "innerLeft": "",
          "center": "FRANKENSTEIN",
          "innerRight": "",
          "outerRight": "BATHORY"
        },
        {
          "characterID": "FRANKENSTEIN",
          "backgroundID": "burningwoods",
          "dialogueText": "Oh, I can guess exactly who.",
          "emotionID": "happy",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "GEPPETTO",
          "innerLeft": "",
          "center": "FRANKENSTEIN",
          "innerRight": "",
          "outerRight": "BATHORY"
        },
        {
          "characterID": "GEPPETTO",
          "backgroundID": "burningwoods",
          "dialogueText": "The Dreamer?",
          "emotionID": "idle",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "GEPPETTO",
          "innerLeft": "",
          "center": "FRANKENSTEIN",
          "innerRight": "",
          "outerRight": "BATHORY"
        },
        {
          "characterID": "BATHORY",
          "backgroundID": "burningwoods",
          "dialogueText": "Who else would be out here? Defending pitiful villagers, no doubt!",
          "emotionID": "annoy",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "GEPPETTO",
          "innerLeft": "",
          "center": "FRANKENSTEIN",
          "innerRight": "",
          "outerRight": "BATHORY"
        },
        {
          "characterID": "GEPPETTO",
          "backgroundID": "burningwoods",
          "dialogueText": "What do we do? The Dreamer complicates everything! What if he defeats the Horseman before we can capture him?",
          "emotionID": "idle",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "GEPPETTO",
          "innerLeft": "",
          "center": "FRANKENSTEIN",
          "innerRight": "",
          "outerRight": "BATHORY"
        },
        {
          "characterID": "GEPPETTO",
          "backgroundID": "burningwoods",
          "dialogueText": "What if the Horseman takes the Dreamer's head as his prize and returns to his hiding place, somewhere we can't follow?",
          "emotionID": "idle",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "GEPPETTO",
          "innerLeft": "",
          "center": "FRANKENSTEIN",
          "innerRight": "",
          "outerRight": "BATHORY"
        },
        {
          "characterID": "FRANKENSTEIN",
          "backgroundID": "burningwoods",
          "dialogueText": "Then we had better get there quickly, before this opportunity passes us by!",
          "emotionID": "angry",
          "musicID": "",
          "sfxID": "bounce",
          "animationID": "",
          "outerLeft": "GEPPETTO",
          "innerLeft": "",
          "center": "FRANKENSTEIN",
          "innerRight": "",
          "outerRight": "BATHORY"
        },
        {
          "characterID": "FRANKENSTEIN",
          "backgroundID": "burningwoods",
          "dialogueText": "It's a good job I predicted this would happen and factored it into my calculations!",
          "emotionID": "happy",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "GEPPETTO",
          "innerLeft": "",
          "center": "FRANKENSTEIN",
          "innerRight": "",
          "outerRight": "BATHORY"
        }
      ]
    },
    {
      "languageID": "Korean",
      "dialogueLines": [
        {
          "characterID": "",
          "backgroundID": "burningwoods",
          "dialogueText": "인상을 찡그리고 숲을 바라보던 프랑켄슈타인이 손을 뻗어 길을 막는다.",
          "emotionID": "",
          "musicID": "threateningbattle",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "GEPPETTO",
          "innerLeft": "",
          "center": "FRANKENSTEIN",
          "innerRight": "",
          "outerRight": "BATHORY"
        },
        {
          "characterID": "FRANKENSTEIN",
          "backgroundID": "burningwoods",
          "dialogueText": "잠깐! 이게 무슨 소리지?",
          "emotionID": "surprise",
          "musicID": "",
          "sfxID": "bounce",
          "animationID": "",
          "outerLeft": "GEPPETTO",
          "innerLeft": "",
          "center": "FRANKENSTEIN",
          "innerRight": "",
          "outerRight": "BATHORY"
        },
        {
          "characterID": "GEPPETTO",
          "backgroundID": "burningwoods",
          "dialogueText": "...",
          "emotionID": "surprise",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "GEPPETTO",
          "innerLeft": "",
          "center": "FRANKENSTEIN",
          "innerRight": "",
          "outerRight": "BATHORY"
        },
        {
          "characterID": "GEPPETTO",
          "backgroundID": "burningwoods",
          "dialogueText": "무슨 소리?",
          "emotionID": "annoy",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "GEPPETTO",
          "innerLeft": "",
          "center": "FRANKENSTEIN",
          "innerRight": "",
          "outerRight": "BATHORY"
        },
        {
          "characterID": "FRANKENSTEIN",
          "backgroundID": "burningwoods",
          "dialogueText": "저 소리 안 들려?",
          "emotionID": "surprise",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "GEPPETTO",
          "innerLeft": "",
          "center": "FRANKENSTEIN",
          "innerRight": "",
          "outerRight": "BATHORY"
        },
        {
          "characterID": "FRANKENSTEIN",
          "backgroundID": "burningwoods",
          "dialogueText": "누가 싸우고 있는 것 같은데...",
          "emotionID": "surprise",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "GEPPETTO",
          "innerLeft": "",
          "center": "FRANKENSTEIN",
          "innerRight": "",
          "outerRight": "BATHORY"
        },
        {
          "characterID": "GEPPETTO",
          "backgroundID": "burningwoods",
          "dialogueText": "좀비랑 밥이 싸우고 있긴 하지.",
          "emotionID": "annoy",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "GEPPETTO",
          "innerLeft": "",
          "center": "FRANKENSTEIN",
          "innerRight": "",
          "outerRight": "BATHORY"
        },
        {
          "characterID": "BATHORY",
          "backgroundID": "burningwoods",
          "dialogueText": "아뇨, 그보다 조금 더 먼 소리에요. 숲 건너편 같은데요?",
          "emotionID": "idle",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "GEPPETTO",
          "innerLeft": "",
          "center": "FRANKENSTEIN",
          "innerRight": "",
          "outerRight": "BATHORY"
        },
        {
          "characterID": "GEPPETTO",
          "backgroundID": "burningwoods",
          "dialogueText": "너희 말이야. 보통 사람은 숲 건너편의 소리를 못 들어.",
          "emotionID": "idle",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "GEPPETTO",
          "innerLeft": "",
          "center": "FRANKENSTEIN",
          "innerRight": "",
          "outerRight": "BATHORY"
        },
        {
          "characterID": "GEPPETTO",
          "backgroundID": "burningwoods",
          "dialogueText": "몰랐으면 지금이라도 알아두길 바랄게.",
          "emotionID": "annoy",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "GEPPETTO",
          "innerLeft": "",
          "center": "FRANKENSTEIN",
          "innerRight": "",
          "outerRight": "BATHORY"
        },
        {
          "characterID": "GEPPETTO",
          "backgroundID": "burningwoods",
          "dialogueText": "하나는 목 없는 기사 같은데, 상대는... 난 처음 듣는 소리야.",
          "emotionID": "annoy",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "GEPPETTO",
          "innerLeft": "",
          "center": "FRANKENSTEIN",
          "innerRight": "",
          "outerRight": "BATHORY"
        },
        {
          "characterID": "FRANKENSTEIN",
          "backgroundID": "burningwoods",
          "dialogueText": "으음, 누군지 알 것 같기도하고.",
          "emotionID": "happy",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "GEPPETTO",
          "innerLeft": "",
          "center": "FRANKENSTEIN",
          "innerRight": "",
          "outerRight": "BATHORY"
        },
        {
          "characterID": "GEPPETTO",
          "backgroundID": "burningwoods",
          "dialogueText": "꿈꾸는 자?",
          "emotionID": "idle",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "GEPPETTO",
          "innerLeft": "",
          "center": "FRANKENSTEIN",
          "innerRight": "",
          "outerRight": "BATHORY"
        },
        {
          "characterID": "BATHORY",
          "backgroundID": "burningwoods",
          "dialogueText": "하! 그자 말고 누가 감히 제 앞을 가로막겠어요?",
          "emotionID": "annoy",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "GEPPETTO",
          "innerLeft": "",
          "center": "FRANKENSTEIN",
          "innerRight": "",
          "outerRight": "BATHORY"
        },
        {
          "characterID": "GEPPETTO",
          "backgroundID": "burningwoods",
          "dialogueText": "프랑켄슈타인, 어떡할 거야?",
          "emotionID": "idle",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "GEPPETTO",
          "innerLeft": "",
          "center": "FRANKENSTEIN",
          "innerRight": "",
          "outerRight": "BATHORY"
        },
        {
          "characterID": "GEPPETTO",
          "backgroundID": "burningwoods",
          "dialogueText": "꿈꾸는 자가 목 없는 기사를 쓰러트릴지도 몰라. 그 반대의 경우도 성립될 수 있고.",
          "emotionID": "idle",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "GEPPETTO",
          "innerLeft": "",
          "center": "FRANKENSTEIN",
          "innerRight": "",
          "outerRight": "BATHORY"
        },
        {
          "characterID": "FRANKENSTEIN",
          "backgroundID": "burningwoods",
          "dialogueText": "우하학! 걱정하지 마, 어차피 전부 예측했으니까.",
          "emotionID": "angry",
          "musicID": "",
          "sfxID": "bounce",
          "animationID": "",
          "outerLeft": "GEPPETTO",
          "innerLeft": "",
          "center": "FRANKENSTEIN",
          "innerRight": "",
          "outerRight": "BATHORY"
        },
        {
          "characterID": "FRANKENSTEIN",
          "backgroundID": "burningwoods",
          "dialogueText": "... 그래도 혹시 모르니 서두르자. 알겠지? 혹시 모르니까!",
          "emotionID": "happy",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "GEPPETTO",
          "innerLeft": "",
          "center": "FRANKENSTEIN",
          "innerRight": "",
          "outerRight": "BATHORY"
        }
      ]
    }
  ]
}