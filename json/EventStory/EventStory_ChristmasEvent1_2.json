    "m_Name": "EventStory_ChristmasEvent1_2",
    "unskippable": 0,
    "dialogueDataEntries": [
        {
            "languageID": "English",
            "dialogueLines": [
                {
                    "characterID": "",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "Out in the wilds of the winter woodlands, a disgruntled female voice is raised against the quiet.",
                    "emotionID": "",
                    "musicID": "haunted",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "A young looking girl with large curving horns and a bad tempered pout is complaining about something.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Krampus",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Krampus",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "I hate Christmas!",
                    "emotionID": "annoy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Krampus",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "She is complaining about Christmas to the only compatriot she has at her side.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Krampus",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Big Bad Wolf",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "Awoo... how come, Krampus?",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Krampus",
                    "center": "",
                    "innerRight": "Fullbody_Hero_BigBadWolf",
                    "outerRight": ""
                },
                {
                    "characterID": "Big Bad Wolf",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "Isn't it the time of year everyone is happiest?",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Krampus",
                    "center": "",
                    "innerRight": "Fullbody_Hero_BigBadWolf",
                    "outerRight": ""
                },
                {
                    "characterID": "Krampus",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "Exactly! Everyone's so cheerful all the time, even though it's so cold and it's always dark! It's so annoying!",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Krampus",
                    "center": "",
                    "innerRight": "Fullbody_Hero_BigBadWolf",
                    "outerRight": ""
                },
                {
                    "characterID": "Krampus",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "They're not scared and shivering like they should be!",
                    "emotionID": "annoy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Krampus",
                    "center": "",
                    "innerRight": "Fullbody_Hero_BigBadWolf",
                    "outerRight": ""
                },
                {
                    "characterID": "Krampus",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "It's all that stupid Santa Claus's fault!",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Krampus",
                    "center": "",
                    "innerRight": "Fullbody_Hero_BigBadWolf",
                    "outerRight": ""
                },
                {
                    "characterID": "Big Bad Wolf",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "Didn't you used to work for him at his grotto? I thought you had the job of giving coal to all the naughty kids?",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Krampus",
                    "center": "",
                    "innerRight": "Fullbody_Hero_BigBadWolf",
                    "outerRight": ""
                },
                {
                    "characterID": "Krampus",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "Yes, but Santa kicked me out for giving out TOO MUCH coal!",
                    "emotionID": "annoy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Krampus",
                    "center": "",
                    "innerRight": "Fullbody_Hero_BigBadWolf",
                    "outerRight": ""
                },
                {
                    "characterID": "Krampus",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "As if I could give out too much coal to all those annoying, cheerful people!",
                    "emotionID": "annoy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Krampus",
                    "center": "",
                    "innerRight": "Fullbody_Hero_BigBadWolf",
                    "outerRight": ""
                },
                {
                    "characterID": "Krampus",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "Grrrr! I'd just love to give them ALL coal!",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Krampus",
                    "center": "",
                    "innerRight": "Fullbody_Hero_BigBadWolf",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "It's at that moment that a cultured and silkily seductive voice calls from out of the trees.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Bathory",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "You know, if you really want to spoil Christmas for everyone, I might have just the man to talk to.",
                    "emotionID": "happy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Krampus",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "Wha-?!",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Krampus",
                    "center": "",
                    "innerRight": "Fullbody_Hero_BigBadWolf",
                    "outerRight": ""
                },
                {
                    "characterID": "Krampus",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "Who's there!",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Krampus",
                    "center": "",
                    "innerRight": "Fullbody_Hero_BigBadWolf",
                    "outerRight": ""
                },
                {
                    "characterID": "Bathory",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "Who do you think, Krampus?",
                    "emotionID": "happy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Big Bad Wolf",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "Aah! The voice is coming from over there! I'm sure!",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Krampus",
                    "center": "",
                    "innerRight": "Fullbody_Hero_BigBadWolf",
                    "outerRight": ""
                },
                {
                    "characterID": "Bathory",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "Ahem... no, I'm here, actually.",
                    "emotionID": "annoy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Bathory",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Krampus",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_BigBadWolf"
                },
                {
                    "characterID": "Big Bad Wolf",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "Ahhh!",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Bathory",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Krampus",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_BigBadWolf"
                },
                {
                    "characterID": "Krampus",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "Lady Bathory! How did you get behind us?!",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Bathory",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Krampus",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_BigBadWolf"
                },
                {
                    "characterID": "Bathory",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "I was here all along. To be honest, I wasn't exactly being subtle about it.",
                    "emotionID": "annoy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Bathory",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Krampus",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_BigBadWolf"
                },
                {
                    "characterID": "Big Bad Wolf",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "It's magic, I'm sure! She's a witch, definitely! Awoo!",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Bathory",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Krampus",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_BigBadWolf"
                },
                {
                    "characterID": "Big Bad Wolf",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "Go away, witch! Halloween was months ago!",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "bounce",
                    "outerLeft": "Fullbody_Hero_Bathory",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Krampus",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_BigBadWolf"
                },
                {
                    "characterID": "Bathory",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "<i>Sigh...</i>",
                    "emotionID": "annoy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Bathory",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Krampus",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_BigBadWolf"
                },
                {
                    "characterID": "Bathory",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "Look, will you two just listen? If you truly want to ruin Christmas, there's only one target you should be thinking about.",
                    "emotionID": "annoy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Bathory",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Krampus",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_BigBadWolf"
                },
                {
                    "characterID": "Bathory",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "The one who brings all the Christmas cheer in the first place.",
                    "emotionID": "happy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Bathory",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Krampus",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_BigBadWolf"
                },
                {
                    "characterID": "Krampus",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "You mean... Santa?",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Bathory",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Krampus",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_BigBadWolf"
                },
                {
                    "characterID": "Krampus",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "I don't know... he's pretty scary!",
                    "emotionID": "pout",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Bathory",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Krampus",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_BigBadWolf"
                },
                {
                    "characterID": "Bathory",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "No, not Santa himself... but his workshop should do quite nicely!",
                    "emotionID": "happy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Bathory",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Krampus",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_BigBadWolf"
                },
                {
                    "characterID": "Bathory",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "If you blow that up, or something, that will stop him delivering all his presents and Christmas cheer, don't you think?",
                    "emotionID": "happy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Bathory",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Krampus",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_BigBadWolf"
                },
                {
                    "characterID": "Krampus",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "Destroy Santa's workshop?",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Bathory",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Krampus",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_BigBadWolf"
                },
                {
                    "characterID": "Krampus",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "I-I don't know... it would be really difficult to do with just the three of us. It's pretty well protected.",
                    "emotionID": "pout",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Bathory",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Krampus",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_BigBadWolf"
                },
                {
                    "characterID": "Big Bad Wolf",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "Yeah, we can't just march in there and wreck the place!",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Bathory",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Krampus",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_BigBadWolf"
                },
                {
                    "characterID": "Bathory",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "Oh no, no. For that you'd definitely need an army.",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Bathory",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Krampus",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_BigBadWolf"
                },
                {
                    "characterID": "Bathory",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "And I just so happen to have one going spare, plus someone to lead it.",
                    "emotionID": "happy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Bathory",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Krampus",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_BigBadWolf"
                },
                {
                    "characterID": "Bathory",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "All that person needs is the location of the workshop and how to get there.",
                    "emotionID": "happy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Bathory",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Krampus",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_BigBadWolf"
                },
                {
                    "characterID": "Bathory",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "Now, who would have something like that?",
                    "emotionID": "happy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Bathory",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Krampus",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_BigBadWolf"
                },
                {
                    "characterID": "Krampus",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "Oh! I do!",
                    "emotionID": "conspire",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "bounce",
                    "outerLeft": "Fullbody_Hero_Bathory",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Krampus",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_BigBadWolf"
                },
                {
                    "characterID": "",
                    "backgroundID": "none",
                    "dialogueText": "And so, a nefarious Christmas plan is hatched.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                }
            ]
        },
        {
            "languageID": "Korean",
            "dialogueLines": [
                {
                    "characterID": "",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "높은 나무 위에서 눈이 우수수 떨어진다.",
                    "emotionID": "",
                    "musicID": "haunted",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "뿔에 쌓인 눈을 손으로 털어낸 소녀가 불만스레 입술을 삐죽였다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Krampus",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "크람푸스",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "난 크리스마스가 정말 싫어!",
                    "emotionID": "annoy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Krampus",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "위에서 떨어진 눈을 뒤집어쓴 여자가 나무 위를 바라본다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Krampus",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "나쁜 늑대",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "으으음...  크람푸스야",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Krampus",
                    "center": "",
                    "innerRight": "Fullbody_Hero_BigBadWolf",
                    "outerRight": ""
                },
                {
                    "characterID": "나쁜 늑대",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "너 크리스마스라면 사족을 못 쓰지 않았니?",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Krampus",
                    "center": "",
                    "innerRight": "Fullbody_Hero_BigBadWolf",
                    "outerRight": ""
                },
                {
                    "characterID": "크람푸스",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "아니! 절대 싫어! 겨울 같은 건 질색이야, 날씨도 춥고 해도 금방 지고!",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Krampus",
                    "center": "",
                    "innerRight": "Fullbody_Hero_BigBadWolf",
                    "outerRight": ""
                },
                {
                    "characterID": "크람푸스",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "해가 금방 지면 모여서 유령 이야기나 할 것이지, 나만 빼고 다 행복해 보이잖아!",
                    "emotionID": "annoy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Krampus",
                    "center": "",
                    "innerRight": "Fullbody_Hero_BigBadWolf",
                    "outerRight": ""
                },
                {
                    "characterID": "크람푸스",
                    "backgroundID": "snowyforestnight",
                    "dialogueText": "이게 다 산타클로스 때문이야, 흥! 크리스마스가 뭐라고 다들 들떠서는!",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
