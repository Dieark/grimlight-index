  "m_Name": "EventStory_SleepHollow_2",
  "unskippable": 0,
  "dialogueDataEntries": [
    {
      "languageID": "English",
      "dialogueLines": [
        {
          "characterID": "",
          "backgroundID": "redmoon",
          "dialogueText": "A blood moon hangs over Haven, turning the sky crimson.",
          "emotionID": "",
          "musicID": "haunted",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "",
          "center": "",
          "innerRight": "",
          "outerRight": ""
        },
        {
          "characterID": "",
          "backgroundID": "redmoon",
          "dialogueText": "Everyone who sees the shining red disc shivers and hurries into their homes, bolting their doors and barring their windows.",
          "emotionID": "",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "",
          "center": "",
          "innerRight": "",
          "outerRight": ""
        },
        {
          "characterID": "",
          "backgroundID": "havenrednight",
          "dialogueText": "No one feels safe outdoors...",
          "emotionID": "",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "",
          "center": "",
          "innerRight": "",
          "outerRight": ""
        },
        {
          "characterID": "",
          "backgroundID": "havenrednight",
          "dialogueText": "The ominous air is even affecting your companions.",
          "emotionID": "",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "",
          "center": "",
          "innerRight": "",
          "outerRight": ""
        },
        {
          "characterID": "VITA",
          "backgroundID": "havenrednight",
          "dialogueText": "It's a bad omen! I'm sure of it!",
          "emotionID": "annoy",
          "musicID": "",
          "sfxID": "",
          "animationID": "bounce",
          "outerLeft": "",
          "innerLeft": "",
          "center": "VITA",
          "innerRight": "",
          "outerRight": ""
        },
        {
          "characterID": "ESME",
          "backgroundID": "havenrednight",
          "dialogueText": "Omens aren't real, Vita.",
          "emotionID": "annoy",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "VITA",
          "center": "",
          "innerRight": "ESME",
          "outerRight": ""
        },
        {
          "characterID": "VITA",
          "backgroundID": "havenrednight",
          "dialogueText": "Are so! Isn't that right, big sis?",
          "emotionID": "surprise",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "VITA",
          "center": "",
          "innerRight": "ESME",
          "outerRight": ""
        },
        {
          "characterID": "AURORA",
          "backgroundID": "havenrednight",
          "dialogueText": "I don't believe in such things, normally, but...",
          "emotionID": "sad",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "VITA",
          "innerLeft": "",
          "center": "AURORA",
          "innerRight": "",
          "outerRight": "ESME"
        },
        {
          "characterID": "AURORA",
          "backgroundID": "havenrednight",
          "dialogueText": "Dreamer, I have a bad feeling that I can't shake. Something is wrong in Phantasia.",
          "emotionID": "surprise",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "VITA",
          "innerLeft": "",
          "center": "AURORA",
          "innerRight": "",
          "outerRight": "ESME"
        },
        {
          "characterID": "",
          "backgroundID": "havenrednight",
          "dialogueText": "At that moment, Red bursts in on you.",
          "emotionID": "",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "",
          "center": "LITTLERED",
          "innerRight": "",
          "outerRight": ""
        },
        {
          "characterID": "LITTLERED",
          "backgroundID": "havenrednight",
          "dialogueText": "Dreamer! The Horseman! The Headless Horseman is abroad!",
          "emotionID": "surprise",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "",
          "center": "LITTLERED",
          "innerRight": "",
          "outerRight": ""
        },
        {
          "characterID": "",
          "backgroundID": "havenrednight",
          "dialogueText": "The what now?",
          "emotionID": "",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "",
          "center": "LITTLERED",
          "innerRight": "",
          "outerRight": ""
        },
        {
          "characterID": "AURORA",
          "backgroundID": "havenrednight",
          "dialogueText": "It can't be! I thought he was just a legend?",
          "emotionID": "surprise",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "VITA",
          "innerLeft": "",
          "center": "AURORA",
          "innerRight": "",
          "outerRight": "ESME"
        },
        {
          "characterID": "LITTLERED",
          "backgroundID": "havenrednight",
          "dialogueText": "I'm sure of it! There are reports coming back from the patrols, they're saying that settlers have seen him outside Haven!",
          "emotionID": "angry",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "",
          "center": "LITTLERED",
          "innerRight": "",
          "outerRight": ""
        },
        {
          "characterID": "",
          "backgroundID": "havenrednight",
          "dialogueText": "Who is this Headless Horseman?",
          "emotionID": "",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "",
          "center": "LITTLERED",
          "innerRight": "",
          "outerRight": ""
        },
        {
          "characterID": "VITA",
          "backgroundID": "havenrednight",
          "dialogueText": "The omen! The omen I was talking about!",
          "emotionID": "surprise",
          "musicID": "",
          "sfxID": "",
          "animationID": "bounce",
          "outerLeft": "VITA",
          "innerLeft": "",
          "center": "AURORA",
          "innerRight": "",
          "outerRight": "ESME"
        },
        {
          "characterID": "ESME",
          "backgroundID": "havenrednight",
          "dialogueText": "It's nonsense! It must be a prank...",
          "emotionID": "annoy",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "VITA",
          "innerLeft": "",
          "center": "AURORA",
          "innerRight": "",
          "outerRight": "ESME"
        },
        {
          "characterID": "LITTLERED",
          "backgroundID": "havenrednight",
          "dialogueText": "The Headless Horseman is a legendary monster who appears once every ten years on the night of a blood moon – a night like tonight!",
          "emotionID": "idle",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "",
          "center": "LITTLERED",
          "innerRight": "",
          "outerRight": ""
        },
        {
          "characterID": "LITTLERED",
          "backgroundID": "havenrednight",
          "dialogueText": "He rides across the land searching for his missing head.",
          "emotionID": "",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "",
          "center": "LITTLERED",
          "innerRight": "",
          "outerRight": ""
        },
        {
          "characterID": "ESME",
          "backgroundID": "havenrednight",
          "dialogueText": "Like I said, it's a legend, it's not real!",
          "emotionID": "angry",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "LITTLERED",
          "center": "",
          "innerRight": "ESME",
          "outerRight": ""
        },
        {
          "characterID": "LITTLERED",
          "backgroundID": "havenrednight",
          "dialogueText": "Only... he isn't just a legend!",
          "emotionID": "",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "LITTLERED",
          "center": "",
          "innerRight": "ESME",
          "outerRight": ""
        },
        {
          "characterID": "AURORA",
          "backgroundID": "havenrednight",
          "dialogueText": "How do you know, Red?",
          "emotionID": "surprise",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "AURORA",
          "innerLeft": "",
          "center": "LITTLERED",
          "innerRight": "",
          "outerRight": "ESME"
        },
        {
          "characterID": "LITTLERED",
          "backgroundID": "havenrednight",
          "dialogueText": "Because I've seen him myself. Ten years ago...",
          "emotionID": "",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "AURORA",
          "innerLeft": "",
          "center": "LITTLERED",
          "innerRight": "",
          "outerRight": "ESME"
        },
        {
          "characterID": "ESME",
          "backgroundID": "havenrednight",
          "dialogueText": "You've seen him?!",
          "emotionID": "surprise",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "AURORA",
          "innerLeft": "",
          "center": "LITTLERED",
          "innerRight": "",
          "outerRight": "ESME"
        },
        {
          "characterID": "LITTLERED",
          "backgroundID": "havenrednight",
          "dialogueText": "Not only that, but I fought him.",
          "emotionID": "annoy",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "AURORA",
          "innerLeft": "",
          "center": "LITTLERED",
          "innerRight": "",
          "outerRight": "ESME"
        },
        {
          "characterID": "ESME",
          "backgroundID": "havenrednight",
          "dialogueText": "Then how is that possible? I thought the Headless Horseman was supposed to take the head of their foe as a trophy!",
          "emotionID": "surprise",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "AURORA",
          "innerLeft": "",
          "center": "LITTLERED",
          "innerRight": "",
          "outerRight": "ESME"
        },
        {
          "characterID": "LITTLERED",
          "backgroundID": "havenrednight",
          "dialogueText": "What can I tell you? I barely escaped with it still on my shoulders!",
          "emotionID": "sad",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "AURORA",
          "innerLeft": "",
          "center": "LITTLERED",
          "innerRight": "",
          "outerRight": "ESME"
        },
        {
          "characterID": "LITTLERED",
          "backgroundID": "havenrednight",
          "dialogueText": "It was a long battle, we fought each other to a standstill. Then he just... left. I can't explain it.",
          "emotionID": "sad",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "AURORA",
          "innerLeft": "",
          "center": "LITTLERED",
          "innerRight": "",
          "outerRight": "ESME"
        },
        {
          "characterID": "",
          "backgroundID": "havenrednight",
          "dialogueText": "That sounds very strange.",
          "emotionID": "",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "AURORA",
          "innerLeft": "",
          "center": "LITTLERED",
          "innerRight": "",
          "outerRight": "ESME"
        },
        {
          "characterID": "LITTLERED",
          "backgroundID": "havenrednight",
          "dialogueText": "Dreamer, the Horseman is a force of nature, implacable and relentless. I have no idea why he left me alone.",
          "emotionID": "idle",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "AURORA",
          "innerLeft": "",
          "center": "LITTLERED",
          "innerRight": "",
          "outerRight": "ESME"
        },
        {
          "characterID": "LITTLERED",
          "backgroundID": "havenrednight",
          "dialogueText": "My sword was the only thing that saved me, but even then I wasn't able to inflict significant damage on him.",
          "emotionID": "",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "AURORA",
          "innerLeft": "",
          "center": "LITTLERED",
          "innerRight": "",
          "outerRight": "ESME"
        },
        {
          "characterID": "LITTLERED",
          "backgroundID": "havenrednight",
          "dialogueText": "I've never heard of him stopping in his attack before. He always claims a head before he disappears again.",
          "emotionID": "",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "AURORA",
          "innerLeft": "",
          "center": "LITTLERED",
          "innerRight": "",
          "outerRight": "ESME"
        },
        {
          "characterID": "",
          "backgroundID": "havenrednight",
          "dialogueText": "Aurora looks worried.",
          "emotionID": "",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "AURORA",
          "innerLeft": "",
          "center": "LITTLERED",
          "innerRight": "",
          "outerRight": "ESME"
        },
        {
          "characterID": "AURORA",
          "backgroundID": "havenrednight",
          "dialogueText": "Dreamer, if the Horseman is out in the countryside around Haven, then there will be people in danger.",
          "emotionID": "sad",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "AURORA",
          "innerLeft": "",
          "center": "LITTLERED",
          "innerRight": "",
          "outerRight": "ESME"
        },
        {
          "characterID": "AURORA",
          "backgroundID": "havenrednight",
          "dialogueText": "Since you came, the surrounding lands have become much safer, so much so that some people from Haven have left to attempt to resettle those lands.",
          "emotionID": "sad",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "AURORA",
          "innerLeft": "",
          "center": "LITTLERED",
          "innerRight": "",
          "outerRight": "ESME"
        },
        {
          "characterID": "LITTLERED",
          "backgroundID": "havenrednight",
          "dialogueText": "That's where these reports come from. There's a settlement called Settler's Hollow, many of the people there have seen the Horseman.",
          "emotionID": "",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "AURORA",
          "innerLeft": "",
          "center": "LITTLERED",
          "innerRight": "",
          "outerRight": "ESME"
        },
        {
          "characterID": "",
          "backgroundID": "havenrednight",
          "dialogueText": "Then perhaps you should head out and find this Horseman.",
          "emotionID": "",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "AURORA",
          "innerLeft": "",
          "center": "LITTLERED",
          "innerRight": "",
          "outerRight": "ESME"
        },
        {
          "characterID": "VITA",
          "backgroundID": "havenrednight",
          "dialogueText": "That's dangerous! The Headless Horseman is a supernatural unkillable monster! What if he decides to take <i>your</i> head, Dreamer!",
          "emotionID": "surprise",
          "musicID": "",
          "sfxID": "",
          "animationID": "bounce",
          "outerLeft": "",
          "innerLeft": "",
          "center": "VITA",
          "innerRight": "",
          "outerRight": ""
        },
        {
          "characterID": "",
          "backgroundID": "havenrednight",
          "dialogueText": "Well, if Vita is scared, perhaps you should stay here.",
          "emotionID": "",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "",
          "center": "VITA",
          "innerRight": "",
          "outerRight": ""
        },
        {
          "characterID": "VITA",
          "backgroundID": "havenrednight",
          "dialogueText": "Scared?! I'll show you! I bet I can take down the Horseman all on my own!",
          "emotionID": "annoy",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "",
          "center": "VITA",
          "innerRight": "",
          "outerRight": ""
        },
        {
          "characterID": "VITA",
          "backgroundID": "havenrednight",
          "dialogueText": "Beating TEN Headless Horsemen would be child's play to the Lifestone!",
          "emotionID": "angry",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "",
          "center": "VITA",
          "innerRight": "",
          "outerRight": ""
        },
        {
          "characterID": "ESME",
          "backgroundID": "havenrednight",
          "dialogueText": "Vita...",
          "emotionID": "sad",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "VITA",
          "center": "",
          "innerRight": "ESME",
          "outerRight": ""
        },
        {
          "characterID": "VITA",
          "backgroundID": "havenrednight",
          "dialogueText": "Come on, Sleepy! Let's go find this Settler's Hollow!",
          "emotionID": "happy",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "VITA",
          "center": "",
          "innerRight": "ESME",
          "outerRight": ""
        }
      ]
    },
    {
      "languageID": "Korean",
      "dialogueLines": [
        {
          "characterID": "",
          "backgroundID": "redmoon",
          "dialogueText": "도시의 주민들이 모두 문을 굳게 잠그고 불길한 밤이 지나가길 기다리는 동안",
          "emotionID": "",
          "musicID": "haunted",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "",
          "center": "",
          "innerRight": "",
          "outerRight": ""
        },
        {
          "characterID": "",
          "backgroundID": "redmoon",
          "dialogueText": "핏빛 달이 안식의 도시에 붉은 그림자를 드리웠다.",
          "emotionID": "",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "",
          "center": "",
          "innerRight": "",
          "outerRight": ""
        },
        {
          "characterID": "",
          "backgroundID": "havenrednight",
          "dialogueText": "긴장한 눈으로 밤하늘을 바라보던 메르헨들이 몸을 뒤척였다.",
          "emotionID": "",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "",
          "center": "",
          "innerRight": "",
          "outerRight": ""
        },
        {
          "characterID": "",
          "backgroundID": "havenrednight",
          "dialogueText": "비타가 가장 먼저 몸을 일으키더니 투덜거렸다.",
          "emotionID": "",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "",
          "center": "",
          "innerRight": "",
          "outerRight": ""
        },
        {
          "characterID": "VITA",
          "backgroundID": "havenrednight",
          "dialogueText": "이건 나쁜 일의 징조야. 분명해!",
          "emotionID": "annoy",
          "musicID": "",
          "sfxID": "",
          "animationID": "bounce",
          "outerLeft": "",
          "innerLeft": "",
          "center": "VITA",
          "innerRight": "",
          "outerRight": ""
        },
        {
          "characterID": "ESME",
          "backgroundID": "havenrednight",
          "dialogueText": "하? 바보야! 이렇게 큰 징조가 어딨어?",
          "emotionID": "annoy",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "VITA",
          "center": "",
          "innerRight": "ESME",
          "outerRight": ""
        },
        {
          "characterID": "VITA",
          "backgroundID": "havenrednight",
          "dialogueText": "맞거든? 진짜거든? 그치, 큰 언니!?",
          "emotionID": "surprise",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "VITA",
          "center": "",
          "innerRight": "ESME",
          "outerRight": ""
        },
        {
          "characterID": "AURORA",
          "backgroundID": "havenrednight",
          "dialogueText": "비타의 의견에 동의하고 싶지 않지만...",
          "emotionID": "sad",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "VITA",
          "innerLeft": "",
          "center": "AURORA",
          "innerRight": "",
          "outerRight": "ESME"
        },
        {
          "characterID": "AURORA",
          "backgroundID": "havenrednight",
          "dialogueText": "불길한 느낌이 들어요. 무언가 잘못된 것처럼요.",
          "emotionID": "surprise",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "VITA",
          "innerLeft": "",
          "center": "AURORA",
          "innerRight": "",
          "outerRight": "ESME"
        },
        {
          "characterID": "",
          "backgroundID": "havenrednight",
          "dialogueText": "오로라의 작은 중얼거림이 빨간 망토의 외침에 묻혔다.",
          "emotionID": "",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "",
          "center": "LITTLERED",
          "innerRight": "",
          "outerRight": ""
        },
        {
          "characterID": "LITTLERED",
          "backgroundID": "havenrednight",
          "dialogueText": "꿈꾸는 자! 목 없는 기사가 나타났어!",
          "emotionID": "surprise",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "",
          "center": "LITTLERED",
          "innerRight": "",
          "outerRight": ""
        },
        {
          "characterID": "",
          "backgroundID": "havenrednight",
          "dialogueText": "오로라가 빨간 망토를 향해 고개를 돌렸다.",
          "emotionID": "",
          "musicID": "",
          "sfxID": "",
          "animationID": "",
          "outerLeft": "",
          "innerLeft": "",
          "center": "LITTLERED",
          "innerRight": "",
          "outerRight": ""
        },
        {
          "characterID": "AURORA",
          "backgroundID": "havenrednight",
