    "m_Name": "EventStory_Chess_4",
    "unskippable": 0,
    "dialogueDataEntries": [
        {
            "languageID": "English",
            "dialogueLines": [
                {
                    "characterID": "",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "As you travel the strange labyrinth, you come across a rift like you did in the Enchanted Realm...",
                    "emotionID": "",
                    "musicID": "intothewild",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Lily",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "Oh! I haven't seen anything like <i>that</i> before!",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Lily",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "The energy this rift crackles with feels ominously familiar to that of the Dreamless.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "Sure enough, as you approach it, more Dreamless minions pour through... followed by an absolutely massive Dreamless creature you've never encountered before!",
                    "emotionID": "",
                    "musicID": "ominous",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_NPC_DreamlessPawn_Red",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Aurora",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "What is that thing?!",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Lily",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "A beast from the Queen of Hearts' menagerie! Turned into something even more monstrous!",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Aurora",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Lily",
                    "outerRight": ""
                },
                {
                    "characterID": "Lily",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "We have to run!",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Aurora",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Lily",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "You can't run now! And besides, where would you run to?",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Aurora",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Lily",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "All you can do is face this new monstrosity and hope you prevail without the Wonderland crew to help out!",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Aurora",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Lily",
                    "outerRight": ""
                },
                {
                    "characterID": "Aurora",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "Dreamer! Look!",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Aurora",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Lily",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "Leaping down from a ridge of dark crystal, a flash of red and a throaty cry of rage!",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_RedKnight",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Red Knight",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "Taste my blade, Dreamless scum!",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "bounce",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_RedKnight",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "Red Knight!",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_RedKnight",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Red Knight",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "I'll hold the smaller Dreamless off, you take that thing down! Hurry, Dreamer!",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_RedKnight",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "Right! As she says!",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_RedKnight",
                    "innerRight": "",
                    "outerRight": ""
                }
            ]
        },
        {
            "languageID": "Korean",
            "dialogueLines": [
                {
                    "characterID": "",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "미로 한구석에서 우리는 끝을 알 수 없는 균열을 발견했다.",
                    "emotionID": "",
                    "musicID": "intothewild",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "화이트 폰 릴리",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "와아, 저 이런 건 처음 봐요! 예전에는 없었는데, 이상하네요?",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Lily",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "균열에서 흘러나오는 불길한 에너지는 드림리스의 것과 비슷했다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "순간 새카만 균열에 노란빛이 가득 차오른다. 이질적인 눈동자들이 우리를 똑바로 쳐다본다.",
                    "emotionID": "",
                    "musicID": "ominous",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_NPC_DreamlessPawn_Red",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "오로라",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "조심하세요, 꿈꾸는 자!",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Aurora",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "화이트 폰 릴리",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "설마 퀸 오브 하트가 키우던... ",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Aurora",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Lily",
                    "outerRight": ""
                },
                {
                    "characterID": "화이트 폰 릴리",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "저쪽! 저쪽으로 도망쳐요!",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Aurora",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Lily",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "릴리가 가리킨 방향으로 달렸지만 곧 막다른 길이 나타났다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Aurora",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Lily",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "이제 맞서 싸우는 것 외에는 방법이 없다. ",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Aurora",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Lily",
                    "outerRight": ""
                },
                {
                    "characterID": "오로라",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "꿈꾸는 자!",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Aurora",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Lily",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "오로라가 높은 곳을 가리키며 소리치자 붉은 섬광이 검은 벽을 갈랐다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_RedKnight",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "붉은 기사",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "목숨을 건 결투만큼 나를 흥분시키는 건 없지!",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "bounce",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_RedKnight",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "붉은 기사가 나타났다!",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_RedKnight",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "붉은 기사",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "하하! 가자! 저 큰 놈은 맡기겠다, 꿈꾸는 자!",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_RedKnight",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "shatteredchessrealm",
                    "dialogueText": "붉은 기사의 호탕한 목소리가 불안을 몰아낸다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_RedKnight",
                    "innerRight": "",
                    "outerRight": ""
                }
            ]
        }
    ]
}