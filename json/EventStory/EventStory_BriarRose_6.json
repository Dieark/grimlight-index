    "m_Name": "EventStory_BriarRose_6",
    "unskippable": 0,
    "dialogueDataEntries": [
        {
            "languageID": "English",
            "dialogueLines": [
                {
                    "characterID": "",
                    "backgroundID": "storybriarroseportal4",
                    "dialogueText": "Another portal appears before you and it is a great relief to step through it.",
                    "emotionID": "",
                    "musicID": "threateningbattle",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "storybriarroseportal4",
                    "dialogueText": "You leave the nightmare version of Haven behind, however the place you walk into doesn’t seem much more appealing.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "endlessgaol",
                    "dialogueText": "You seem to be inside a large, gloomy tower, circular steps extending both upwards and downwards with no end in sight in either direction.",
                    "emotionID": "",
                    "musicID": "none",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "endlessgaol",
                    "dialogueText": "A great many darkened doorways lead off the spiral staircase, each emanating a palpable ominous feeling.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Vita",
                    "backgroundID": "endlessgaol",
                    "dialogueText": "What is this place?",
                    "emotionID": "idle",
                    "musicID": "none",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Vita",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Aurora",
                    "backgroundID": "endlessgaol",
                    "dialogueText": "I... I don't know. I've never seen it before...",
                    "emotionID": "annoy",
                    "musicID": "haunted",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Aurora",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Vita",
                    "outerRight": ""
                },
                {
                    "characterID": "Esme",
                    "backgroundID": "endlessgaol",
                    "dialogueText": "This place doesn’t feel like it’s real at all! This staircase doesn't seem to end...",
                    "emotionID": "idle",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Aurora",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Vita",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "Esme",
                    "backgroundID": "endlessgaol",
                    "dialogueText": "Does that mean we're getting close?",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Aurora",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Vita",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "",
                    "backgroundID": "endlessgaol",
                    "dialogueText": "You can only hope.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Aurora",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Vita",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "Aurora",
                    "backgroundID": "endlessgaol",
                    "dialogueText": "Vita? Which direction?",
                    "emotionID": "annoy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Aurora",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Vita",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "Vita",
                    "backgroundID": "endlessgaol",
                    "dialogueText": "Ah! Down! Definitely down!",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Aurora",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Vita",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "Vita",
                    "backgroundID": "endlessgaol",
                    "dialogueText": "I can hear Briar talking clearly now. It’s not good!",
                    "emotionID": "idle",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Aurora",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Vita",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "Esme",
                    "backgroundID": "endlessgaol",
                    "dialogueText": "What is she saying?",
                    "emotionID": "idle",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Aurora",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Vita",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "Vita",
                    "backgroundID": "endlessgaol",
                    "dialogueText": "That she's losing!",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Aurora",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Vita",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "Aurora",
                    "backgroundID": "endlessgaol",
                    "dialogueText": "We have to hurry!",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Aurora",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Vita",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "",
                    "backgroundID": "endlessgaol",
                    "dialogueText": "You all sprint down the stairs, following Vita's directions until you eventually come upon a doorway to a large stone-built chamber.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Aurora",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Vita",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "Vita",
                    "backgroundID": "endlessgaol",
                    "dialogueText": "In here!",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Aurora",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Vita",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "",
                    "backgroundID": "endlessgaol",
                    "dialogueText": "Inside the chamber, a girl lies on a plinth apparently fast asleep in a shaft of light that you can't work out the source of.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Aurora",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Vita",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "Vita",
                    "backgroundID": "endlessgaol",
                    "dialogueText": "It's Briar!",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Aurora",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Vita",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "Esme",
                    "backgroundID": "endlessgaol",
                    "dialogueText": "Why is she asleep?!",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Aurora",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Vita",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "Vita",
                    "backgroundID": "endlessgaol",
                    "dialogueText": "She isn't asleep, she's... she's fighting!",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Aurora",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Vita",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "Esme",
                    "backgroundID": "endlessgaol",
                    "dialogueText": "Fighting what?!",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Aurora",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Vita",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "",
                    "backgroundID": "storybriarrosecorruption",
                    "dialogueText": "Suddenly, Briar sits up. Vines start violently coiling around her body as she turns towards you, standing up.",
                    "emotionID": "",
                    "musicID": "ominous",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "storybriarrosecorruption",
                    "dialogueText": "As Briar Rose rises from the plinth, she changes and warps, her form and visage becoming nightmarish, until a monster with only a passing resemblance to the princess stands before you.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Esme",
                    "backgroundID": "endlessgaol",
                    "dialogueText": "What is that thing?!",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Aurora",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Vita",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "Vita",
                    "backgroundID": "endlessgaol",
                    "dialogueText": "It's Briar! She's still calling out to me!",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Aurora",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Vita",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "Vita",
                    "backgroundID": "endlessgaol",
                    "dialogueText": "She's trapped inside whatever that is! She's trying to find a way out, but she can't!",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Aurora",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Vita",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "Esme",
                    "backgroundID": "endlessgaol",
                    "dialogueText": "Is it a Dreamless?!",
                    "emotionID": "idle",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Aurora",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Vita",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "Aurora",
                    "backgroundID": "endlessgaol",
                    "dialogueText": "No! This is something very different...",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Aurora",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Vita",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "",
                    "backgroundID": "endlessgaol",
                    "dialogueText": "The Nightmare Briar roars with unearthly anger and then charges. From the shadows in the corners of the chamber, more Nightmares impossibly pour forth, more than you've ever seen!",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_NPC_NightmareVassal",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "endlessgaol",
                    "dialogueText": "This must be the end of the line!",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_NPC_NightmareVassal",
                    "innerRight": "",
                    "outerRight": ""
                }
            ]
        },
        {
            "languageID": "Korean",
            "dialogueLines": [
                {
                    "characterID": "",
                    "backgroundID": "storybriarroseportal4",
                    "dialogueText": "눈앞에 또 다른 차원문이 나타났다.",
                    "emotionID": "",
                    "musicID": "threateningbattle",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "storybriarroseportal4",
                    "dialogueText": "멸망하는 안식의 도시를 벗어날 수 있는 것만으로도 다행이라고 생각했지만, 새로 도착한 장소 역시 썩 좋은 풍경은 아니었다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "endlessgaol",
                    "dialogueText": "우중충한 분위기의 거대한 탑 안에 떨어졌다.<br>나선으로 이어진 계단이 끝없이 이어진다.",
                    "emotionID": "",
                    "musicID": "none",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "endlessgaol",
                    "dialogueText": "수많은 문과 계단이 서로 연결된 모습의 탑에 소름이 돋을 정도로 불길한 기운이 일렁이고 있었다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "비타",
                    "backgroundID": "endlessgaol",
                    "dialogueText": "여기는 또 어디야?",
                    "emotionID": "idle",
                    "musicID": "none",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Vita",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "오로라",
                    "backgroundID": "endlessgaol",
                    "dialogueText": "이곳은 저도 처음 보는 곳이에요...",
                    "emotionID": "annoy",
                    "musicID": "haunted",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_Aurora",
                    "center": "",
                    "innerRight": "Fullbody_Hero_Vita",
                    "outerRight": ""
                },
                {
                    "characterID": "에스메",
                    "backgroundID": "endlessgaol",
                    "dialogueText": "이것도 환상이겠지.<br>이 계단을 좀 봐, 이런 게 현실에 존재할 수 있겠어?",
                    "emotionID": "idle",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Aurora",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Vita",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "에스메",
                    "backgroundID": "endlessgaol",
                    "dialogueText": "아무래도 거의 다 온 것 같아.",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Aurora",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Vita",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "",
                    "backgroundID": "endlessgaol",
                    "dialogueText": "오로라가 비타를 바라보며 방향을 물었다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Aurora",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Vita",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "오로라",
                    "backgroundID": "endlessgaol",
                    "dialogueText": "비타, 어느 쪽이지요?",
                    "emotionID": "annoy",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Aurora",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Vita",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "비타",
                    "backgroundID": "endlessgaol",
                    "dialogueText": "우리 밑이야!<br>아주 가까워, 바로 아래쪽에 있는 것 같아!",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Aurora",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Vita",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "비타",
                    "backgroundID": "endlessgaol",
                    "dialogueText": "이제 로즈가 무슨 말을 하고 있는지 명확하게 들려.<br>그런데... ...",
                    "emotionID": "idle",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Aurora",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Vita",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "에스메",
                    "backgroundID": "endlessgaol",
                    "dialogueText": "뭐라고 하는데?",
                    "emotionID": "idle",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Aurora",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Vita",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "비타",
                    "backgroundID": "endlessgaol",
                    "dialogueText": "미, 밀리고 있다고...?<br>로즈, 밀리고 있다는 게 무슨 소리야?<br>브라이어 로즈가 밀리고 있다고 말했어!",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Aurora",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Vita",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "오로라",
                    "backgroundID": "endlessgaol",
                    "dialogueText": "서두르는 것이 좋겠어요!",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Aurora",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Vita",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "",
                    "backgroundID": "endlessgaol",
                    "dialogueText": "우리는 비타가 알려준 방향을 따라 계단을 타고 달려,<br>돌로 만들어진 방의 입구 앞에 도착했다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Aurora",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Vita",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "비타",
                    "backgroundID": "endlessgaol",
                    "dialogueText": "여기야!",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Aurora",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_Vita",
                    "innerRight": "",
                    "outerRight": "Fullbody_Hero_Esme"
                },
                {
                    "characterID": "",
                    "backgroundID": "endlessgaol",
                    "dialogueText": "망설일 것도 없이 문을 열고 방 안으로 들어섰다.<br>근원지를 찾을 수 없는 한 줄기 빛,<br>그사이에 놓인 바위 위에 누군가 누워있다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "Fullbody_Hero_Aurora",
