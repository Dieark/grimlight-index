    "m_Name": "EventStory_EvilQueen_7",
    "unskippable": 0,
    "dialogueDataEntries": [
        {
            "languageID": "English",
            "dialogueLines": [
                {
                    "characterID": "",
                    "backgroundID": "woodlandscreepy",
                    "dialogueText": "The Dreamless minions surrounding the Evil Queen are vanquished and Snow White engages her nemesis directly.",
                    "emotionID": "",
                    "musicID": "threateningbattle",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_EvilQueen",
                    "center": "",
                    "innerRight": "Fullbody_Hero_SnowWhite",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "woodlandscreepy",
                    "dialogueText": "The battle between the two beings is titanic, albeit brief and ends with Snow White's mace finding its mark.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_EvilQueen",
                    "center": "",
                    "innerRight": "Fullbody_Hero_SnowWhite",
                    "outerRight": ""
                },
                {
                    "characterID": "Evil Queen",
                    "backgroundID": "woodlandscreepy",
                    "dialogueText": "Nooo!",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_EvilQueen",
                    "center": "",
                    "innerRight": "Fullbody_Hero_SnowWhite",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "woodlandscreepy",
                    "dialogueText": "The mace connects heavily and the queen is battered to the ground, where she lies defeated, panting heavily.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_EvilQueen",
                    "center": "",
                    "innerRight": "Fullbody_Hero_SnowWhite",
                    "outerRight": ""
                },
                {
                    "characterID": "Snow White",
                    "backgroundID": "woodlandscreepy",
                    "dialogueText": "It's over, Narcissa. Please stop torturing yourself and everyone else!",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_EvilQueen",
                    "center": "",
                    "innerRight": "Fullbody_Hero_SnowWhite",
                    "outerRight": ""
                },
                {
                    "characterID": "Snow White",
                    "backgroundID": "woodlandscreepy",
                    "dialogueText": "We don't have to fight!",
                    "emotionID": "sad",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_EvilQueen",
                    "center": "",
                    "innerRight": "Fullbody_Hero_SnowWhite",
                    "outerRight": ""
                },
                {
                    "characterID": "Evil Queen",
                    "backgroundID": "woodlandscreepy",
                    "dialogueText": "NO! It's never over!",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "bounce",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_EvilQueen",
                    "center": "",
                    "innerRight": "Fullbody_Hero_SnowWhite",
                    "outerRight": ""
                },
                {
                    "characterID": "Evil Queen",
                    "backgroundID": "woodlandscreepy",
                    "dialogueText": "I just need more power! More!",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_EvilQueen",
                    "center": "",
                    "innerRight": "Fullbody_Hero_SnowWhite",
                    "outerRight": ""
                },
                {
                    "characterID": "Evil Queen",
                    "backgroundID": "woodlandscreepy",
                    "dialogueText": "I <i>will</i> be the fairest!",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_EvilQueen",
                    "center": "",
                    "innerRight": "Fullbody_Hero_SnowWhite",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "woodlandscreepy",
                    "dialogueText": "The Evil Queen begins drawing in power from her surroundings, from the fallen Dreamless still fizzling away into oblivion, drinking in the last dregs of their essence.",
                    "emotionID": "",
                    "musicID": "ominous",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_EvilQueen",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "woodlandscreepy",
                    "dialogueText": "But the power is already corrupted, poisoned, like the apple she once gave to Snow.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_EvilQueen",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "woodlandscreepy",
                    "dialogueText": "She swells with the power, but is also consumed by it, losing her mind as the Dreamless corruption begins to transform her into a new, hideously beautiful form!",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_EvilQueen",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "Evil Queen",
                    "backgroundID": "woodlandscreepy",
                    "dialogueText": "I WILL ALWAYS BE THE FAIREST!",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_EvilQueen",
                    "innerRight": "",
                    "outerRight": ""
                }
            ]
        },
        {
            "languageID": "Korean",
            "dialogueLines": [
                {
                    "characterID": "",
                    "backgroundID": "woodlandscreepy",
                    "dialogueText": "이블 퀸을 보호하던 드림리스들이 쓰러지고, 마침내 일대일 대결이 시작되었다.",
                    "emotionID": "",
                    "musicID": "threateningbattle",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_EvilQueen",
                    "center": "",
                    "innerRight": "Fullbody_Hero_SnowWhite",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "woodlandscreepy",
                    "dialogueText": "강력한 두 존재의 싸움은 순식간에 끝났지만, 그 여파는 숲 속에 오래도록 메아리쳤다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_EvilQueen",
                    "center": "",
                    "innerRight": "Fullbody_Hero_SnowWhite",
                    "outerRight": ""
                },
                {
                    "characterID": "이블 퀸",
                    "backgroundID": "woodlandscreepy",
                    "dialogueText": "... 용납할 수 없어.",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_EvilQueen",
                    "center": "",
                    "innerRight": "Fullbody_Hero_SnowWhite",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "woodlandscreepy",
                    "dialogueText": "백설공주의 메이스가 이블 퀸의 목을 노리고 있다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_EvilQueen",
                    "center": "",
                    "innerRight": "Fullbody_Hero_SnowWhite",
                    "outerRight": ""
                },
                {
                    "characterID": "백설공주",
                    "backgroundID": "woodlandscreepy",
                    "dialogueText": "다 끝났어요, 나르시사. 그러니 이제 제발... 더 이상 자신과 다른 사람들을 괴롭히지 마세요.",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_EvilQueen",
                    "center": "",
                    "innerRight": "Fullbody_Hero_SnowWhite",
                    "outerRight": ""
                },
                {
                    "characterID": "백설공주",
                    "backgroundID": "woodlandscreepy",
                    "dialogueText": "우리는 굳이 싸울 필요가 없어요.",
                    "emotionID": "sad",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_EvilQueen",
                    "center": "",
                    "innerRight": "Fullbody_Hero_SnowWhite",
                    "outerRight": ""
                },
                {
                    "characterID": "이블 퀸",
                    "backgroundID": "woodlandscreepy",
                    "dialogueText": "아니, 아니야! 아무것도 끝나지 않았어!",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "bounce",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_EvilQueen",
                    "center": "",
                    "innerRight": "Fullbody_Hero_SnowWhite",
                    "outerRight": ""
                },
                {
                    "characterID": "이블 퀸",
                    "backgroundID": "woodlandscreepy",
                    "dialogueText": "난 내가 사랑하는 모든 것을 너 때문에 잃었어! 그런데 너는?",
                    "emotionID": "surprise",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_EvilQueen",
                    "center": "",
                    "innerRight": "Fullbody_Hero_SnowWhite",
                    "outerRight": ""
                },
                {
                    "characterID": "이블 퀸",
                    "backgroundID": "woodlandscreepy",
                    "dialogueText": "넌 여전히 널 사랑해 마지않는 것들과 함께 하고 있지, 용서할 수 없어!",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "Fullbody_Hero_EvilQueen",
                    "center": "",
                    "innerRight": "Fullbody_Hero_SnowWhite",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "woodlandscreepy",
                    "dialogueText": "이블 퀸이 쓰러진 드림리스로부터 검은 힘을 흡수하기 시작했다.",
                    "emotionID": "",
                    "musicID": "ominous",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_EvilQueen",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "woodlandscreepy",
                    "dialogueText": "이블 퀸의 손끝이 그녀가 백설공주에게 내밀었던 독이 든 사과처럼 검게 물들였다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_EvilQueen",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "",
                    "backgroundID": "woodlandscreepy",
                    "dialogueText": "새로운 힘이 이블 퀸의 몸 속에서 넘실거린다. 이성을 잃어버린 여왕이 소름끼치게 미소지었다.",
                    "emotionID": "",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_EvilQueen",
                    "innerRight": "",
                    "outerRight": ""
                },
                {
                    "characterID": "이블 퀸",
                    "backgroundID": "woodlandscreepy",
                    "dialogueText": "이 세상에서 가장 아름다운 건 나야. 그렇지 않니? 모두 날 사랑해야 해, 네가 아닌 나를!",
                    "emotionID": "angry",
                    "musicID": "",
                    "sfxID": "",
                    "animationID": "",
                    "outerLeft": "",
                    "innerLeft": "",
                    "center": "Fullbody_Hero_EvilQueen",
                    "innerRight": "",
                    "outerRight": ""
                }
            ]
        }
    ]
}